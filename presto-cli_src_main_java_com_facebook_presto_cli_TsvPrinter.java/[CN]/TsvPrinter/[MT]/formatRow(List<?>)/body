{
  StringBuilder sb=new StringBuilder();
  Iterator<?> iter=row.iterator();
  while (iter.hasNext()) {
    Object value=iter.next();
    String s=(value == null) ? "" : value.toString();
    for (int i=0; i < s.length(); i++) {
      escapeCharacter(sb,s.charAt(i));
    }
    if (iter.hasNext()) {
      sb.append('\t');
    }
  }
  sb.append('\n');
  return sb.toString();
}
