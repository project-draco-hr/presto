{
  if (blockCursor == null) {
    return false;
  }
  isValid=true;
  if (!blockCursor.advanceNextValue()) {
    if (iterator.hasNext()) {
      blockCursor=iterator.next().cursor();
      blockCursor.advanceNextPosition();
    }
 else {
      blockCursor=null;
      return false;
    }
  }
  return true;
}
