{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  PartitioningHandle that=(PartitioningHandle)o;
  if (connectorId.isPresent() || that.connectorId.isPresent()) {
    return false;
  }
  return Objects.equals(connectorId,that.connectorId) && Objects.equals(transactionHandle,that.transactionHandle) && Objects.equals(connectorHandle,that.connectorHandle);
}
