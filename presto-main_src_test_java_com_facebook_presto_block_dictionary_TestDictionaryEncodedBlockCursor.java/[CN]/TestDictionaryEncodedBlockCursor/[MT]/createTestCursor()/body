{
  Dictionary dictionary=new Dictionary(TupleInfo.SINGLE_VARBINARY,createTuple("apple").getTupleSlice(),createTuple("banana").getTupleSlice(),createTuple("cherry").getTupleSlice(),createTuple("date").getTupleSlice());
  return new DictionaryEncodedBlock(dictionary,createLongsBlock(0,0,0,1,1,1,1,1,2,2,3)).cursor();
}
