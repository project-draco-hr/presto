{
  @Language("SQL") String actual="" + "SELECT LOWER(table_name) FROM " + "  information_schema.tables "+ "WHERE "+ "  table_catalog = 'hive' "+ "  and table_schema LIKE 'tpch' "+ "  and table_name LIKE '%orders'";
  @Language("SQL") String expected="" + "SELECT LOWER(table_name) FROM " + "  information_schema.tables "+ "WHERE "+ "  table_name LIKE '%ORDERS'";
  assertQuery(actual,expected);
}
