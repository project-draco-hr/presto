{
  this(connectorId,metastore,namenodeStats,hdfsEnvironment,directoryLister,new BoundedExecutor(executorService,hiveClientConfig.getMaxSplitIteratorThreads()),hiveClientConfig.getMaxOutstandingSplits(),hiveClientConfig.getMinPartitionBatchSize(),hiveClientConfig.getMaxPartitionBatchSize(),hiveClientConfig.getMaxSplitSize(),hiveClientConfig.getMaxInitialSplitSize(),hiveClientConfig.getMaxInitialSplits(),hiveClientConfig.getRecursiveDirWalkerEnabled());
}
