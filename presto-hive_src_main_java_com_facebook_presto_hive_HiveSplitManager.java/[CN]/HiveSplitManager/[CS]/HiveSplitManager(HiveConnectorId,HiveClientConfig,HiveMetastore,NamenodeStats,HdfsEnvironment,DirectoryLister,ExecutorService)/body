{
  this(connectorId,metastore,namenodeStats,hdfsEnvironment,directoryLister,DateTimeZone.forTimeZone(hiveClientConfig.getTimeZone()),new BoundedExecutor(executorService,hiveClientConfig.getMaxGlobalSplitIteratorThreads()),hiveClientConfig.getMaxOutstandingSplits(),hiveClientConfig.getMaxSplitIteratorThreads(),hiveClientConfig.getMinPartitionBatchSize(),hiveClientConfig.getMaxPartitionBatchSize(),hiveClientConfig.getMaxSplitSize(),hiveClientConfig.getMaxInitialSplitSize(),hiveClientConfig.getMaxInitialSplits(),hiveClientConfig.isForceLocalScheduling(),hiveClientConfig.isAssumeCanonicalPartitionKeys(),hiveClientConfig.getRecursiveDirWalkerEnabled());
}
