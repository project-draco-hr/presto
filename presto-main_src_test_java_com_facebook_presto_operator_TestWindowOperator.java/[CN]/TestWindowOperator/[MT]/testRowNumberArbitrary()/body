{
  List<Page> input=rowPagesBuilder(SINGLE_LONG).row(1).row(3).row(5).row(7).pageBreak().row(2).row(4).row(6).row(8).build();
  WindowOperatorFactory operatorFactory=new WindowOperatorFactory(0,ImmutableList.of(SINGLE_LONG),ints(0),ROW_NUMBER,ints(),ints(),sortOrder(),10);
  Operator operator=operatorFactory.createOperator(driverContext);
  MaterializedResult expected=resultBuilder(FIXED_INT_64,FIXED_INT_64).row(1,1).row(3,2).row(5,3).row(7,4).row(2,5).row(4,6).row(6,7).row(8,8).build();
  assertOperatorEquals(operator,input,expected);
}
