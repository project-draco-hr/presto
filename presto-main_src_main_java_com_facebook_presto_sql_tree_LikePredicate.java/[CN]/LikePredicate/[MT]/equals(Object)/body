{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  LikePredicate that=(LikePredicate)o;
  if (escape != null ? !escape.equals(that.escape) : that.escape != null) {
    return false;
  }
  if (!pattern.equals(that.pattern)) {
    return false;
  }
  if (!value.equals(that.value)) {
    return false;
  }
  return true;
}
