{
  List<Type> types=ImmutableList.of(BIGINT,DOUBLE,VARCHAR);
  List<Type> sortTypes=ImmutableList.copyOf(types);
  List<Integer> sortChannels=Ints.asList(0,1,2);
  List<SortOrder> sortOrders=Collections.nCopies(sortChannels.size(),ASC_NULLS_FIRST);
  List<Page> inputPages=RowPagesBuilder.rowPagesBuilder(types).row(2,1.1,"d").row(1,2.2,"c").pageBreak().row(1,2.2,"b").row(1,2.2,"a").pageBreak().row(1,2.2,null).row(1,null,"z").row(1,null,null).build();
  List<Page> expectedPages=RowPagesBuilder.rowPagesBuilder(types).row(1,null,null).row(1,null,"z").row(1,2.2,null).row(1,2.2,"a").row(1,2.2,"b").row(1,2.2,"c").row(2,1.1,"d").build();
  assertSorted(inputPages,expectedPages,types,sortTypes,sortChannels,sortOrders,100);
}
