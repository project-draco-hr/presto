{
  List<Type> types=ImmutableList.of(BIGINT,DOUBLE,VARCHAR);
  List<Integer> sortChannels=Ints.asList(0);
  List<SortOrder> sortOrders=ImmutableList.of(ASC_NULLS_FIRST);
  List<Page> inputPages=RowPagesBuilder.rowPagesBuilder(types).row(2L,1.1,"c").row(1L,2.2,"d").pageBreak().row(-2L,2.2,"b").row(-12L,2.2,"a").build();
  List<Page> expectedPages=RowPagesBuilder.rowPagesBuilder(types).row(-12L,2.2,"a").row(-2L,2.2,"b").pageBreak().row(1L,2.2,"d").row(2L,1.1,"c").build();
  assertSorted(inputPages,expectedPages,types,sortChannels,sortOrders,2);
}
