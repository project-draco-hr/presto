{
  if (outputStage != null) {
    List<TaskInfo> outStage=outputStage.getTasks();
    for (    TaskInfo outputTask : outStage) {
      for (      BufferInfo outputBuffer : outputTask.getOutputBuffers().getBuffers()) {
        if (outputBuffer.getBufferedPages() > 0) {
          return true;
        }
      }
    }
  }
  return false;
}
