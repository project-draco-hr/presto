{
  checkArgument(page.getChannelCount() == 1);
  long result=0;
  Block block=page.getBlock(0);
  for (int i=0; i < block.getPositionCount(); i++) {
    if (block.isNull(i)) {
      result+=PRIME64;
    }
 else {
      result+=type.hash(block,i) * PRIME64;
    }
  }
  return new SqlVarbinary(wrappedLongArray(result).getBytes());
}
