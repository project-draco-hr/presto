{
  OrcPageSourceFactory orcPageSourceFactory=new OrcPageSourceFactory(TYPE_MANAGER,false,HDFS_ENVIRONMENT);
  return orcPageSourceFactory.createPageSource(new Configuration(),SESSION,fileSplit.getPath(),fileSplit.getStart(),fileSplit.getLength(),schema,columns,partitionKeys,TupleDomain.<HiveColumnHandle>all(),DateTimeZone.UTC).get();
}
