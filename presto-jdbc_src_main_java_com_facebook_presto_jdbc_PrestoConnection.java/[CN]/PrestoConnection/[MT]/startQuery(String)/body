{
  URI uri=createHttpUri(address);
  String source=firstNonNull(clientInfo.get("ApplicationName"),"presto-jdbc");
  ClientSession session=new ClientSession(uri,user,source,catalog.get(),schema.get(),timeZoneId.get(),locale.get(),ImmutableMap.copyOf(sessionProperties),false,new Duration(2,MINUTES));
  return queryExecutor.startQuery(session,sql);
}
