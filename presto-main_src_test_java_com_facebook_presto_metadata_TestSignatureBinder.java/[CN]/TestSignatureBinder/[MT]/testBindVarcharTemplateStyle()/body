{
  Signature function=functionSignature().returnType("T2").argumentTypes("T1").typeVariableConstraints(ImmutableList.of(new TypeVariableConstraint("T1",true,false,"varchar"),new TypeVariableConstraint("T2",true,false,"varchar"))).build();
  assertThat(function).boundTo(ImmutableList.of("varchar(42)"),"varchar(1)").produces(new BoundVariables(ImmutableMap.of("T1",type("varchar(42)"),"T2",type("varchar(1)")),ImmutableMap.of()));
}
