{
  Signature function=functionSignature().returnType("decimal(p3,s)").argumentTypes("decimal(p1,s)","decimal(p2,s)").literalParameters("p1","p2","p3","s").longVariableConstraints(longVariableExpression("p3","p1 + p2")).build();
  assertThat(function).boundTo("decimal(2,1)","decimal(3,1)").produces(new BoundVariables(ImmutableMap.of(),ImmutableMap.of("p1",2L,"p2",3L,"p3",5L,"s",1L)));
}
