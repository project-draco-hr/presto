{
  Signature function=functionSignature().returnType("varchar(42)").argumentTypes("varchar(42)").build();
  assertThat(function).boundTo(ImmutableList.of("varchar(1)"),"varchar(1)").fails();
  assertThat(function).boundTo(ImmutableList.of("varchar(1)"),"varchar(1)").withCoercion().succeeds();
  assertThat(function).boundTo(ImmutableList.of("varchar(44)"),"varchar(44)").withCoercion().fails();
}
