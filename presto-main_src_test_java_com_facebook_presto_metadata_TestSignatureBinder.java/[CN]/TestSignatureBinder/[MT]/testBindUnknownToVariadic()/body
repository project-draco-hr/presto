{
  Signature rowFunction=functionSignature().returnType("boolean").argumentTypes("T","T").typeVariableConstraints(ImmutableList.of(withVariadicBound("T","row"))).build();
  assertThat(rowFunction).boundTo("unknown","row(a bigint)").withCoercion().produces(new BoundVariables(ImmutableMap.of("T",type("row(a bigint)")),ImmutableMap.of()));
  Signature arrayFunction=functionSignature().returnType("boolean").argumentTypes("T","T").typeVariableConstraints(ImmutableList.of(withVariadicBound("T","array"))).build();
  assertThat(arrayFunction).boundTo("unknown","array(bigint)").withCoercion().produces(new BoundVariables(ImmutableMap.of("T",type("array(bigint)")),ImmutableMap.of()));
}
