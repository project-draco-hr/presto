{
  requireNonNull(input,"input is null");
  if (input.length() == 0) {
    return EMPTY_SLICE;
  }
switch (codec) {
case GZIP:
    return decompressGzip(input,uncompressedSize);
case SNAPPY:
  return decompressSnappy(input,uncompressedSize);
case UNCOMPRESSED:
return input;
default :
throw new ParquetCorruptionException("Codec not supported in Parquet: " + codec);
}
}
