{
  SqlTaskExecution task=new SqlTaskExecution(session,nodeInfo,taskId,location,fragment,maxBufferSize,dataStreamProvider,exchangeClientProvider,metadata,storageManager,shardExecutor,maxOperatorMemoryUsage,queryMonitor,taskMasterExecutor,globalStats);
  try (SetThreadName setThreadName=new SetThreadName("Task-%s",taskId)){
    task.start(taskMasterExecutor);
    return task;
  }
