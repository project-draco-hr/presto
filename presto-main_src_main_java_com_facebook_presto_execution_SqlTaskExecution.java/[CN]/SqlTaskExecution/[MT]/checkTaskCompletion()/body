{
  if (taskStateMachine.getState().isDone()) {
    return;
  }
  if (partitionedSourceId != null && !noMorePartitionedSplits.get()) {
    return;
  }
  if (remainingDrivers.get() != 0) {
    return;
  }
  sharedBuffer.finish();
  if (!sharedBuffer.isFinished()) {
    return;
  }
  taskStateMachine.finished();
}
