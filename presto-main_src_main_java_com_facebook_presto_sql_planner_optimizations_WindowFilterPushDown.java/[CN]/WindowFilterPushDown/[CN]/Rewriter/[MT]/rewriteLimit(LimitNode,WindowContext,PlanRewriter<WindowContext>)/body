{
  if (context != null && context.getExpression() instanceof LongLiteral && ((LongLiteral)context.getExpression()).getValue() < node.getCount()) {
    PlanNode rewrittenSource=planRewriter.rewrite(node.getSource(),context);
    if (rewrittenSource != node.getSource()) {
      return rewrittenSource;
    }
  }
  return planRewriter.defaultRewrite(node,null);
}
