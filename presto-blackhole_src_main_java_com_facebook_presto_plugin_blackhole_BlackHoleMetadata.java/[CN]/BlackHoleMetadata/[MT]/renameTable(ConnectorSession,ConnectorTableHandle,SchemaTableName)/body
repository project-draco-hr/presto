{
  BlackHoleTableHandle oldTableHandle=checkType(tableHandle,BlackHoleTableHandle.class,"tableHandle");
  BlackHoleTableHandle newTableHandle=new BlackHoleTableHandle(oldTableHandle.getSchemaName(),newTableName.getTableName(),oldTableHandle.getColumnHandles(),oldTableHandle.getSplitsCount(),oldTableHandle.getPagesPerSplit(),oldTableHandle.getRowsPerPage());
synchronized (tables) {
    tables.remove(oldTableHandle.getTableName());
    tables.put(newTableName.getTableName(),newTableHandle);
  }
}
