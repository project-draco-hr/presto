{
  if (this == o) {
    return true;
  }
  if (!(o instanceof Slice)) {
    return false;
  }
  if (o instanceof ByteArraySlice) {
    return equals((ByteArraySlice)o);
  }
  Slice slice=(Slice)o;
  if (length != slice.length()) {
    return false;
  }
  return toByteBuffer().equals(slice.toByteBuffer());
}
