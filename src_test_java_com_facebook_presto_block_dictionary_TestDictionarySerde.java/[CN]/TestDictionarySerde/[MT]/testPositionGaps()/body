{
  TupleStream tupleStream=new UncompressedBlockStream(TupleInfo.SINGLE_VARBINARY,Blocks.createBlock(1,"a","a","b","a","c"),Blocks.createBlock(6,"c","a","b","b","b"),Blocks.createBlock(100,"y","y","a","y","b"),Blocks.createBlock(200,"b"));
  dictionarySerde.serialize(tupleStream,sliceOutput);
  Blocks.assertBlockStreamEquals(tupleStream,dictionarySerde.deserialize(sliceOutput.slice()));
}
