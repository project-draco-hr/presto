{
  BytesInput decompressed;
  if (codec != null) {
    decompressor.reset();
    InputStream inputStream=codec.createInputStream(new ByteArrayInputStream(bytes.toByteArray()),decompressor);
    decompressed=BytesInput.from(inputStream,uncompressedSize);
  }
 else {
    decompressed=bytes;
  }
  return decompressed;
}
