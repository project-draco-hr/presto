{
  int index=0;
  for (  TupleInfo.Type type : tupleInfo.getTypes()) {
switch (type) {
case FIXED_INT_64:
      builder.append(getLong(index));
    break;
case DOUBLE:
  builder.append(getDouble(index));
break;
case VARIABLE_BINARY:
builder.append(getSlice(index));
break;
default :
throw new AssertionError("unhandled type: " + type);
}
index++;
}
}
