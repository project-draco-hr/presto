{
  checkNotNull(split,"split is null");
  checkState(this.split == null,"Table scan split already set");
  if (finishing) {
    return;
  }
  this.split=split;
  Object splitInfo=split.getInfo();
  if (splitInfo != null) {
    operatorContext.setInfoSupplier(Suppliers.ofInstance(splitInfo));
  }
  blocked.set(null);
}
