{
  return new Function<Object,Object>(){
    @Nullable @Override public Object apply(    @Nullable Object input){
      if (input instanceof Float) {
        input=((Float)input).doubleValue();
      }
      Map<Object,Object> data=new LinkedHashMap<>();
      data.put("a",input);
      data.put("b",input);
      return data;
    }
  }
;
}
