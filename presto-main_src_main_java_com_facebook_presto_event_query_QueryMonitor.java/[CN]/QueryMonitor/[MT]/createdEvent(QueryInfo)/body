{
  eventClient.post(new QueryCreatedEvent(queryInfo.getQueryId(),queryInfo.getSession().getUser(),queryInfo.getSession().getPrincipal().orElse(null),queryInfo.getSession().getSource().orElse(null),serverVersion,environment,queryInfo.getSession().getCatalog().orElse(null),queryInfo.getSession().getSchema().orElse(null),queryInfo.getSession().getRemoteUserAddress().orElse(null),queryInfo.getSession().getUserAgent().orElse(null),queryInfo.getSelf(),queryInfo.getQuery(),queryInfo.getQueryStats().getCreateTime()));
}
