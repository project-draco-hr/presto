{
  eventClient.post(new QueryCreatedEvent(queryInfo.getQueryId(),queryInfo.getSession().getUser(),queryInfo.getSession().getSource(),serverVersion,environment,queryInfo.getSession().getCatalog(),queryInfo.getSession().getSchema(),queryInfo.getSession().getRemoteUserAddress(),queryInfo.getSession().getUserAgent(),queryInfo.getSelf(),queryInfo.getQuery(),queryInfo.getQueryStats().getCreateTime()));
}
