{
  try {
    eventClient.post(new SplitCompletionEvent(taskInfo.getTaskId().getQueryId(),taskInfo.getTaskId().getStageId(),taskInfo.getTaskId(),environment,splitExecutionStats.getQueuedTime(),splitExecutionStats.getExecutionStartTime(),splitExecutionStats.getTimeToFirstByte(),splitExecutionStats.getTimeToLastByte(),splitExecutionStats.getCompletedDataSize(),splitExecutionStats.getCompletedPositions(),splitExecutionStats.getWall(),splitExecutionStats.getCpu(),splitExecutionStats.getUser(),objectMapper.writeValueAsString(splitExecutionStats.getSplitInfo())));
  }
 catch (  JsonProcessingException e) {
    throw Throwables.propagate(e);
  }
}
