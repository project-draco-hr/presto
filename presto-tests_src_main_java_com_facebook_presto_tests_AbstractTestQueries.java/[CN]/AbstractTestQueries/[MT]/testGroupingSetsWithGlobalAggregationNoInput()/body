{
  assertQuery("SELECT linenumber, suppkey, SUM(CAST(quantity AS BIGINT)) " + "FROM lineitem " + "WHERE quantity < 0 "+ "GROUP BY GROUPING SETS ((linenumber, suppkey), (suppkey), ())","SELECT linenumber, suppkey, SUM(CAST(quantity AS BIGINT)) " + "FROM lineitem " + "WHERE quantity < 0 "+ "GROUP BY linenumber, suppkey "+ "UNION "+ "SELECT NULL, suppkey, SUM(CAST(quantity AS BIGINT)) "+ "FROM lineitem "+ "WHERE quantity < 0 "+ "GROUP BY suppkey "+ "UNION "+ "SELECT NULL, NULL, SUM(CAST(quantity AS BIGINT)) "+ "FROM lineitem "+ "WHERE quantity < 0");
