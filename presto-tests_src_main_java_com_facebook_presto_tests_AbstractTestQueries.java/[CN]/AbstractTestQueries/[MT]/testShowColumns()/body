{
  MaterializedResult actual=computeActual("SHOW COLUMNS FROM orders");
  MaterializedResult expected=resultBuilder(getSession(),VARCHAR,VARCHAR,BOOLEAN,BOOLEAN,VARCHAR).row("orderkey","bigint",true,false,"").row("custkey","bigint",true,false,"").row("orderstatus","varchar",true,false,"").row("totalprice","double",true,false,"").row("orderdate","varchar",true,false,"").row("orderpriority","varchar",true,false,"").row("clerk","varchar",true,false,"").row("shippriority","bigint",true,false,"").row("comment","varchar",true,false,"").build();
  assertEquals(actual,expected);
}
