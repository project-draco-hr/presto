{
  MaterializedResult actual=computeActual("SHOW COLUMNS FROM orders");
  MaterializedResult expected=resultBuilder(getSession(),VARCHAR,VARCHAR,VARCHAR).row("orderkey","bigint","").row("custkey","bigint","").row("orderstatus","varchar","").row("totalprice","double","").row("orderdate","date","").row("orderpriority","varchar","").row("clerk","varchar","").row("shippriority","integer","").row("comment","varchar","").build();
  assertEquals(actual,expected);
}
