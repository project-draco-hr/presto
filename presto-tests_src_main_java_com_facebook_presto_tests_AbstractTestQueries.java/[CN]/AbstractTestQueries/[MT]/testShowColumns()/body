{
  MaterializedResult actual=computeActual("SHOW COLUMNS FROM orders");
  MaterializedResult expected=resultBuilder(getSession(),VARCHAR,VARCHAR,VARCHAR).row("orderkey","bigint","").row("custkey","bigint","").row("orderstatus",VARCHAR_MAX,"").row("totalprice","double","").row("orderdate","date","").row("orderpriority",VARCHAR_MAX,"").row("clerk",VARCHAR_MAX,"").row("shippriority","bigint","").row("comment",VARCHAR_MAX,"").build();
  assertEquals(actual,expected);
}
