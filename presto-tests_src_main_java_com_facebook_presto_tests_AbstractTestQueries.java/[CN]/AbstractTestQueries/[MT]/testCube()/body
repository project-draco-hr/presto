  assertQuery("SELECT linenumber, suppkey, SUM(quantity) FROM lineitem GROUP BY CUBE (linenumber, suppkey)","SELECT linenumber, suppkey, SUM(quantity) FROM lineitem GROUP BY linenumber, suppkey UNION ALL " + "SELECT linenumber, NULL, SUM(quantity) FROM lineitem GROUP BY linenumber UNION ALL " + "SELECT NULL, suppkey, SUM(quantity) FROM lineitem GROUP BY suppkey UNION ALL "+ "SELECT NULL, NULL, SUM(quantity) FROM lineitem");
