{
  assertQuery("SELECT linenumber, suppkey, SUM(DISTINCT quantity), COUNT(DISTINCT linestatus) FROM lineitem GROUP BY GROUPING SETS ((linenumber, suppkey), (suppkey))","SELECT linenumber, suppkey, SUM(DISTINCT quantity), COUNT(DISTINCT linestatus) FROM lineitem GROUP BY linenumber, suppkey UNION " + "SELECT NULL, suppkey, SUM(DISTINCT quantity), COUNT(DISTINCT linestatus) FROM lineitem GROUP BY suppkey");
}
