{
  String queryPrefix="SELECT * " + "FROM " + "    (VALUES 1,2,3) t(x) "+ "  JOIN "+ "    (VALUES 1,2,3) t2(y) "+ "  ON ";
  assertQuery(queryPrefix + " (x+y in (VALUES 4, 5))","VALUES (1,3), (2,2), (2,3), (3,1), (3,2)");
  assertQuery(queryPrefix + " (x+y in (VALUES 4, 5)) AND (x*y in (VALUES 4))","VALUES (2,2)");
  assertQuery(queryPrefix + " (x+y in (VALUES 4, 5)) = (x*y IN (VALUES 4,5))","VALUES (1,1), (1,2), (2,1), (2,2), (3,3)");
  assertQuery(queryPrefix + "(x+y in (VALUES (4),(5))) " + "AND "+ "(x in (VALUES 1)) != (y in (VALUES 3))","VALUES (2,3)");
  assertQuery(queryPrefix + "(x+y in (VALUES (4),(5))) " + "AND "+ "(x in (VALUES 1)) = (y in (VALUES 3))","VALUES (1,3), (2,2), (3,2), (3,1)");
}
