{
  Iterator<UncompressedBlock> blockIterator=client.execute(prepareGet().setUri(location).build(),new BlockResponseHandler(TupleInfo.SINGLE_VARBINARY));
  int count=0;
  while (blockIterator.hasNext()) {
    UncompressedBlock block=blockIterator.next();
    count+=block.getCount();
  }
  return count;
}
