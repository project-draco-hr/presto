{
  QueryDriversTupleStream tupleStream=new QueryDriversTupleStream(TupleInfo.SINGLE_VARBINARY,10,new HttpQueryProvider("query",httpClient,server1.getBaseUrl().resolve("/v1/presto/query")),new HttpQueryProvider("query",httpClient,server2.getBaseUrl().resolve("/v1/presto/query")),new HttpQueryProvider("query",httpClient,server3.getBaseUrl().resolve("/v1/presto/query")));
  int count=0;
  Cursor cursor=tupleStream.cursor();
  while (cursor.advanceNextPosition()) {
    count++;
  }
  assertEquals(count,312 * 3);
}
