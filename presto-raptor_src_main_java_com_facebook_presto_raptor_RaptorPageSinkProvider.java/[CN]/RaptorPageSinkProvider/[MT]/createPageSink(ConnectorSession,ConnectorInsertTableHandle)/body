{
  RaptorInsertTableHandle handle=checkType(tableHandle,RaptorInsertTableHandle.class,"tableHandle");
  return new RaptorPageSink(pageSorter,storageManager,shardInfoCodec,toColumnIds(handle.getColumnHandles()),handle.getColumnTypes(),Optional.empty(),toColumnIds(handle.getSortColumnHandles()),handle.getSortOrders(),maxBufferSize);
}
