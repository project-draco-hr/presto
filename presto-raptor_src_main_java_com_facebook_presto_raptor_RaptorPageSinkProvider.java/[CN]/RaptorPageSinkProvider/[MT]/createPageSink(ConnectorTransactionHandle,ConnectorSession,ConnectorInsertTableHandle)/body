{
  RaptorInsertTableHandle handle=checkType(tableHandle,RaptorInsertTableHandle.class,"tableHandle");
  return new RaptorPageSink(pageSorter,storageManager,shardInfoCodec,handle.getTransactionId(),toColumnIds(handle.getColumnHandles()),handle.getColumnTypes(),Optional.empty(),toColumnIds(handle.getSortColumnHandles()),handle.getSortOrders(),handle.getBucketCount(),toColumnIds(handle.getBucketColumnHandles()),maxBufferSize);
}
