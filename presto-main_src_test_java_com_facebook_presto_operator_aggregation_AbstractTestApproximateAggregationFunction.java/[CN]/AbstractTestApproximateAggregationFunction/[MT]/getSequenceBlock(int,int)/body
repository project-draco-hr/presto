{
  BlockBuilder blockBuilder=getType().createBlockBuilder(new BlockBuilderStatus());
  for (int i=start; i < start + length; i++) {
    if (getType() == BIGINT) {
      blockBuilder.appendLong((long)i);
    }
 else {
      blockBuilder.appendDouble((double)i);
    }
  }
  return blockBuilder.build();
}
