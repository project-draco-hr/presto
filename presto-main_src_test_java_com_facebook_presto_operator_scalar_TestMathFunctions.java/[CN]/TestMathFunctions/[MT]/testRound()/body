{
  assertFunction("round( 3)",3);
  assertFunction("round(-3)",-3);
  assertFunction("round( 3.0)",3);
  assertFunction("round(-3.0)",-3);
  assertFunction("round( 3.499)",3);
  assertFunction("round(-3.499)",-3);
  assertFunction("round( 3.5)",4);
  assertFunction("round(-3.5)",-3);
  assertFunction("round(-3.5001)",-4);
  assertFunction("round(-3.99)",-4);
}
