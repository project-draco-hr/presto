{
  InternalAggregationFunction function=METADATA.getExactFunction(new Signature("min_by",StandardTypes.VARCHAR,StandardTypes.VARCHAR,StandardTypes.DOUBLE)).getAggregationFunction();
  assertAggregation(function,1.0,"z",createPage(new String[]{"z","a"},new Double[]{1.0,2.0}),createPage(new String[]{"x","b"},new Double[]{2.0,3.0}));
  assertAggregation(function,1.0,"a",createPage(new String[]{"zz","hi"},new Double[]{0.0,1.0}),createPage(new String[]{"bb","a"},new Double[]{2.0,-1.0}));
}
