{
  long currentWallTime=System.nanoTime();
  long currentCpuTime=THREAD_MX_BEAN.getCurrentThreadCpuTime();
  long currentUserTime=THREAD_MX_BEAN.getCurrentThreadUserTime();
  return new CpuDuration(nanosBetween(intervalWallStart,currentWallTime),nanosBetween(intervalCpuStart,currentCpuTime),nanosBetween(intervalUserStart,currentUserTime));
}
