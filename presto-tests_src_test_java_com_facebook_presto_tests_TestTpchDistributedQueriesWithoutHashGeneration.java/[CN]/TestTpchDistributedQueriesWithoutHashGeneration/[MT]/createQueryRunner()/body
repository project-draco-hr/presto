{
  Session session=Session.builder().setUser("user").setSource("test").setCatalog("tpch").setSchema("tiny").setTimeZoneKey(UTC_KEY).setLocale(ENGLISH).build();
  DistributedQueryRunner queryRunner=new DistributedQueryRunner(session,4,ImmutableMap.of("optimizer.optimize-hash-generation","false"));
  try {
    queryRunner.installPlugin(new TpchPlugin());
    queryRunner.createCatalog("tpch","tpch");
    queryRunner.installPlugin(new SampledTpchPlugin());
    queryRunner.createCatalog("tpch_sampled","tpch_sampled");
    return queryRunner;
  }
 catch (  Exception e) {
    queryRunner.close();
    throw e;
  }
}
