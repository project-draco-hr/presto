{
synchronized (this) {
    if (noMoreLocations) {
      return;
    }
    noMoreLocations=true;
  }
  for (  WeakReference<ExchangeClient> activeClientReference : activeClients) {
    ExchangeClient activeClient=activeClientReference.get();
    if (activeClient != null) {
      activeClient.noMoreLocations();
    }
  }
}
