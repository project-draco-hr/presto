  return zoneId.equals("utc") || zoneId.equals("z") || zoneId.equals("ut")|| zoneId.equals("uct")|| zoneId.equals("ut")|| zoneId.equals("gmt")|| zoneId.equals("gmt0")|| zoneId.equals("greenwich")|| zoneId.equals("universal")|| zoneId.equals("zulu")|| zoneId.equals("+00:00")|| zoneId.equals("-00:00");
