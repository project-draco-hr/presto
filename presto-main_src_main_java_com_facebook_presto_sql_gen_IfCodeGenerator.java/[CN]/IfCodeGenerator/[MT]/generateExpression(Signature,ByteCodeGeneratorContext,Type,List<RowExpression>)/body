{
  Preconditions.checkArgument(arguments.size() == 3);
  Block condition=new Block(context.getContext()).append(context.generate(arguments.get(0))).comment("... and condition value was not null").getVariable("wasNull").invokeStatic(CompilerOperations.class,"not",boolean.class,boolean.class).invokeStatic(CompilerOperations.class,"and",boolean.class,boolean.class,boolean.class).putVariable("wasNull",false);
  return new IfStatement(context.getContext(),condition,context.generate(arguments.get(1)),context.generate(arguments.get(2)));
}
