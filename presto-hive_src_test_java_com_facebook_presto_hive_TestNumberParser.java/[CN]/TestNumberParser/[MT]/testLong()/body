{
  assertParseLong("1");
  assertParseLong("12");
  assertParseLong("123");
  assertParseLong("-1");
  assertParseLong("-12");
  assertParseLong("-123");
  assertParseLong("+1");
  assertParseLong("+12");
  assertParseLong("+123");
  assertParseLong("0");
  assertParseLong("-0");
  assertParseLong("+0");
  assertParseLong(Long.toString(Long.MAX_VALUE));
  assertParseLong(Long.toString(Long.MIN_VALUE));
}
