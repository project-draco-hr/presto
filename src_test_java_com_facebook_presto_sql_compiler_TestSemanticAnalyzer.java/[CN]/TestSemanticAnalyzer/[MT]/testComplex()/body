{
  String query="" + "SELECT id, SUM(x) total FROM (" + "      SELECT id, title, AVG(value) x, MIN(value) y "+ "      FROM T "+ "      WHERE (description LIKE '%hello%' AND id > 10) OR value < 5 "+ "      GROUP BY id, title "+ ") S "+ "WHERE id = title AND y > 20 "+ "GROUP BY id";
  process(query);
}
