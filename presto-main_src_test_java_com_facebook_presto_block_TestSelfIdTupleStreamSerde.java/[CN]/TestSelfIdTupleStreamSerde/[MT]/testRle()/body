{
  TupleStreamSerde serde=new SelfDescriptiveSerde(new RunLengthEncodedSerde());
  serde.createSerializer().createTupleStreamWriter(sliceOutput).append(tupleStream).finish();
  Blocks.assertTupleStreamEquals(SelfDescriptiveSerde.DESERIALIZER.deserialize(Range.ALL,sliceOutput.slice()),tupleStream);
}
