{
  if (this == obj) {
    return true;
  }
 else   if ((obj == null) || getClass() != obj.getClass()) {
    return false;
  }
  PeriodicImportJob other=(PeriodicImportJob)obj;
  return Objects.equal(srcTable,other.srcTable) && Objects.equal(dstTable,other.dstTable) && intervalSeconds == other.intervalSeconds;
}
