{
  boolean done=false;
  while (!done && !operatorStats.isDone()) {
    boolean processed=false;
    for (    Driver driver : drivers) {
      if (!driver.isFinished()) {
        driver.process();
        processed=true;
      }
    }
    Page page=inMemoryExchange.removePage();
    if (page != null) {
      return page;
    }
    done=!processed;
  }
  return endOfData();
}
