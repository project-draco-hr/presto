{
  try (SetThreadName setThreadName=new SetThreadName("Stage-%s",stageId)){
    for (    StageExecutionNode subStage : subStages.values()) {
      subStage.scheduleStartTasks();
    }
    return executor.submit(new Runnable(){
      @Override public void run(){
        startTasks();
      }
    }
);
  }
 }
