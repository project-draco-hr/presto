{
  closed.set(true);
  for (  HttpPageBufferClient client : allClients.values()) {
    closeQuietly(client);
  }
  pageBuffer.clear();
  bufferBytes=0;
  if (pageBuffer.peekLast() != NO_MORE_PAGES) {
    checkState(pageBuffer.add(NO_MORE_PAGES),"Could not add no more pages marker");
  }
  notifyBlockedCallers();
}
