{
  InPredicateRewriteResult inPredicateRewriteResult=rewriteInPredicates(context.defaultRewrite(node,context.get()),ImmutableList.of(node.getPredicate()));
  return new FilterNode(inPredicateRewriteResult.node.getId(),getOnlyElement(inPredicateRewriteResult.node.getSources()),replaceExpression(node.getPredicate(),inPredicateRewriteResult.inPredicateMapping));
}
