{
  InPredicateRewriteResult inPredicateRewriteResult=rewriteInPredicates(context.defaultRewrite(node,context.get()),ImmutableList.of(node.getPredicate()));
  inPredicateMappings.add(inPredicateRewriteResult.inPredicateMapping);
  return new FilterNode(inPredicateRewriteResult.node.getId(),getOnlyElement(inPredicateRewriteResult.node.getSources()),replaceInPredicates(node.getPredicate()));
}
