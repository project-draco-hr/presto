{
  InPredicateRewriteResult inPredicateRewriteResult=rewriteInPredicates(context.defaultRewrite(node,context.get()),node.getAssignments().values());
  return new ProjectNode(inPredicateRewriteResult.node.getId(),getOnlyElement(inPredicateRewriteResult.node.getSources()),rewriteAssignments(node,inPredicateRewriteResult.inPredicateMapping));
}
