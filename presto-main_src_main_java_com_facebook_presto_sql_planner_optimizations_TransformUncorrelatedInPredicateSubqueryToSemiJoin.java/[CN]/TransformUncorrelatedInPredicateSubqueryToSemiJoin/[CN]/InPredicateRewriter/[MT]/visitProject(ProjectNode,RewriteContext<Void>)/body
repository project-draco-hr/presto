{
  InPredicateRewriteResult inPredicateRewriteResult=rewriteInPredicates(context.defaultRewrite(node,context.get()),node.getAssignments().values());
  inPredicateMappings.add(inPredicateRewriteResult.inPredicateMapping);
  return new ProjectNode(inPredicateRewriteResult.node.getId(),getOnlyElement(inPredicateRewriteResult.node.getSources()),replaceInPredicateInAssignments(node));
}
