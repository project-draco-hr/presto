{
  try (FileChannel in=FileChannel.open(source,READ)){
    long size=in.size();
    try (FileChannel out=FileChannel.open(target,WRITE,CREATE_NEW)){
      long position=0;
      while (position < size) {
        position+=in.transferTo(position,size - position,out);
      }
      out.force(false);
    }
   }
 }
