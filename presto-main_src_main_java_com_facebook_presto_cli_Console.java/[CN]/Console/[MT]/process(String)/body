{
  if (line.endsWith(";")) {
    line=line.substring(0,line.length() - 1);
  }
  if (line.equalsIgnoreCase("quit")) {
    return false;
  }
  if (line.toLowerCase().startsWith("select ")) {
    line="sql:" + line;
  }
  Main.Execute query=new Main.Execute();
  query.server=server;
  query.query=line;
  query.run();
  return true;
}
