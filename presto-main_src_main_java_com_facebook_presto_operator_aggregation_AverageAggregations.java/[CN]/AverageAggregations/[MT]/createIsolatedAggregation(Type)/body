{
  Class<? extends AggregationFunction> functionClass=IsolatedClass.isolateClass(AggregationFunction.class,AverageAggregation.class,SimpleAggregationFunction.class,AverageGroupedAccumulator.class,SimpleGroupedAccumulator.class,AverageAccumulator.class,SimpleAccumulator.class);
  try {
    return functionClass.getConstructor(Type.class).newInstance(parameterType);
  }
 catch (  Exception e) {
    throw Throwables.propagate(e);
  }
