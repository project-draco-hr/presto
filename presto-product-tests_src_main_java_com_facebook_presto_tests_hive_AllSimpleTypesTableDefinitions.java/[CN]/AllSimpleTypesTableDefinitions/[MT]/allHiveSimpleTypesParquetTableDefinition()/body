{
  String tableName="parquet_all_types";
  HiveDataSource dataSource=createResourceDataSource(tableName,"" + System.currentTimeMillis(),"com/facebook/presto/tests/hive/data/all_types/data.parquet");
  return HiveTableDefinition.builder(tableName).setCreateTableDDLTemplate("" + "CREATE EXTERNAL TABLE %NAME%(" + "   c_tinyint            TINYINT,"+ "   c_smallint           SMALLINT,"+ "   c_int                INT,"+ "   c_bigint             BIGINT,"+ "   c_double             DOUBLE,"+ "   c_decimal            DECIMAL,"+ "   c_decimal_w_params   DECIMAL(10,5),"+ "   c_timestamp          TIMESTAMP,"+ "   c_string             STRING,"+ "   c_varchar            VARCHAR(10),"+ "   c_boolean            BOOLEAN,"+ "   c_binary             BINARY"+ ") "+ "STORED AS PARQUET "+ "LOCATION '%LOCATION%'").setDataSource(dataSource).build();
}
