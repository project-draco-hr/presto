{
  Blocks.assertTupleStreamEquals(ColumnMappingTupleStream.map(ColumnMappingTupleStream.map(baseTupleStream,2,1,0),0),Blocks.createTupleStream(0,"test","fuu","t"));
  Blocks.assertTupleStreamEquals(ColumnMappingTupleStream.map(ColumnMappingTupleStream.map(baseTupleStream,1,1),1),Blocks.createLongsTupleStream(0,15,0,-1));
}
