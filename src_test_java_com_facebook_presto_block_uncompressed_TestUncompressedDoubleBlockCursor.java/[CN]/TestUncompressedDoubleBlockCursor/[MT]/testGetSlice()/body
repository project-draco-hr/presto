{
  Cursor cursor=createCursor();
  try {
    cursor.getSlice(0);
    fail("Expected UnsupportedOperationException");
  }
 catch (  UnsupportedOperationException expected) {
  }
  assertTrue(cursor.advanceNextValue());
  try {
    cursor.getSlice(0);
    fail("Expected UnsupportedOperationException");
  }
 catch (  UnsupportedOperationException expected) {
  }
}
