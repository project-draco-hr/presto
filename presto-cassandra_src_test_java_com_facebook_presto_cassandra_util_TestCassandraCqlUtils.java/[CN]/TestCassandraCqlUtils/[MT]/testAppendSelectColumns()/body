{
  List<CassandraColumnHandle> columns=ImmutableList.of(new CassandraColumnHandle("","foo",0,CassandraType.VARCHAR,null,false,false),new CassandraColumnHandle("","bar",0,CassandraType.VARCHAR,null,false,false),new CassandraColumnHandle("","table",0,CassandraType.VARCHAR,null,false,false));
  StringBuilder sb=new StringBuilder();
  CassandraCqlUtils.appendSelectColumns(sb,columns);
  String str=sb.toString();
  assertEquals("foo,bar,\"table\"",str);
}
