{
  Object value=column(columnIndex);
  if (value == null) {
    return null;
  }
  Period period=INTERVAL_DAY_TO_SECOND_FORMATTER.parsePeriod(String.valueOf(value));
  return new PrestoIntervalDayTime(period.getValue(DAY_FIELD),period.getValue(HOUR_FIELD),period.getValue(MINUTE_FIELD),period.getValue(SECOND_FIELD),period.getValue(MILLIS_FIELD));
}
