{
  EqualityInference inference=new EqualityInference.Builder().addEquality(equalExpression("a1","b1")).addEquality(equalExpression("a1","c1")).addEquality(equalExpression("c1","a1")).build();
  Expression expression=inference.rewritePredicate(someExpression("a1","b1"),matchesSymbols("c1"));
  Assert.assertEquals(expression,someExpression("c1","c1"));
}
