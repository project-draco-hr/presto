{
  return new PrivateModule(){
    @Override protected void configure(){
      bind(ObjectMapper.class).toProvider(ObjectMapperProvider.class);
      bind(HttpClient.class).toInstance(new JettyHttpClient());
    }
    @Inject @Provides @Exposed @Singleton QueryStatsClient getQueryStatsClient(    HttpClient httpClient,    ObjectMapper objectMapper,    @Named("presto_rest.base_uri") String prestoRestInterfaceBaseUri){
      return new HttpQueryStatsClient(httpClient,objectMapper,URI.create(prestoRestInterfaceBaseUri));
    }
  }
;
}
