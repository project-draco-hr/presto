{
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("exchange.max-buffer-size","1GB").put("exchange.concurrent-request-multiplier","13").put("exchange.min-error-duration","13s").put("exchange.max-response-size","1kB").build();
  ExchangeClientConfig expected=new ExchangeClientConfig().setMaxBufferSize(new DataSize(1,Unit.GIGABYTE)).setConcurrentRequestMultiplier(13).setMinErrorDuration(new Duration(13,TimeUnit.SECONDS)).setMaxResponseSize(new DataSize(1,Unit.KILOBYTE));
  assertFullMapping(properties,expected);
}
