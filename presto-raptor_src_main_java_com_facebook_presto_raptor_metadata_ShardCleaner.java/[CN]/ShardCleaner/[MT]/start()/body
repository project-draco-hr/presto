{
  checkState(!enabled,"shard cleaner is currently broken and must be disabled");
  if (enabled) {
    if (started.compareAndSet(false,true)) {
      this.scheduledFuture.set(executorService.scheduleAtFixedRate(new ShardCleanerRunnable(),interval.toMillis(),interval.toMillis(),TimeUnit.MILLISECONDS));
    }
  }
}
