{
  this(dbi,nodeManager.getCurrentNode().getNodeIdentifier(),nodeManager.getCoordinators().contains(nodeManager.getCurrentNode()),storageService,backupStore,config.getMaxTransactionAge(),config.getLocalCleanerInterval(),config.getLocalCleanTime(),config.getLocalPurgeTime(),config.getBackupCleanerInterval(),config.getBackupCleanTime(),config.getBackupPurgeTime(),config.getBackupDeletionThreads());
}
