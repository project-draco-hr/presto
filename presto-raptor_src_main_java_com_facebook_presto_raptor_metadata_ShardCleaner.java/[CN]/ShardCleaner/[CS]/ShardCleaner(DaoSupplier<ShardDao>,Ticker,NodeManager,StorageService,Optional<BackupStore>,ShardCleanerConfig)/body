{
  this(shardDaoSupplier,nodeManager.getCurrentNode().getNodeIdentifier(),nodeManager.getCoordinators().contains(nodeManager.getCurrentNode()),ticker,storageService,backupStore,config.getMaxTransactionAge(),config.getTransactionCleanerInterval(),config.getLocalCleanerInterval(),config.getLocalCleanTime(),config.getBackupCleanerInterval(),config.getBackupCleanTime(),config.getBackupDeletionThreads());
}
