{
  String typeName=base;
  if (!parameters.isEmpty()) {
    typeName+="<";
    boolean first=true;
    for (    TypeSignature parameter : parameters) {
      if (!first) {
        typeName+=",";
      }
      first=false;
      typeName+=parameter.toString();
    }
    typeName+=">";
  }
  return typeName;
}
