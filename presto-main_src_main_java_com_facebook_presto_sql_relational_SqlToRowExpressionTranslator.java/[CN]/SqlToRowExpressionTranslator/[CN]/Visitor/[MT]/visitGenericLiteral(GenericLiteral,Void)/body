{
  Type type=metadata.getType(node.getType());
  if (type == null) {
    throw new IllegalArgumentException("Unsupported type: " + node.getType());
  }
  return call(castSignature(types.get(node),VARCHAR),types.get(node),constant(Slices.copiedBuffer(node.getValue(),StandardCharsets.UTF_8),VARCHAR));
}
