{
  RowExpression result=new Visitor(functionType,types,typeManager,session.getTimeZoneKey()).process(expression,null);
  requireNonNull(result,"translated expression is null");
  if (optimize) {
    ExpressionOptimizer optimizer=new ExpressionOptimizer(functionRegistry,typeManager,session);
    return optimizer.optimize(result);
  }
  return result;
}
