{
  ByteBuffer dst=toByteBuffer(index,length);
  while (dst.remaining() > 0) {
    int n=channelRead(in,dst,position + dst.position());
    if (n < 0) {
      if (dst.position() == 0) {
        return -1;
      }
      break;
    }
 else     if (n == 0) {
      break;
    }
  }
  return dst.position();
}
