{
  if (!context.isDefaultRewrite()) {
    PlanNode result=nodeRewriter.rewriteTopNRowNumber(node,context.get(),PlanRewriter.this);
    if (result != null) {
      return result;
    }
  }
  PlanNode source=rewrite(node.getSource(),context.get());
  if (source != node.getSource()) {
    return new TopNRowNumberNode(node.getId(),source,node.getPartitionBy(),node.getOrderBy(),node.getOrderings(),node.getRowNumberSymbol(),node.getMaxRowCountPerPartition(),false,node.getHashSymbol());
  }
  return node;
}
