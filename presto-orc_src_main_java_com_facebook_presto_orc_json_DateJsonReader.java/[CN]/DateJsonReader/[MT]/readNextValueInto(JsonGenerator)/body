{
  if (presentStream != null && !presentStream.nextBit()) {
    generator.writeNull();
    return;
  }
  if (dataStream == null) {
    throw new OrcCorruptionException("Value is not null but data stream is not present");
  }
  long millis=dataStream.next() * MILLIS_IN_DAY;
  generator.writeNumber(millis);
}
