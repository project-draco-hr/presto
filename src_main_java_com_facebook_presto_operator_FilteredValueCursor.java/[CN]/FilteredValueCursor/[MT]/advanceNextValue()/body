{
  isValid=true;
  do {
    if (!blockCursor.advanceToNextValue()) {
      if (iterator.hasNext()) {
        blockCursor=iterator.next().blockCursor();
        blockCursor.advanceNextPosition();
      }
 else {
        blockCursor=null;
        return false;
      }
    }
  }
 while (!predicate.apply(blockCursor));
  return true;
}
