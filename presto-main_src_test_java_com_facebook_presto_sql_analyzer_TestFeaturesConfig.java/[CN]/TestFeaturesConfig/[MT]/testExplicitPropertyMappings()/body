{
  Map<String,String> propertiesLegacy=new ImmutableMap.Builder<String,String>().put("analyzer.experimental-syntax-enabled","true").put("distributed-index-joins-enabled","true").put("distributed-joins-enabled","true").build();
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("experimental-syntax-enabled","true").put("distributed-index-joins-enabled","true").put("distributed-joins-enabled","true").build();
  FeaturesConfig expected=new FeaturesConfig().setExperimentalSyntaxEnabled(true).setDistributedIndexJoinsEnabled(true).setDistributedJoinsEnabled(true);
  assertFullMapping(properties,expected);
  assertDeprecatedEquivalence(FeaturesConfig.class,properties,propertiesLegacy);
}
