{
  Map<String,String> propertiesLegacy=new ImmutableMap.Builder<String,String>().put("analyzer.experimental-syntax-enabled","true").put("distributed-index-joins-enabled","true").put("distributed-joins-enabled","true").put("redistribute-writes","false").put("optimizer.optimize-metadata-queries","true").put("optimizer.optimize-hash-generation","true").put("optimizer.optimize-single-distinct","false").build();
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("experimental-syntax-enabled","true").put("distributed-index-joins-enabled","true").put("distributed-joins-enabled","true").put("redistribute-writes","false").put("optimizer.optimize-metadata-queries","true").put("optimizer.optimize-hash-generation","true").put("optimizer.optimize-single-distinct","false").build();
  FeaturesConfig expected=new FeaturesConfig().setExperimentalSyntaxEnabled(true).setDistributedIndexJoinsEnabled(true).setDistributedJoinsEnabled(true).setRedistributeWrites(false).setOptimizeMetadataQueries(true).setOptimizeHashGeneration(true).setOptimizeSingleDistinct(false);
  assertFullMapping(properties,expected);
  assertDeprecatedEquivalence(FeaturesConfig.class,properties,propertiesLegacy);
}
