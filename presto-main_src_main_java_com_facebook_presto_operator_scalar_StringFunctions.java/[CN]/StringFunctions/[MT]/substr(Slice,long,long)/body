{
  if ((start == 0) || (length <= 0)) {
    return Slices.EMPTY_SLICE;
  }
  if (start > 0) {
    start--;
  }
 else {
    start+=slice.length();
    if (start < 0) {
      return Slices.EMPTY_SLICE;
    }
  }
  if ((start + length) > slice.length()) {
    length=slice.length() - start;
  }
  if (start >= slice.length()) {
    return Slices.EMPTY_SLICE;
  }
  return slice.slice((int)start,(int)length);
}
