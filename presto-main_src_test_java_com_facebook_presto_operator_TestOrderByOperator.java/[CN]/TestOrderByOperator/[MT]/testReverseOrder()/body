{
  List<Page> input=rowPagesBuilder(SINGLE_LONG,SINGLE_DOUBLE).row(1,0.1).row(2,0.2).pageBreak().row(-1,-0.1).row(4,0.4).build();
  OrderByOperatorFactory operatorFactory=new OrderByOperatorFactory(0,ImmutableList.of(SINGLE_LONG,SINGLE_DOUBLE),new int[]{0},10,new int[]{0},new SortOrder[]{SortOrder.DESC_NULLS_LAST});
  Operator operator=operatorFactory.createOperator(driverContext);
  MaterializedResult expected=resultBuilder(FIXED_INT_64).row(4).row(2).row(1).row(-1).build();
  assertOperatorEquals(operator,input,expected);
}
