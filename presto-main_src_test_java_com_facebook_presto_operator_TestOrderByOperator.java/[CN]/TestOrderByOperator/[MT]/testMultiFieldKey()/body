{
  List<Page> input=rowPagesBuilder(SINGLE_VARBINARY,SINGLE_LONG).row("a",1).row("b",2).pageBreak().row("b",3).row("a",4).build();
  OrderByOperatorFactory operatorFactory=new OrderByOperatorFactory(0,ImmutableList.of(SINGLE_VARBINARY,SINGLE_LONG),new int[]{0,1},10,new int[]{0,1},new SortOrder[]{SortOrder.ASC_NULLS_LAST,SortOrder.DESC_NULLS_LAST});
  Operator operator=operatorFactory.createOperator(driverContext);
  MaterializedResult expected=MaterializedResult.resultBuilder(VARIABLE_BINARY,FIXED_INT_64).row("a",4).row("a",1).row("b",3).row("b",2).build();
  assertOperatorEquals(operator,input,expected);
}
