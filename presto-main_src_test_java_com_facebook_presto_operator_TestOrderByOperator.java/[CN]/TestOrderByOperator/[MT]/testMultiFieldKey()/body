{
  List<Page> input=rowPagesBuilder(VARCHAR,BIGINT).row("a",1).row("b",2).pageBreak().row("b",3).row("a",4).build();
  OrderByOperatorFactory operatorFactory=new OrderByOperatorFactory(0,ImmutableList.of(VARCHAR,BIGINT),ImmutableList.of(0,1),10,ImmutableList.of(0,1),ImmutableList.of(ASC_NULLS_LAST,DESC_NULLS_LAST));
  Operator operator=operatorFactory.createOperator(driverContext);
  MaterializedResult expected=MaterializedResult.resultBuilder(VARCHAR,BIGINT).row("a",4).row("a",1).row("b",3).row("b",2).build();
  assertOperatorEquals(operator,input,expected);
}
