{
  Preconditions.checkNotNull(queryId,"queryId is null");
  QueryWorker query=queries.get(queryId);
  if (query == null) {
    throw new NoSuchElementException();
  }
  try {
    return query.getQueryInfo();
  }
 catch (  RuntimeException e) {
    e.printStackTrace();
    queries.remove(queryId);
    throw e;
  }
}
