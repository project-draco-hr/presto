{
  this.masterQueryState=new MasterQueryState(queryId,new QueryState(ImmutableList.of(new TupleInfo(FIXED_INT_64,FIXED_INT_64,FIXED_INT_64)),1,pageBufferMax));
  this.executor=executor;
  this.codec=codec;
  httpClient=new ApacheHttpClient(new HttpClientConfig().setConnectTimeout(new Duration(5,TimeUnit.MINUTES)).setReadTimeout(new Duration(5,TimeUnit.MINUTES)));
  this.metadata=metadata;
  this.splitManager=splitManager;
}
