{
  this.queryState=queryState;
  this.codec=codec;
  ApacheHttpClient httpClient=new ApacheHttpClient(new HttpClientConfig().setConnectTimeout(new Duration(5,TimeUnit.MINUTES)).setReadTimeout(new Duration(5,TimeUnit.MINUTES)));
  asyncHttpClient=new AsyncHttpClient(httpClient,executor);
  this.metadata=metadata;
  this.splitManager=splitManager;
}
