{
  try {
    Operator dataStream=dataStreamProvider.createDataStream(split,columnHandles);
    HashJoinOperator hashJoinOperator=new HashJoinOperator(hashProvider,dataStream,0);
    AggregationOperator sumOperator=new AggregationOperator(hashJoinOperator,ImmutableList.of(partialAggregation(countAggregation(1,0)),partialAggregation(longSumAggregation(1,0)),partialAggregation(longSumAggregation(2,0))),ImmutableList.of(singleColumn(FIXED_INT_64,0,0),singleColumn(FIXED_INT_64,1,0),singleColumn(FIXED_INT_64,2,0)));
    for (    Page page : sumOperator) {
      queryState.addPage(page);
    }
    return null;
  }
 catch (  Exception e) {
    queryState.queryFailed(e);
    throw e;
  }
}
