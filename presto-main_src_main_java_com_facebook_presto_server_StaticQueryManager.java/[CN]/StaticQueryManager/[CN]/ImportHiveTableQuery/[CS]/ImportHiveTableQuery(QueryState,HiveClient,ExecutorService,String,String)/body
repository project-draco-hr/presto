{
  this.queryState=queryState;
  this.hiveClient=hiveClient;
  this.databaseName=databaseName;
  this.tableName=tableName;
  ApacheHttpClient httpClient=new ApacheHttpClient(new HttpClientConfig().setConnectTimeout(new Duration(15,TimeUnit.MINUTES)).setReadTimeout(new Duration(30,TimeUnit.MINUTES)));
  asyncHttpClient=new AsyncHttpClient(httpClient,executorService);
}
