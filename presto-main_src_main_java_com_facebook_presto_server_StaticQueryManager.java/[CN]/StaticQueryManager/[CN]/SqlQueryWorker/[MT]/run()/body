{
  try {
    taskScheduler.schedule(outputStage,stages);
    if (stages.get(outputStage.getStageId()).isEmpty()) {
      queryState.set(State.FINISHED);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    queryState.set(State.FAILED);
    cancel();
    throw Throwables.propagate(e);
  }
}
