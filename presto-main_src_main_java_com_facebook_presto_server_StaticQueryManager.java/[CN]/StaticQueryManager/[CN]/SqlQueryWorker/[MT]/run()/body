{
  try {
    Integer outputStage=taskScheduler.schedule(fragments,stages);
    if (!stages.isEmpty()) {
      this.outputStage.set(String.valueOf(outputStage));
    }
 else {
      queryState.set(State.FINISHED);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    queryState.set(State.FAILED);
    cancel();
    throw Throwables.propagate(e);
  }
}
