{
  Builder table=InMemoryRecordSet.builder(QUERY_TABLE);
  for (  QueryInfo queryInfo : queryManager.getAllQueryInfo()) {
    table.addRow(nodeId,queryInfo.getQueryId().toString(),queryInfo.getState().toString(),queryInfo.getSession().getUser(),queryInfo.getQuery(),MILLISECONDS.toSeconds(queryInfo.getQueryStats().getCreateTime().getMillis()));
  }
  return table.build().cursor();
}
