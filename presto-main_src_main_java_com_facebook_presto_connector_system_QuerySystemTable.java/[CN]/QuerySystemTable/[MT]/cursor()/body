{
  Builder table=InMemoryRecordSet.builder(QUERY_TABLE);
  for (  QueryInfo queryInfo : queryManager.getAllQueryInfo()) {
    QueryStats queryStats=queryInfo.getQueryStats();
    table.addRow(nodeId,queryInfo.getQueryId().toString(),queryInfo.getState().toString(),queryInfo.getSession().getUser(),queryInfo.getQuery(),toMillis(queryStats.getQueuedTime()),toMillis(queryStats.getAnalysisTime()),toMillis(queryStats.getDistributedPlanningTime()),toTimeStamp(queryStats.getCreateTime()),toTimeStamp(queryStats.getExecutionStartTime()),toTimeStamp(queryStats.getLastHeartbeat()),toTimeStamp(queryStats.getEndTime()));
  }
  return table.build().cursor();
}
