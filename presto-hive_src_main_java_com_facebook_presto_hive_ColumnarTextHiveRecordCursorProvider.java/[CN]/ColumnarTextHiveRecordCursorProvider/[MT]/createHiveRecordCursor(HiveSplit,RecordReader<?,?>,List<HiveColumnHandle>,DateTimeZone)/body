{
  if (usesColumnarTextSerDe(split)) {
    return Optional.<HiveRecordCursor>of(new ColumnarTextHiveRecordCursor<>(columnarTextRecordReader(recordReader),split.getLength(),split.getSchema(),split.getPartitionKeys(),columns,hiveStorageTimeZone,DateTimeZone.forID(split.getSession().getTimeZoneKey().getTimeZoneId())));
  }
  return Optional.absent();
}
