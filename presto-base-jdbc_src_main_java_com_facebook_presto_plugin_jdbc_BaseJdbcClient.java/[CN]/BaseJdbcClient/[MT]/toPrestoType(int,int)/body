{
switch (jdbcType) {
case Types.BIT:
case Types.BOOLEAN:
    return BOOLEAN;
case Types.TINYINT:
case Types.SMALLINT:
case Types.INTEGER:
  return INTEGER;
case Types.BIGINT:
return BIGINT;
case Types.FLOAT:
case Types.REAL:
case Types.DOUBLE:
case Types.NUMERIC:
case Types.DECIMAL:
return DOUBLE;
case Types.CHAR:
case Types.NCHAR:
case Types.VARCHAR:
case Types.NVARCHAR:
case Types.LONGVARCHAR:
case Types.LONGNVARCHAR:
return createVarcharType(min(columnSize,VarcharType.MAX_LENGTH));
case Types.BINARY:
case Types.VARBINARY:
case Types.LONGVARBINARY:
return VARBINARY;
case Types.DATE:
return DATE;
case Types.TIME:
return TIME;
case Types.TIMESTAMP:
return TIMESTAMP;
}
return null;
}
