{
  int index=0;
  for (  TupleInfo.Type type : tuple.getTupleInfo().getTypes()) {
switch (type) {
case FIXED_INT_64:
      append(tuple.getLong(index));
    break;
case VARIABLE_BINARY:
  append(tuple.getSlice(index));
break;
default :
throw new IllegalStateException("Type not yet supported: " + type);
}
index++;
}
}
