{
  CsvFileScanner firstColumn=new CsvFileScanner(inputSupplier,0,',');
  Assert.assertEquals(ImmutableList.copyOf(new PairsIterator(firstColumn.iterator())),ImmutableList.of(new Pair(0,"0"),new Pair(1,"1"),new Pair(2,"2"),new Pair(3,"3")));
  CsvFileScanner secondColumn=new CsvFileScanner(inputSupplier,1,',');
  Assert.assertEquals(ImmutableList.copyOf(new PairsIterator(secondColumn.iterator())),ImmutableList.of(new Pair(0,"apple"),new Pair(1,"banana"),new Pair(2,"cherry"),new Pair(3,"date")));
  CsvFileScanner thirdColumn=new CsvFileScanner(inputSupplier,2,',');
  Assert.assertEquals(ImmutableList.copyOf(new PairsIterator(thirdColumn.iterator())),ImmutableList.of(new Pair(0,"alice"),new Pair(1,"bob"),new Pair(2,"charlie"),new Pair(3,"dave")));
}
