{
  int sizeInBytes=this.sizeInBytes.get();
  if (sizeInBytes < 0) {
    assureLoaded();
    sizeInBytes=getSliceArraySizeInBytes(values);
    if (dictionary) {
      sizeInBytes+=ids.length * SIZE_OF_INT;
      sizeInBytes+=isNull.length * SIZE_OF_BYTE;
    }
    this.sizeInBytes.set(sizeInBytes);
  }
  return sizeInBytes;
}
