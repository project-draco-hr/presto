{
  return new HashAggregationOperator(new GroupByOperator(inputStreamProvider.getTupleStream(TpchSchema.LineItem.LINESTATUS,TupleStreamSerdes.Encoding.RAW)),new UncompressedBinaryOperator(inputStreamProvider.getTupleStream(TpchSchema.LineItem.ORDERKEY,TupleStreamSerdes.Encoding.RAW),inputStreamProvider.getTupleStream(TpchSchema.LineItem.PARTKEY,TupleStreamSerdes.Encoding.RAW),new SubtractionOperation()),SumAggregation.PROVIDER);
}
