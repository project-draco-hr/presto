{
  return new Function<TableMetadata,List<TableColumn>>(){
    @Override public List<TableColumn> apply(    TableMetadata input){
      ImmutableList.Builder<TableColumn> columns=ImmutableList.builder();
      int position=1;
      for (      ColumnMetadata column : input.getColumns()) {
        columns.add(new TableColumn(input.getTable(),column.getName(),position,column.getType()));
        position++;
      }
      return columns.build();
    }
  }
;
}
