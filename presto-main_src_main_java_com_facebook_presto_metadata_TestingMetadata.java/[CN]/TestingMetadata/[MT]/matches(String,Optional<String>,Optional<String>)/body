{
  checkTableName(catalogName,schemaName,tableName);
  return new Predicate<QualifiedTableName>(){
    @Override public boolean apply(    QualifiedTableName key){
      return catalogName.equals(key.getCatalogName()) && (!schemaName.isPresent() || schemaName.equals(key.getSchemaName())) && (!tableName.isPresent() || tableName.equals(key.getTableName()));
    }
  }
;
}
