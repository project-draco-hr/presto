{
  checkNotNull(prefix,"prefix is null");
  return new Predicate<QualifiedTableName>(){
    @Override public boolean apply(    QualifiedTableName key){
      return prefix.getCatalogName().equals(key.getCatalogName()) && (!prefix.hasSchemaName() || prefix.getSchemaName().get().equals(key.getSchemaName())) && (!prefix.hasTableName() || prefix.getTableName().get().equals(key.getTableName()));
    }
  }
;
}
