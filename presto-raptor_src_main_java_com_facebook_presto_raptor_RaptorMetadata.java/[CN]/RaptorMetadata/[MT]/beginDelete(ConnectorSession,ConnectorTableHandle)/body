{
  RaptorTableHandle handle=checkType(tableHandle,RaptorTableHandle.class,"tableHandle");
  long transactionId=shardManager.beginTransaction();
  setTransactionId(transactionId);
  return new RaptorTableHandle(connectorId,handle.getSchemaName(),handle.getTableName(),handle.getTableId(),handle.getDistributionId(),handle.getBucketCount(),OptionalLong.of(transactionId),handle.getSampleWeightColumnHandle(),true);
}
