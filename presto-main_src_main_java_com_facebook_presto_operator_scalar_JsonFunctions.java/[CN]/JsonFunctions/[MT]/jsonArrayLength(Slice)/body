{
  try (JsonParser parser=JSON_FACTORY.createParser(json.getInput())){
    if (parser.nextToken() != START_ARRAY) {
      return null;
    }
    long length=0;
    while (true) {
      JsonToken token=parser.nextToken();
      if (token == null) {
        return null;
      }
      if (token == END_ARRAY) {
        return length;
      }
      parser.skipChildren();
      length++;
    }
  }
 catch (  IOException e) {
    return null;
  }
}
