{
  try (JsonParser parser=JSON_FACTORY.createParser(json.getInput())){
    if (parser.nextToken() != START_ARRAY) {
      return null;
    }
    while (true) {
      JsonToken token=parser.nextToken();
      if (token == null) {
        return null;
      }
      if (token == END_ARRAY) {
        return false;
      }
      parser.skipChildren();
      if ((token == VALUE_NUMBER_INT) && ((parser.getNumberType() == NumberType.INT) || (parser.getNumberType() == NumberType.LONG)) && (parser.getLongValue() == value)) {
        return true;
      }
    }
  }
 catch (  IOException e) {
    return null;
  }
}
