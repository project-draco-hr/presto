{
  try {
    Slice extractedJson=JsonExtract.extractJson(json,jsonPath);
    if (extractedJson == null) {
      return null;
    }
    ObjectMapper mapper=new ObjectMapper();
    JsonNode rootNode=mapper.readTree(extractedJson.getInput());
    return new Long(rootNode.size());
  }
 catch (  Throwable e) {
    return null;
  }
}
