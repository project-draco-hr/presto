{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  final Partitioning other=(Partitioning)obj;
  return Objects.equals(this.partitioningColumns,other.partitioningColumns) && Objects.equals(this.hashingOrder,other.hashingOrder);
}
