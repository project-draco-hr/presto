{
  WindowFunctionSupplier windowFunctionSupplier=new ReflectionWindowFunctionSupplier<>(new Signature(name,returnType.getName(),Lists.transform(ImmutableList.copyOf(argumentTypes),nameGetter())),functionClass);
  functions.add(new FunctionInfo(windowFunctionSupplier.getSignature(),windowFunctionSupplier.getDescription(),windowFunctionSupplier));
  return this;
}
