{
  return selectEmpty("" + "SELECT TABLE_CAT, TABLE_SCHEM, TABLE_NAME, COLUMN_NAME, DATA_TYPE,\n" + "  COLUMN_SIZE, DECIMAL_DIGITS, NUM_PREC_RADIX, COLUMN_USAGE, REMARKS,\n"+ "  CHAR_OCTET_LENGTH, IS_NULLABLE\n"+ "FROM system.jdbc.pseudo_columns\n"+ "ORDER BY TABLE_CAT, table_SCHEM, TABLE_NAME, COLUMN_NAME");
}
