{
  return selectEmpty("" + "SELECT PROCEDURE_CAT, PROCEDURE_SCHEM, PROCEDURE_NAME, " + "  COLUMN_NAME, COLUMN_TYPE, DATA_TYPE, TYPE_NAME,\n"+ "  PRECISION, LENGTH, SCALE, RADIX,\n"+ "  NULLABLE, REMARKS, COLUMN_DEF, SQL_DATA_TYPE, SQL_DATETIME_SUB,\n"+ "  CHAR_OCTET_LENGTH, ORDINAL_POSITION, IS_NULLABLE, SPECIFIC_NAME\n"+ "FROM system.jdbc.procedure_columns\n"+ "ORDER BY PROCEDURE_CAT, PROCEDURE_SCHEM, PROCEDURE_NAME, SPECIFIC_NAME, COLUMN_NAME");
}
