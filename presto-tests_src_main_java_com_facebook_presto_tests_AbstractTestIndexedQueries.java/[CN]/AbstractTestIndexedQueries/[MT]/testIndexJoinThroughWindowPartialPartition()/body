{
  assertQuery("" + "SELECT *\n" + "FROM (\n"+ "  SELECT *\n"+ "  FROM lineitem\n"+ "  WHERE partkey % 16 = 0) l\n"+ "JOIN (\n"+ "  SELECT *, COUNT(*) OVER (PARTITION BY orderkey, custkey)\n"+ "  FROM orders) o\n"+ "  ON l.orderkey = o.orderkey","" + "SELECT *\n" + "FROM (\n"+ "  SELECT *\n"+ "  FROM lineitem\n"+ "  WHERE partkey % 16 = 0) l\n"+ "JOIN (\n"+ "  SELECT *, 1\n"+ "  FROM orders) o\n"+ "  ON l.orderkey = o.orderkey");
