{
  assertQuery("" + "SELECT *\n" + "FROM (\n"+ "  SELECT orderkey % 64 AS a, (suppkey % 2) + 1 AS b\n"+ "  FROM lineitem\n"+ "  WHERE partkey % 8 = 0) l\n"+ "JOIN (\n"+ "  SELECT orderkey AS a, SUM(LENGTH(comment)) % 2 AS b\n"+ "  FROM orders\n"+ "  GROUP BY orderkey) o\n"+ "  ON l.a = o.a AND l.b = o.b");
}
