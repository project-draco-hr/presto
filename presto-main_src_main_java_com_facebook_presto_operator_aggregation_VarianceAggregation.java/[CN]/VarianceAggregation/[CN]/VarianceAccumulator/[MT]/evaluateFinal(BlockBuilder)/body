{
  if (population) {
    if (calculator.getCount() == 0) {
      output.appendNull();
    }
 else {
      double result=calculator.getPopulationVariance();
      if (standardDeviation) {
        result=Math.sqrt(result);
      }
      output.append(result);
    }
  }
 else {
    if (calculator.getCount() < 2) {
      output.appendNull();
    }
 else {
      double result=calculator.getSampleVariance();
      if (standardDeviation) {
        result=Math.sqrt(result);
      }
      output.append(result);
    }
  }
}
