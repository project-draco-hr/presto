{
  return new OrcRecordReader(footer.getNumberOfRows(),footer.getStripes(),footer.getFileStats(),metadata.getStripeStatsList(),orcDataSource,offset,length,checkNotNull(tupleDomain,"tupleDomain is null"),checkNotNull(columnHandles,"columnHandles is null"),footer.getTypes(),compressionKind,bufferSize,footer.getRowsInRowGroup(),hiveStorageTimeZone,sessionTimeZone,metadataReader,typeManager);
}
