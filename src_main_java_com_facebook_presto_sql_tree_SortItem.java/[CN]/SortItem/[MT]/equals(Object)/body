{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  SortItem sortItem=(SortItem)o;
  if (nullOrdering != sortItem.nullOrdering) {
    return false;
  }
  if (ordering != sortItem.ordering) {
    return false;
  }
  if (!sortKey.equals(sortItem.sortKey)) {
    return false;
  }
  return true;
}
