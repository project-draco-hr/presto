{
  if (block == null) {
    return FINISHED;
  }
  if (index < 0) {
    index=0;
    offset=0;
    return SUCCESS;
  }
 else   if (index < block.getCount() - 1) {
    index++;
    offset+=SIZE_OF_LONG;
    return SUCCESS;
  }
 else   if (iterator.hasNext()) {
    block=iterator.next();
    index=0;
    offset=0;
    return SUCCESS;
  }
 else {
    block=null;
    index=-1;
    offset=-1;
    return FINISHED;
  }
}
