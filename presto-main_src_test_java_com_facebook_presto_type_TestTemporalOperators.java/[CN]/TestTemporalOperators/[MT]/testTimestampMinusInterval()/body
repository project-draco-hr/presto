{
  assertFunction("TIMESTAMP '2001-1-22 03:04:05.321' - INTERVAL '3' day",new Timestamp(new DateTime(2001,1,19,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("TIMESTAMP '2001-1-22 03:04:05.321 +05:09' - INTERVAL '3' day",new SqlTimestampWithTimeZone(new DateTime(2001,1,19,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
}
