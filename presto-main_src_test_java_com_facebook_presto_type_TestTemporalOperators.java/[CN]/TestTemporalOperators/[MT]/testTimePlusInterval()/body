{
  assertFunction("TIME '03:04:05.321' + INTERVAL '3' hour",new Time(new DateTime(1970,1,1,6,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("INTERVAL '3' hour + TIME '03:04:05.321'",new Time(new DateTime(1970,1,1,6,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("TIME '03:04:05.321' + INTERVAL '3' day",new Time(new DateTime(1970,1,1,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("INTERVAL '3' day + TIME '03:04:05.321'",new Time(new DateTime(1970,1,1,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("TIME '03:04:05.321' + INTERVAL '3' month",new Time(new DateTime(1970,1,1,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("INTERVAL '3' month + TIME '03:04:05.321'",new Time(new DateTime(1970,1,1,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("TIME '03:04:05.321' + INTERVAL '3' year",new Time(new DateTime(1970,1,1,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("INTERVAL '3' year + TIME '03:04:05.321'",new Time(new DateTime(1970,1,1,3,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("TIME '03:04:05.321' + INTERVAL '27' hour",new Time(new DateTime(1970,1,1,6,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("INTERVAL '27' hour + TIME '03:04:05.321'",new Time(new DateTime(1970,1,1,6,4,5,321,TIME_ZONE).getMillis()));
  assertFunction("TIME '03:04:05.321 +05:09' + INTERVAL '3' hour",new TimeWithTimeZone(new DateTime(1970,1,1,6,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("INTERVAL '3' hour + TIME '03:04:05.321 +05:09'",new TimeWithTimeZone(new DateTime(1970,1,1,6,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("TIME '03:04:05.321 +05:09' + INTERVAL '3' day",new TimeWithTimeZone(new DateTime(1970,1,1,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("INTERVAL '3' day + TIME '03:04:05.321 +05:09'",new TimeWithTimeZone(new DateTime(1970,1,1,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("TIME '03:04:05.321 +05:09' + INTERVAL '3' month",new TimeWithTimeZone(new DateTime(1970,1,1,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("INTERVAL '3' month + TIME '03:04:05.321 +05:09'",new TimeWithTimeZone(new DateTime(1970,1,1,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("TIME '03:04:05.321 +05:09' + INTERVAL '3' year",new TimeWithTimeZone(new DateTime(1970,1,1,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("INTERVAL '3' year + TIME '03:04:05.321 +05:09'",new TimeWithTimeZone(new DateTime(1970,1,1,3,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("TIME '03:04:05.321 +05:09' + INTERVAL '27' hour",new TimeWithTimeZone(new DateTime(1970,1,1,6,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
  assertFunction("INTERVAL '27' hour + TIME '03:04:05.321 +05:09'",new TimeWithTimeZone(new DateTime(1970,1,1,6,4,5,321,WEIRD_TIME_ZONE).getMillis(),WEIRD_TIME_ZONE_KEY));
}
