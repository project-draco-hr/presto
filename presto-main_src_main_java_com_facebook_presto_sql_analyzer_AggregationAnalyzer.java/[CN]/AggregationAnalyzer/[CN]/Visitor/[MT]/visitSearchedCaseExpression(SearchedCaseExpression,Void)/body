{
  if (isInGroupBy(node)) {
    return true;
  }
  for (  WhenClause whenClause : node.getWhenClauses()) {
    if (!process(whenClause.getOperand(),null) || !process(whenClause.getResult(),null)) {
      return false;
    }
  }
  if (node.getDefaultValue() != null && !process(node.getDefaultValue(),null)) {
    return false;
  }
  return true;
}
