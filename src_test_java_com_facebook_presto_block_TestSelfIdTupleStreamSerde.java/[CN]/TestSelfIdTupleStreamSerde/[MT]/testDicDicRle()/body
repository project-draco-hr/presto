{
  TupleStreamSerde serde=new SelfIdTupleStreamSerde(new DictionarySerde(new DictionarySerde(new RunLengthEncodedSerde())));
  serde.createSerializer().createTupleStreamWriter(sliceOutput).append(tupleStream).finish();
  Blocks.assertTupleStreamEquals(SelfIdTupleStreamSerde.DESERIALIZER.deserialize(sliceOutput.slice()),tupleStream);
}
