{
  if (object == this) {
    return true;
  }
 else   if (object == null || (getClass() != object.getClass())) {
    return false;
  }
  TaskSource that=(TaskSource)object;
  return Objects.equal(this.planNodeIds,that.planNodeIds) && Objects.equal(this.splits,that.splits) && this.noMoreSplits == that.noMoreSplits;
}
