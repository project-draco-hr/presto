{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ArithmeticExpression that=(ArithmeticExpression)o;
  if (!left.equals(that.left)) {
    return false;
  }
  if (!right.equals(that.right)) {
    return false;
  }
  if (type != that.type) {
    return false;
  }
  return true;
}
