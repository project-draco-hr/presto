{
  QueryDriversOperator operator=new QueryDriversOperator(10,TUPLE_INFOS,createHttpQueryProvider(server1),createHttpQueryProvider(server2),createHttpQueryProvider(server3));
  int count=0;
  QueryDriversIterator iterator=operator.iterator(new OperatorStats());
  while (count < 20 && iterator.hasNext()) {
    Page page=iterator.next();
    BlockCursor cursor=page.getBlock(0).cursor();
    while (count < 20 && cursor.advanceNextPosition()) {
      count++;
    }
  }
  assertEquals(count,20);
  assertTrue(iterator.hasNext());
  iterator.close();
  assertFalse(iterator.hasNext());
}
