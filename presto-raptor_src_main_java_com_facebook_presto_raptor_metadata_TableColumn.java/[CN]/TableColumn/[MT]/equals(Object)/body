{
  if (obj == this) {
    return true;
  }
  if ((obj == null) || (getClass() != obj.getClass())) {
    return false;
  }
  TableColumn o=(TableColumn)obj;
  return Objects.equals(table,o.table) && Objects.equals(columnName,o.columnName) && Objects.equals(ordinalPosition,o.ordinalPosition)&& Objects.equals(dataType,o.dataType);
}
