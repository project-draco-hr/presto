{
  double zScore=0;
  try {
    zScore=NORMAL_DISTRIBUTION.inverseCumulativeProbability((1 + confidence) / 2);
  }
 catch (  MathException e) {
    throw Throwables.propagate(e);
  }
  StringBuilder sb=new StringBuilder();
  sb.append(mean);
  sb.append(" +/- ");
  sb.append(zScore * Math.sqrt(variance / count));
  return sb.toString();
}
