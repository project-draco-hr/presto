{
  clazz=Primitives.unwrap(clazz);
  if (clazz == long.class) {
    return BIGINT;
  }
  if (clazz == double.class) {
    return DOUBLE;
  }
  if (clazz == Slice.class) {
    return VARCHAR;
  }
  if (clazz == boolean.class) {
    return BOOLEAN;
  }
  throw new IllegalArgumentException("Unhandled type: " + clazz.getName());
}
