{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  LocalFileColumnHandle that=(LocalFileColumnHandle)o;
  return Objects.equals(columnName,that.columnName) && Objects.equals(columnType,that.columnType) && Objects.equals(ordinalPosition,that.ordinalPosition);
}
