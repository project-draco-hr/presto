{
  StringBuilder sb=new StringBuilder("ColumnMetadata{");
  sb.append("name='").append(name).append('\'');
  sb.append(", type=").append(type);
  if (comment != null) {
    sb.append(", comment='").append(comment).append('\'');
  }
  if (hidden) {
    sb.append(", hidden");
  }
  sb.append('}');
  return sb.toString();
}
