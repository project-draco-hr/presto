{
  FunctionInfo function=generatorContext.getRegistry().getCoercion(fromType,toType);
  FunctionBinding binding=generatorContext.getBootstrapBinder().bindFunction(function.getSignature().getName(),generatorContext.generateGetSession(),ImmutableList.of(argument),function.getFunctionBinder());
  return generateFunctionCall(function.getSignature(),generatorContext.getContext(),binding,"cast(" + fromType + ", "+ toType+ ")");
}
