{
  ConfigAssertions.assertRecordedDefaults(ConfigAssertions.recordDefaults(QueryManagerConfig.class).setMaxQueryAge(new Duration(15,TimeUnit.MINUTES)).setMaxQueryHistory(100).setClientTimeout(new Duration(5,TimeUnit.MINUTES)).setMaxPendingSplitsPerNode(100).setQueryManagerExecutorPoolSize(5).setRemoteTaskMaxConsecutiveErrorCount(10).setRemoteTaskMinErrorDuration(new Duration(2,TimeUnit.MINUTES)));
}
