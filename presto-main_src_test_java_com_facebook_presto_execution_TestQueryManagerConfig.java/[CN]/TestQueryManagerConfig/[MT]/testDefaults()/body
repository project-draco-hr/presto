{
  ConfigAssertions.assertRecordedDefaults(ConfigAssertions.recordDefaults(QueryManagerConfig.class).setMaxQueryAge(new Duration(15,TimeUnit.MINUTES)).setMaxQueryHistory(100).setClientTimeout(new Duration(5,TimeUnit.MINUTES)).setScheduleSplitBatchSize(1000).setMaxConcurrentQueries(1000).setMaxQueuedQueries(5000).setQueueConfigFile(null).setInitialHashPartitions(8).setQueryManagerExecutorPoolSize(5).setRemoteTaskMaxConsecutiveErrorCount(10).setRemoteTaskMinErrorDuration(new Duration(2,TimeUnit.MINUTES)).setRemoteTaskMaxCallbackThreads(1000).setQueryExecutionPolicy("all-at-once").setQueryMaxRunTime(new Duration(100,TimeUnit.DAYS)));
}
