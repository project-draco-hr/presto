{
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("import.enabled","false").put("query.operator.max-memory","1GB").put("query.shard.max-threads","3").build();
  QueryManagerConfig expected=new QueryManagerConfig().setMaxOperatorMemoryUsage(new DataSize(1,Unit.GIGABYTE)).setMaxShardProcessorThreads(3).setImportsEnabled(false);
  ConfigAssertions.assertFullMapping(properties,expected);
}
