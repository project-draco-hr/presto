{
  if (usesColumnarBinarySerDe(split)) {
    return Optional.<HiveRecordCursor>of(new ColumnarBinaryHiveRecordCursor<>(bytesRecordReader(recordReader),split.getLength(),split.getSchema(),split.getPartitionKeys(),columns,DateTimeZone.forID(split.getSession().getTimeZoneKey().getId()),typeManager));
  }
  return Optional.absent();
}
