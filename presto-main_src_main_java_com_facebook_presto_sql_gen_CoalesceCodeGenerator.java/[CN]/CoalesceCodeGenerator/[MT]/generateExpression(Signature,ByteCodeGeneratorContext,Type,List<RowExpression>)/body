{
  List<ByteCodeNode> operands=new ArrayList<>();
  for (  RowExpression expression : arguments) {
    operands.add(generatorContext.generate(expression));
  }
  CompilerContext context=generatorContext.getContext();
  ByteCodeNode nullValue=new Block(context).putVariable(context.getVariable("wasNull"),true).pushJavaDefault(returnType.getJavaType());
  for (  ByteCodeNode operand : Lists.reverse(operands)) {
    IfStatement ifStatement=new IfStatement();
    ifStatement.condition().append(operand).append(context.getVariable("wasNull"));
    ifStatement.ifTrue().pop(returnType.getJavaType()).putVariable(context.getVariable("wasNull"),false).append(nullValue);
    nullValue=ifStatement;
  }
  return nullValue;
}
