{
  NewOperator operator=createAlignmentOperator();
  List<Page> expected=rowPagesBuilder(SINGLE_VARBINARY,SINGLE_LONG).row("alice",0).row("bob",1).row("charlie",2).row("dave",3).pageBreak().row("alice",4).row("bob",5).row("charlie",6).row("dave",7).pageBreak().row("alice",8).row("bob",9).row("charlie",10).row("dave",11).build();
  assertOperatorEquals(operator,expected);
}
