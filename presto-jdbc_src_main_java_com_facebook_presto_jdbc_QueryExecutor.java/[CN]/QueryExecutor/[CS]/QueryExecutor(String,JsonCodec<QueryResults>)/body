{
  checkNotNull(userAgent,"userAgent is null");
  checkNotNull(queryResultsCodec,"queryResultsCodec is null");
  this.queryInfoCodec=queryResultsCodec;
  this.httpClient=new StandaloneNettyAsyncHttpClient("jdbc",new HttpClientConfig().setConnectTimeout(new Duration(10,TimeUnit.SECONDS)),new NettyAsyncHttpClientConfig(),new NettyIoPoolConfig(),ImmutableSet.of(new UserAgentRequestFilter(userAgent)));
}
