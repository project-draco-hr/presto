{
  checkNotNull(userAgent,"userAgent is null");
  this.queryInfoCodec=checkNotNull(queryInfoCodec,"queryInfoCodec is null");
  this.httpClient=new StandaloneNettyAsyncHttpClient("jdbc",new HttpClientConfig().setConnectTimeout(new Duration(1,TimeUnit.DAYS)).setReadTimeout(new Duration(10,TimeUnit.DAYS)),new NettyAsyncHttpClientConfig(),new NettyIoPoolConfig(),ImmutableSet.of(new UserAgentRequestFilter(userAgent)));
}
