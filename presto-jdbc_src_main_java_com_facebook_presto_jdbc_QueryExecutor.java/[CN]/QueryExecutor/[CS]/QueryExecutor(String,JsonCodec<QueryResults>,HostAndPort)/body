{
  checkNotNull(userAgent,"userAgent is null");
  checkNotNull(queryResultsCodec,"queryResultsCodec is null");
  this.queryInfoCodec=queryResultsCodec;
  this.httpClient=new JettyHttpClient(new HttpClientConfig().setConnectTimeout(new Duration(10,TimeUnit.SECONDS)).setSocksProxy(socksProxy),ImmutableSet.of(new UserAgentRequestFilter(userAgent)));
}
