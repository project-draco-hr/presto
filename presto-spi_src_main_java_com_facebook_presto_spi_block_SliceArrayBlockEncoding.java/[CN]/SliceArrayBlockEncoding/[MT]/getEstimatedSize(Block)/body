{
  int positionCount=block.getPositionCount();
  int size=4;
  int totalLength=0;
  for (int position=0; position < positionCount; position++) {
    if (!block.isNull(position)) {
      totalLength+=block.getLength(position);
    }
    size+=4;
  }
  size+=positionCount / 8 + 1;
  size+=totalLength;
  return size;
}
