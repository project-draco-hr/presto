{
  int ordinalPosition=1;
  for (  ColumnMetadata column : columns) {
    if (column.isHidden()) {
      continue;
    }
    builder.addRow(tableName.getCatalogName(),tableName.getSchemaName(),tableName.getObjectName(),column.getName(),jdbcDataType(column.getType()),column.getType().getDisplayName(),0,0,decimalDigits(column.getType()),10,DatabaseMetaData.columnNullableUnknown,column.getComment(),null,null,null,0,ordinalPosition,"",null,null,null,null,null,null);
    ordinalPosition++;
  }
}
