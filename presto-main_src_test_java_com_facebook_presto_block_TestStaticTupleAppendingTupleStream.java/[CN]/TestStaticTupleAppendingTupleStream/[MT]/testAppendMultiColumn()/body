{
  StaticTupleAppendingTupleStream staticTupleAppendingTupleStream=new StaticTupleAppendingTupleStream(Blocks.createBlock(0,"abc","ddd","fff"),new TupleInfo(FIXED_INT_64,FIXED_INT_64).builder().append(5).append(-1).build());
  Blocks.assertTupleStreamEquals(staticTupleAppendingTupleStream,new BlockBuilder(0,new TupleInfo(VARIABLE_BINARY,FIXED_INT_64,FIXED_INT_64)).append("abc".getBytes(Charsets.UTF_8)).append(5).append(-1).append("ddd".getBytes(Charsets.UTF_8)).append(5).append(-1).append("fff".getBytes(Charsets.UTF_8)).append(5).append(-1).build());
}
