{
  Operator source=createOperator(new Page(BlockAssertions.createLongsBlock(1,2,3)),new Page(BlockAssertions.createLongsBlock(4,5)),new Page(BlockAssertions.createLongsBlock(6,7)));
  LimitOperator actual=new LimitOperator(source,4);
  Operator expected=createOperator(new Page(BlockAssertions.createLongsBlock(1,2,3)),new Page(BlockAssertions.createLongsBlock(4)));
  assertOperatorEquals(actual,expected);
}
