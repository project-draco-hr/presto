{
  Operator source=createOperator(new Page(BlockAssertions.createLongsBlock(0,1,2,3)),new Page(BlockAssertions.createLongsBlock(3,4,5)),new Page(BlockAssertions.createLongsBlock(5,6,7)));
  LimitOperator actual=new LimitOperator(source,4);
  Operator expected=createOperator(new Page(BlockAssertions.createLongsBlock(0,1,2,3)),new Page(BlockAssertions.createLongsBlock(3,4)));
  assertOperatorEquals(actual,expected);
}
