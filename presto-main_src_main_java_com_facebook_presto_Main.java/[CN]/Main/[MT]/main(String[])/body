{
  CliBuilder<Runnable> builder=Cli.buildCli("presto",Runnable.class).withDefaultCommand(Server.class).withCommand(Server.class).withCommand(Execute.class).withCommand(LocalQueryCommand.class).withCommand(Console.class).withCommands(Help.class);
  builder.withGroup("convert").withDescription("convert file formats").withDefaultCommand(Help.class).withCommand(ConvertCsv.class);
  Cli<Runnable> cli=builder.build();
  cli.parse(args).run();
}
