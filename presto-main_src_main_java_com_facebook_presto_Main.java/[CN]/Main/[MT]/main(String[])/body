{
  CliBuilder<Runnable> builder=Cli.<Runnable>builder("presto").withDefaultCommand(Server.class).withCommand(Server.class).withCommand(Execute.class).withCommand(Console.class).withCommand(Help.class);
  builder.withGroup("convert").withDescription("convert file formats").withDefaultCommand(Help.class).withCommand(ConvertCsv.class);
  Cli<Runnable> cli=builder.build();
  cli.parse(args).run();
}
