{
  return new Function<Class<?>,Type>(){
    @Override public Type apply(    Class<?> type){
      if (type == boolean.class) {
        return Type.BOOLEAN;
      }
      if (type == long.class) {
        return Type.LONG;
      }
      if (type == double.class) {
        return Type.DOUBLE;
      }
      if (type == String.class) {
        return Type.STRING;
      }
      throw new UnsupportedOperationException("Unsupported function type " + type);
    }
  }
;
}
