{
  TypedByteCodeNode value=process(node.getValue(),context);
  if (value.type == void.class) {
    return value;
  }
  Preconditions.checkState(value.type == boolean.class);
  return typedByteCodeNode(new Block(context).append(value.node).invokeStatic(Operations.class,"not",boolean.class,boolean.class),boolean.class);
}
