{
  TypedByteCodeNode value=process(node.getValue(),context);
  if (isNumber(value.type)) {
    if (value.type == long.class) {
      return typedByteCodeNode(new Block(context).append(value.node).invokeStatic(Operations.class,"negate",long.class,long.class),long.class);
    }
 else {
      return typedByteCodeNode(new Block(context).append(value.node).invokeStatic(Operations.class,"negate",double.class,double.class),double.class);
    }
  }
  throw new UnsupportedOperationException(String.format("not yet implemented: negate(%s)",value.type));
}
