{
  if (blockCursor == null) {
    return FINISHED;
  }
  hasAdvanced=true;
  AdvanceResult result=blockCursor.advanceNextValue();
  if (result != FINISHED) {
    return result;
  }
  while (iterator.canAdvance()) {
    blockCursor=iterator.next().cursor(session);
    result=blockCursor.advanceNextPosition();
    if (result != FINISHED) {
      return result;
    }
  }
  if (iterator.mustYield()) {
    return MUST_YIELD;
  }
  blockCursor=null;
  return FINISHED;
}
