{
  scanAssignments=ImmutableMap.<Symbol,ColumnHandle>builder().put(A,newColumnHandle("a")).put(B,newColumnHandle("b")).put(C,newColumnHandle("c")).put(D,newColumnHandle("d")).put(E,newColumnHandle("e")).put(F,newColumnHandle("f")).build();
  Map<Symbol,ColumnHandle> assignments=Maps.filterKeys(scanAssignments,Predicates.in(ImmutableList.of(A,B,C,D,E,F)));
  baseTableScan=new TableScanNode(newId(),DUAL_TABLE_HANDLE,ImmutableList.copyOf(assignments.keySet()),assignments,null,Optional.empty());
  expressionNormalizer=new ExpressionIdentityNormalizer();
}
