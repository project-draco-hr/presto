{
  PlanNode node=new UnionNode(newId(),ImmutableList.<PlanNode>of(filter(baseTableScan,greaterThan(AE,number(10))),filter(baseTableScan,and(greaterThan(AE,number(10)),lessThan(AE,number(100)))),filter(baseTableScan,and(greaterThan(AE,number(10)),lessThan(AE,number(100))))),ImmutableListMultimap.of(A,B,A,C,A,E));
  Expression effectivePredicate=EffectivePredicateExtractor.extract(node,TYPES);
  Assert.assertEquals(normalizeConjuncts(effectivePredicate),normalizeConjuncts(greaterThan(AE,number(10))));
}
