{
  return retry().stopOn(NoSuchObjectException.class).stopOnIllegalExceptions().run("getPartitionNamesByParts",new Callable<List<String>>(){
    @Override public List<String> call() throws Exception {
      try (HiveMetastoreClient client=clientProvider.createMetastoreClient()){
        return client.get_partition_names_ps(partitionFilter.getHiveTableName().getDatabaseName(),partitionFilter.getHiveTableName().getTableName(),partitionFilter.getParts(),(short)-1);
      }
     }
  }
);
}
