{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  BufferInfo that=(BufferInfo)o;
  if (bufferedPages != that.bufferedPages) {
    return false;
  }
  if (finished != that.finished) {
    return false;
  }
  if (!bufferId.equals(that.bufferId)) {
    return false;
  }
  return true;
}
