{
  IOException exception=null;
  for (int i=0; i <= maxClientRetry; i++) {
    try {
      openStream();
      int b=in.read();
      if (b != -1) {
        position++;
      }
      return b;
    }
 catch (    IOException e) {
      log.debug("Failed reading from S3 on attempt %s: %s",i + 1,e.getMessage());
      exception=e;
      closeStream();
    }
  }
  throw exception;
}
