{
  BlockCursor cursor=createCursor();
  try {
    cursor.getSlice(0);
    fail("Expected UnsupportedOperationException");
  }
 catch (  UnsupportedOperationException expected) {
  }
  assertTrue(cursor.advanceToNextValue());
  try {
    cursor.getSlice(0);
    fail("Expected UnsupportedOperationException");
  }
 catch (  UnsupportedOperationException expected) {
  }
}
