{
  assertThat(query("SHOW COLUMNS FROM parquet_all_types",QueryType.SELECT).project(1,2)).containsExactly(row("c_tinyint","tinyint"),row("c_smallint","smallint"),row("c_int","integer"),row("c_bigint","bigint"),row("c_double","double"),row("c_decimal","decimal(10,0)"),row("c_decimal_w_params","decimal(10,5)"),row("c_timestamp","timestamp"),row("c_string","varchar"),row("c_varchar","varchar(10)"),row("c_boolean","boolean"),row("c_binary","varbinary"));
  assertThat(query("SELECT * FROM parquet_all_types")).containsOnly(row(127,32767,2147483647,9223372036854775807L,234.567,new BigDecimal("346"),new BigDecimal("345.67800"),parseTimestampInUTC("2015-05-10 12:15:35.123"),"ala ma kota","ala ma kot",true,"kot binarny".getBytes()));
}
