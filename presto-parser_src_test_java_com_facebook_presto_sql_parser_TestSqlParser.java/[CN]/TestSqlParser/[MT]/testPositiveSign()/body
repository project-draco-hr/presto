{
  assertExpression("9",new LongLiteral("9"));
  assertExpression("+9",new LongLiteral("9"));
  assertExpression("++9",new LongLiteral("9"));
  assertExpression("+++9",new LongLiteral("9"));
  assertExpression("+9",new LongLiteral("9"));
  assertExpression("+ +9",new LongLiteral("9"));
  assertExpression("+ + +9",new LongLiteral("9"));
  assertExpression("+ 9",new LongLiteral("9"));
  assertExpression("+ + 9",new LongLiteral("9"));
  assertExpression("+ + + 9",new LongLiteral("9"));
}
