{
  Expression expression=new LongLiteral("9");
  assertExpression("9",expression);
  expression=new NegativeExpression(expression);
  assertExpression("-9",expression);
  assertExpression("- 9",expression);
  assertExpression("- + 9",expression);
  assertExpression("+ - + 9",expression);
  assertExpression("-+9",expression);
  assertExpression("+-+9",expression);
  expression=new NegativeExpression(expression);
  assertExpression("- -9",expression);
  assertExpression("- - 9",expression);
  assertExpression("- + - + 9",expression);
  assertExpression("+ - + - + 9",expression);
  assertExpression("-+-+9",expression);
  assertExpression("+-+-+9",expression);
  expression=new NegativeExpression(expression);
  assertExpression("- - -9",expression);
  assertExpression("- - - 9",expression);
}
