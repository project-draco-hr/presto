{
  return new Callable<Void>(){
    @Override public Void call(){
      executions.add(group);
      if (meetingPoint != null) {
        try {
          meetingPoint.waitForNextPhase();
        }
 catch (        Exception e) {
          throw Throwables.propagate(e);
        }
      }
      return null;
    }
  }
;
}
