{
  TaskInfo info=taskInfo.get();
  if (isSocketError(t)) {
    log.warn("%s task %s: %s: %s","Error updating",info.getTaskId(),t.getMessage(),info.getSelf());
  }
 else {
    log.warn(t,"%s task %s: %s","Error updating",info.getTaskId(),info.getSelf());
  }
  errorRequestRateLimiter.acquire();
  reschedule();
}
