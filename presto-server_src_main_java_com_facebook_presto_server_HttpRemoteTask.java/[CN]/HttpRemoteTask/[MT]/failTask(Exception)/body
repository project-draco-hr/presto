{
  TaskInfo taskInfo=getTaskInfo();
  updateTaskInfo(new TaskInfo(taskInfo.getTaskId(),TaskInfo.MAX_VERSION,TaskState.FAILED,taskInfo.getSelf(),taskInfo.getOutputBuffers(),taskInfo.getNoMoreSplits(),taskInfo.getStats(),ImmutableList.<SplitExecutionStats>of(),ImmutableList.of(toFailure(cause))));
}
