  jsonCodecBinder(binder).bindJsonCodec(TaskUpdateRequest.class);
  jsonCodecBinder(binder).bindJsonCodec(Split.class);
  jsonBinder(binder).addSerializerBinding(Expression.class).to(ExpressionSerializer.class);
