).toProvider(PlanOptimizersFactory.class).in(Scopes.SINGLETON);
  binder.bind(NodeResource.class).in(Scopes.SINGLETON);
  binder.bind(StorageManager.class).to(DatabaseStorageManager.class).in(Scopes.SINGLETON);
