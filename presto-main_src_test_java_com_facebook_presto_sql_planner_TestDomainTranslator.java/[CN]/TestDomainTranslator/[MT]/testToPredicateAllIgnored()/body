{
  TupleDomain<ColumnHandle> tupleDomain=withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(ACH,Domain.singleValue(1L)).put(BCH,Domain.onlyNull(Double.class)).put(CCH,Domain.notNull(Slice.class)).put(DCH,Domain.all(Boolean.class)).build());
  ExtractionResult result=fromPredicate(toPredicate(tupleDomain));
  assertEquals(result.getRemainingExpression(),TRUE_LITERAL);
  assertEquals(result.getTupleDomain(),withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(ACH,Domain.singleValue(1L)).put(BCH,Domain.onlyNull(Double.class)).put(CCH,Domain.notNull(Slice.class)).build()));
}
