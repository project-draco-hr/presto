{
  TupleDomain tupleDomain=withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(ACH,Domain.singleValue(1L)).put(BCH,Domain.onlyNull(Double.class)).put(CCH,Domain.notNull(Slice.class)).put(DCH,Domain.all(Boolean.class)).build());
  ExtractionResult result=fromPredicate(toPredicate(tupleDomain,COLUMN_HANDLES.inverse(),TYPES),TYPES,COLUMN_HANDLES);
  Assert.assertEquals(result.getRemainingExpression(),TRUE_LITERAL);
  Assert.assertEquals(result.getTupleDomain(),withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(ACH,Domain.singleValue(1L)).put(BCH,Domain.onlyNull(Double.class)).put(CCH,Domain.notNull(Slice.class)).build()));
}
