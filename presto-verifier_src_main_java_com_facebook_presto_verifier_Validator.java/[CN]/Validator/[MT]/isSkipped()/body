{
  if (!checkCorrectness) {
    return false;
  }
  if (queryPair.getControl().getQuery().isEmpty() || queryPair.getTest().getQuery().isEmpty()) {
    return true;
  }
  if (getControlResult().getState() != State.SUCCESS) {
    return true;
  }
  if (!isDeterministic()) {
    return true;
  }
  return false;
}
