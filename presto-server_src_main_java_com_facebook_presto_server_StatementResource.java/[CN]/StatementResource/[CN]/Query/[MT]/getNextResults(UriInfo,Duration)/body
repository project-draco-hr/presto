{
  QueryData data=getData(maxWaitTime);
  QueryInfo queryInfo=queryManager.getQueryInfo(queryId,false);
  URI nextResultsUri=null;
  if (!queryInfo.getState().isDone()) {
    nextResultsUri=createNextResultsUri(uriInfo);
  }
  QueryResults queryResults=new QueryResults(uriInfo.getRequestUriBuilder().replaceQuery("").replacePath(queryInfo.getSelf().getPath()).build(),nextResultsUri,data,toStatementStats(uriInfo,queryInfo));
  if (lastResult != null) {
    lastResultPath=lastResult.getNext().getPath();
  }
 else {
    lastResultPath=null;
  }
  lastResult=queryResults;
  return queryResults;
}
