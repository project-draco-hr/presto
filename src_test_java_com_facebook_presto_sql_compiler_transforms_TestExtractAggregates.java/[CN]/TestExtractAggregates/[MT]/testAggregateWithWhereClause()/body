{
  assertRewrite("SELECT orderkey id, SUM(totalprice) * AVG(totalprice) c\n" + "FROM ORDERS\n" + "WHERE totalprice > 10 \n"+ "GROUP BY orderkey","SELECT _R0._a0 id, (_R0._a1 * _R0._a2) c\n" + "FROM (\n" + "   SELECT orderkey _a0, SUM(totalprice) _a1, AVG(totalprice) _a2\n"+ "   FROM ORDERS\n"+ "   WHERE totalprice > 10"+ "   GROUP BY orderkey\n"+ ") _R0");
}
