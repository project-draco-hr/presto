{
  return new PipelineStats(inputPipeline,outputPipeline,totalDrivers,queuedDrivers,queuedPartitionedDrivers,runningDrivers,runningPartitionedDrivers,completedDrivers,memoryReservation,queuedTime,elapsedTime,totalScheduledTime,totalCpuTime,totalUserTime,totalBlockedTime,fullyBlocked,blockedReasons,rawInputDataSize,rawInputPositions,processedInputDataSize,processedInputPositions,outputDataSize,outputPositions,operatorSummaries,ImmutableList.<DriverStats>of());
}
