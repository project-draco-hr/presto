{
  TupleDomain<ColumnHandle> tupleDomain1=TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(A,Domain.all(String.class)).put(B,Domain.notNull(Double.class)).put(C,Domain.onlyNull(Long.class)).put(D,Domain.singleValue(1L)).put(E,Domain.create(SortedRangeSet.of(Range.greaterThanOrEqual(0.0)),true)).build());
  TupleDomain<ColumnHandle> tupleDomain2=TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(A,Domain.singleValue("value")).put(B,Domain.singleValue(0.0)).put(C,Domain.notNull(Long.class)).put(D,Domain.singleValue(1L)).put(E,Domain.create(SortedRangeSet.of(Range.lessThan(10.0)),false)).build());
  TupleDomain<ColumnHandle> expectedTupleDomain=TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>builder().put(A,Domain.all(String.class)).put(B,Domain.notNull(Double.class)).put(C,Domain.all(Long.class)).put(D,Domain.singleValue(1L)).put(E,Domain.all(Double.class)).build());
  Assert.assertEquals(columnWiseUnion(tupleDomain1,tupleDomain2),expectedTupleDomain);
}
