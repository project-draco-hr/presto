{
  Assert.assertTrue(TupleDomain.all().isAll());
  Assert.assertEquals(TupleDomain.<ColumnHandle>all(),TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.all(Long.class))));
  Assert.assertEquals(TupleDomain.<ColumnHandle>all(),TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of()));
}
