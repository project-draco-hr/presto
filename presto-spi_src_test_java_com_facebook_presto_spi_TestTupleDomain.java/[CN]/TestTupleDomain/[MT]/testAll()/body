{
  Assert.assertTrue(TupleDomain.all().isAll());
  Assert.assertEquals(TupleDomain.all(),TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of(A,Domain.all(Long.class))));
  Assert.assertEquals(TupleDomain.all(),TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of()));
}
