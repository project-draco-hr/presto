{
  Assert.assertTrue(TupleDomain.all().isAll());
  Assert.assertEquals(TupleDomain.<ConnectorColumnHandle>all(),TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.all(Long.class))));
  Assert.assertEquals(TupleDomain.<ConnectorColumnHandle>all(),TupleDomain.withColumnDomains(ImmutableMap.<ConnectorColumnHandle,Domain>of()));
}
