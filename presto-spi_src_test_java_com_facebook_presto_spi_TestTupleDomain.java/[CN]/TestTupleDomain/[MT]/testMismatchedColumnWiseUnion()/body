{
  TupleDomain tupleDomain1=TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of(A,Domain.all(Double.class),B,Domain.singleValue("value")));
  TupleDomain tupleDomain2=TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of(A,Domain.create(SortedRangeSet.of(Range.greaterThanOrEqual(0.0)),true),C,Domain.singleValue(1L)));
  TupleDomain expectedTupleDomain=TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of(A,Domain.all(Double.class)));
  Assert.assertEquals(columnWiseUnion(tupleDomain1,tupleDomain2),expectedTupleDomain);
}
