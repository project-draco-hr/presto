{
  Assert.assertTrue(TupleDomain.none().isNone());
  Assert.assertEquals(TupleDomain.none(),TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of(A,Domain.none(Long.class))));
  Assert.assertEquals(TupleDomain.none(),TupleDomain.withColumnDomains(ImmutableMap.<ColumnHandle,Domain>of(A,Domain.all(Long.class),B,Domain.none(String.class))));
}
