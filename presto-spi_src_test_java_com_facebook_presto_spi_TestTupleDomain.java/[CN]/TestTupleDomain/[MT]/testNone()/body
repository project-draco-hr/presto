{
  Assert.assertTrue(TupleDomain.none().isNone());
  Assert.assertEquals(TupleDomain.<ConnectorColumnHandle>none(),TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.none(Long.class))));
  Assert.assertEquals(TupleDomain.<ConnectorColumnHandle>none(),TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.all(Long.class),B,Domain.none(String.class))));
}
