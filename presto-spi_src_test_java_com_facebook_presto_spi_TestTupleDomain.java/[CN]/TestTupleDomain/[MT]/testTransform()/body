{
  Map<Integer,Domain> domains=ImmutableMap.<Integer,Domain>builder().put(1,Domain.singleValue(1)).put(2,Domain.singleValue(2)).put(3,Domain.singleValue(3)).build();
  TupleDomain<Integer> domain=TupleDomain.withColumnDomains(domains);
  TupleDomain<String> transformed=domain.transform(new TupleDomain.Function<Integer,String>(){
    @Override public String apply(    Integer input){
      return input.toString();
    }
  }
);
  Map<String,Domain> expected=ImmutableMap.<String,Domain>builder().put("1",Domain.singleValue(1)).put("2",Domain.singleValue(2)).put("3",Domain.singleValue(3)).build();
  assertEquals(transformed.getDomains(),expected);
}
