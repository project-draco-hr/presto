{
  TupleDomain<ConnectorColumnHandle> tupleDomain1=TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.all(Double.class),B,Domain.singleValue("value")));
  TupleDomain<ConnectorColumnHandle> tupleDomain2=TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.create(SortedRangeSet.of(Range.greaterThanOrEqual(0.0)),true),C,Domain.singleValue(1L)));
  TupleDomain<ConnectorColumnHandle> expectedTupleDomain=TupleDomain.withColumnDomains(ImmutableMap.of(A,Domain.create(SortedRangeSet.of(Range.greaterThanOrEqual(0.0)),true),B,Domain.singleValue("value"),C,Domain.singleValue(1L)));
  Assert.assertEquals(tupleDomain1.intersect(tupleDomain2),expectedTupleDomain);
}
