{
  Operator source=createOperator(new Page(BlockAssertions.createLongsBlock(1,2),BlockAssertions.createDoublesBlock(0.1,0.2)),new Page(BlockAssertions.createLongsBlock(-1,4),BlockAssertions.createDoublesBlock(-0.1,0.4)));
  InMemoryOrderByOperator actual=new InMemoryOrderByOperator(source,0,ImmutableList.of(singleColumn(FIXED_INT_64,0,0),singleColumn(DOUBLE,1,0)));
  Operator expected=createOperator(new Page(BlockAssertions.createLongsBlock(-1,1,2,4),BlockAssertions.createDoublesBlock(-0.1,0.1,0.2,0.4)));
  assertOperatorEquals(actual,expected);
}
