{
  while (true) {
    Iterable<List<Object>> data=client.current().getData();
    if (data != null) {
      for (      List<Object> tuple : data) {
        handler.processRow(Collections.unmodifiableList(tuple));
      }
    }
    if (!client.hasNext()) {
      break;
    }
    client.next();
  }
}
