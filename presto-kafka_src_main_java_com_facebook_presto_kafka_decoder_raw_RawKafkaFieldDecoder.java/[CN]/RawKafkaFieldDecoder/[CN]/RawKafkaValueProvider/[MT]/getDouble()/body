{
  if (isNull()) {
    return 0.0d;
  }
switch (fieldType) {
case FLOAT:
    return value.getFloat();
case DOUBLE:
  return value.getDouble();
default :
throw new PrestoException(KafkaErrorCode.KAFKA_CONVERSION_NOT_SUPPORTED.toErrorCode(),format("conversion %s to double not supported",fieldType));
}
}
