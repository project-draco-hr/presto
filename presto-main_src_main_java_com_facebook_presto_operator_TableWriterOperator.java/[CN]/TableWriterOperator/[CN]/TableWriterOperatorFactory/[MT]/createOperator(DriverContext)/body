{
  try {
    OperatorContext operatorContext=driverContext.addOperatorContext(operatorId,TableWriterOperator.class.getSimpleName());
    return new TableWriterOperator(operatorContext,sourceId,storageManager,nodeIdentifier,columnHandles);
  }
 catch (  IOException e) {
    throw Throwables.propagate(e);
  }
}
