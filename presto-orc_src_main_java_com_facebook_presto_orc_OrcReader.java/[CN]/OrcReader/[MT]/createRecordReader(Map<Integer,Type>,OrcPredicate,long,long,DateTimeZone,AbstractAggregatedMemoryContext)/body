{
  return new OrcRecordReader(requireNonNull(includedColumns,"includedColumns is null"),requireNonNull(predicate,"predicate is null"),footer.getNumberOfRows(),footer.getStripes(),footer.getFileStats(),metadata.getStripeStatsList(),orcDataSource,offset,length,footer.getTypes(),compressionKind,bufferSize,footer.getRowsInRowGroup(),requireNonNull(hiveStorageTimeZone,"hiveStorageTimeZone is null"),metadataReader,maxMergeDistance,maxReadSize,systemMemoryUsage);
}
