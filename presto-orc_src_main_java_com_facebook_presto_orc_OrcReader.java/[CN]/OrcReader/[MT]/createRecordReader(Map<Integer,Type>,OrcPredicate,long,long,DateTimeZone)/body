{
  return new OrcRecordReader(checkNotNull(includedColumns,"includedColumns is null"),checkNotNull(predicate,"predicate is null"),footer.getNumberOfRows(),footer.getStripes(),footer.getFileStats(),metadata.getStripeStatsList(),orcDataSource,offset,length,footer.getTypes(),compressionKind,bufferSize,footer.getRowsInRowGroup(),checkNotNull(hiveStorageTimeZone,"hiveStorageTimeZone is null"),metadataReader);
}
