{
  assertFunction("JSON_ARRAY_GET('[\"jhfa\"]', 0)","jhfa");
  assertFunction("JSON_ARRAY_GET('[\"jhfa\", null]', 1)",null);
  assertFunction("JSON_ARRAY_GET('[\"as\", \"fgs\", \"tehgf\"]', 1)","fgs");
  assertFunction("JSON_ARRAY_GET('[\"as\", \"fgs\", \"tehgf\", \"gjyj\", \"jut\"]', 4)","jut");
}
