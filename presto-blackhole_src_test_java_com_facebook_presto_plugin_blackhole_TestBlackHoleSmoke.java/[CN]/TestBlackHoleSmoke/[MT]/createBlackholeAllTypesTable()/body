{
  assertThatQueryReturnsValue(format("CREATE TABLE blackhole_all_types (" + "  _varchar VARCHAR(10)" + ", _bigint BIGINT"+ ", _integer INTEGER"+ ", _smallint SMALLINT"+ ", _tinyint TINYINT"+ ", _float FLOAT"+ ", _double DOUBLE"+ ", _boolean BOOLEAN"+ ", _date DATE"+ ", _timestamp TIMESTAMP"+ ", _varbinary VARBINARY"+ ", _decimal_short DECIMAL(3,2)"+ ", _decimal_long DECIMAL(30,10)"+ ") WITH ( %s = 1, %s = 1, %s = 1 ) ",ROWS_PER_PAGE_PROPERTY,PAGES_PER_SPLIT_PROPERTY,SPLIT_COUNT_PROPERTY),true);
}
