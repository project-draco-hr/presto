{
  ActualProperties properties=Iterables.getOnlyElement(inputProperties);
  if (properties.isUnpartitioned()) {
    return ActualProperties.builder().unpartitioned().coordinatorOnly(properties).local(LocalProperties.grouped(node.getGroupBy())).build();
  }
  return ActualProperties.builder().partitioned(properties).local(LocalProperties.grouped(node.getGroupBy())).build();
}
