{
  if (currentBlock == null || index >= currentBlock.getCount() - 1) {
    currentBlock=iterator.next();
    index=0;
    offset=0;
  }
 else   if (index < currentBlock.getCount() - 1) {
    index++;
    offset+=info.size(currentBlock.getSlice(),offset);
  }
 else {
    throw new NoSuchElementException();
  }
}
