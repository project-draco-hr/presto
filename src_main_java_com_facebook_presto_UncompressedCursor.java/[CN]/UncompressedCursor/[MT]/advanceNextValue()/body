{
  if (block == null) {
    return false;
  }
  if (index < 0) {
    index=0;
    offset=0;
    size=info.size(block.getSlice(),offset);
    return true;
  }
 else   if (index < block.getCount() - 1) {
    index++;
    offset+=size;
    size=info.size(block.getSlice(),offset);
    return true;
  }
 else   if (iterator.hasNext()) {
    block=iterator.next();
    index=0;
    offset=0;
    size=info.size(block.getSlice(),offset);
    return true;
  }
 else {
    block=null;
    index=-1;
    offset=-1;
    size=-1;
    return false;
  }
}
