{
  TupleStream input=Blocks.createBlock(0,"aa","bb","cc");
  MaterializingTupleStream materializingTupleStream=new MaterializingTupleStream(input,DataSize.valueOf("1B"),1.0);
  Blocks.assertTupleStreamEquals(new GenericTupleStream<>(materializingTupleStream.getTupleInfo(),materializingTupleStream),input);
}
