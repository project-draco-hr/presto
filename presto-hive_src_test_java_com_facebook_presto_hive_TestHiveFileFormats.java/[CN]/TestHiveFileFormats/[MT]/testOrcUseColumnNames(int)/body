{
  TestingConnectorSession session=new TestingConnectorSession(new HiveSessionProperties(new HiveClientConfig()).getSessionProperties());
  assertThatFileFormat(ORC).withWriteColumns(TEST_COLUMNS).withRowsCount(rowCount).withReadColumns(Lists.reverse(TEST_COLUMNS)).withSession(session).isReadableByPageSource(new OrcPageSourceFactory(TYPE_MANAGER,true));
}
