{
  BlockIterable blockIterable=getBlockIterable("orders","totalprice",BlocksFileEncoding.RAW);
  AlignmentOperatorFactory alignmentOperator=new AlignmentOperatorFactory(0,blockIterable);
  TopNOperatorFactory topNOperator=new TopNOperatorFactory(1,100,0,ImmutableList.of(singleColumn(Type.DOUBLE,0,0)),Ordering.from(new FieldOrderedTupleComparator(ImmutableList.of(0),ImmutableList.of(SortItem.Ordering.DESCENDING))),false);
  return ImmutableList.of(alignmentOperator,topNOperator);
}
