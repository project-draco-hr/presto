{
switch (kind) {
case TYPE_SIGNATURE:
    return TypeSignatureParameter.of(getTypeSignature().bindParameters(boundParameters));
case NAMED_TYPE_SIGNATURE:
  return TypeSignatureParameter.of(new NamedTypeSignature(getNamedTypeSignature().getName(),getNamedTypeSignature().getTypeSignature().bindParameters(boundParameters)));
default :
return this;
}
}
