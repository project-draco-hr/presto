{
  return DataTypeTest.create().addRoundTrip(charDataType("char",1),"").addRoundTrip(charDataType("char",1),"a").addRoundTrip(charDataType(1),"").addRoundTrip(charDataType(1),"a").addRoundTrip(charDataType(8),"abc").addRoundTrip(charDataType(8),"12345678").addRoundTrip(charDataType(255),repeat("a",255));
