{
  return new TupleStreamDeserializer(){
    @Override public TupleStream deserialize(    Range totalRange,    Slice slice){
      return readAsStream(totalRange,slice);
    }
    @Override public BlockIterable deserializeBlocks(    Range totalRange,    Slice slice){
      return BlocksSerde.readBlocks(slice,totalRange.getStart());
    }
  }
;
}
