{
  if (actualTypes.size() != expectedTypes.size()) {
    return false;
  }
  for (int i=0; i < expectedTypes.size(); i++) {
    Type expectedType=expectedTypes.get(i);
    Type actualType=actualTypes.get(i);
    if (!canCoerce(actualType,expectedType)) {
      return false;
    }
  }
  return true;
}
