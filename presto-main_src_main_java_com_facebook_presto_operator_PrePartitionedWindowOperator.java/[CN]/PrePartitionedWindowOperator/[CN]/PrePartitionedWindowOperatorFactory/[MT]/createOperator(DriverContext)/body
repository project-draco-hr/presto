{
  checkState(!closed,"Factory is already closed");
  OperatorContext operatorContext=driverContext.addOperatorContext(operatorId,PrePartitionedWindowOperator.class.getSimpleName());
  return new PrePartitionedWindowOperator(operatorContext,sourceTypes,outputChannels,windowFunctionDefinitions,partitionChannels,sortChannels,sortOrder,frameInfo,expectedPositions);
}
