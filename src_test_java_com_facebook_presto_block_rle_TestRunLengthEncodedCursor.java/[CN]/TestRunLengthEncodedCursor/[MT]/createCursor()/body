{
  List<RunLengthEncodedBlock> blocks=ImmutableList.of(new RunLengthEncodedBlock(Tuples.createTuple("apple"),Range.create(0,4)),new RunLengthEncodedBlock(Tuples.createTuple("banana"),Range.create(5,7)),new RunLengthEncodedBlock(Tuples.createTuple("cherry"),Range.create(20,21)),new RunLengthEncodedBlock(Tuples.createTuple("date"),Range.create(30,30)));
  return new RunLengthEncodedCursor(TupleInfo.SINGLE_VARBINARY,blocks.iterator());
}
