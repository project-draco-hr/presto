{
  if (!isOptimizedReaderEnabled(session)) {
    return Optional.empty();
  }
  if (!isDeserializerClass(schema,OrcSerde.class)) {
    return Optional.empty();
  }
  return Optional.of(createOrcPageSource(new DwrfMetadataReader(),configuration,path,start,length,columns,partitionKeys,effectivePredicate,hiveStorageTimeZone,typeManager,getOrcMaxMergeDistance(session),getOrcMaxBufferSize(session),getOrcStreamBufferSize(session)));
}
