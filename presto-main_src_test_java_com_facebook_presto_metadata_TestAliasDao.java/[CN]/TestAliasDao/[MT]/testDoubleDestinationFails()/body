{
  createTables(dao);
  assertNull(dao.getAlias(TABLE_1_CONNECTOR_ID,TABLE_1_SCHEMA_NAME,TABLE_1_TABLE_NAME));
  TableAlias tableAlias=new TableAlias(TABLE_1_CONNECTOR_ID,TABLE_1_SCHEMA_NAME,TABLE_1_TABLE_NAME,TABLE_2_CONNECTOR_ID,TABLE_2_SCHEMA_NAME,TABLE_2_TABLE_NAME);
  dao.insertAlias(tableAlias);
  try {
    tableAlias=new TableAlias(TABLE_3_CONNECTOR_ID,TABLE_3_SCHEMA_NAME,TABLE_3_TABLE_NAME,TABLE_2_CONNECTOR_ID,TABLE_2_SCHEMA_NAME,TABLE_2_TABLE_NAME);
    dao.insertAlias(tableAlias);
    fail("Could insert table twice!");
  }
 catch (  UnableToExecuteStatementException e) {
  }
}
