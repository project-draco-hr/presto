{
  Preconditions.checkNotNull(tuples,"tuples is null");
  checkState(!finished,"already finished");
  for (  Tuple tuple : tuples) {
    if (tupleInfo == null) {
      tupleInfo=tuple.getTupleInfo();
    }
    tuple.writeTo(buffer);
    tupleCount++;
    if (buffer.size() >= MAX_BLOCK_SIZE) {
      writeBlock();
    }
  }
  return this;
}
