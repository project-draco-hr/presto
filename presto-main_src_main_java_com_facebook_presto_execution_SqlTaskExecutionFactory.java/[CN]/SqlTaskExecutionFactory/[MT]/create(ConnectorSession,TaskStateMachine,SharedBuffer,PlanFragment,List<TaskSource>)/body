{
  TaskContext taskContext=new TaskContext(taskStateMachine,taskNotificationExecutor,session,checkNotNull(maxTaskMemoryUsage,"maxTaskMemoryUsage is null"),checkNotNull(operatorPreAllocatedMemory,"operatorPreAllocatedMemory is null"),cpuTimerEnabled);
  return createSqlTaskExecution(taskStateMachine,taskContext,sharedBuffer,fragment,sources,planner,taskExecutor,taskNotificationExecutor,queryMonitor);
}
