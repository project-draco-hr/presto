{
  assertFunction("LTRIM('')",VARCHAR,"");
  assertFunction("LTRIM('   ')",VARCHAR,"");
  assertFunction("LTRIM('  hello  ')",VARCHAR,"hello  ");
  assertFunction("LTRIM('  hello')",VARCHAR,"hello");
  assertFunction("LTRIM('hello  ')",VARCHAR,"hello  ");
  assertFunction("LTRIM(' hello world ')",VARCHAR,"hello world ");
}
