{
  assertFunction("RTRIM('')",VARCHAR,"");
  assertFunction("RTRIM('   ')",VARCHAR,"");
  assertFunction("RTRIM('  hello  ')",VARCHAR,"  hello");
  assertFunction("RTRIM('  hello')",VARCHAR,"  hello");
  assertFunction("RTRIM('hello  ')",VARCHAR,"hello");
  assertFunction("RTRIM(' hello world ')",VARCHAR," hello world");
}
