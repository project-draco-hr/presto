{
switch (tpchType) {
case IDENTIFIER:
    return BigintType.BIGINT;
case INTEGER:
  return IntegerType.INTEGER;
case DATE:
return DateType.DATE;
case DOUBLE:
return DoubleType.DOUBLE;
case VARCHAR:
return createUnboundedVarcharType();
}
throw new IllegalArgumentException("Unsupported type " + tpchType);
}
