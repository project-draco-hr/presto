{
  if (javaType == Boolean.class) {
    return BOOLEAN;
  }
  if (javaType == Long.class) {
    return BIGINT;
  }
  if (javaType == Double.class) {
    return DOUBLE;
  }
  if (javaType == String.class) {
    return VARCHAR;
  }
  throw new IllegalArgumentException("Unsupported type " + javaType.getName());
}
