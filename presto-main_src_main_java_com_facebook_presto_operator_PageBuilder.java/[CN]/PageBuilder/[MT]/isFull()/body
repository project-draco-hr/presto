{
  if (declaredPositions == Integer.MAX_VALUE) {
    return true;
  }
  long sizeInBytes=0;
  for (  BlockBuilder blockBuilder : blockBuilders) {
    if (blockBuilder.isFull()) {
      return true;
    }
    sizeInBytes+=blockBuilder.size();
    if (sizeInBytes > maxSizeInBytes) {
      return true;
    }
  }
  return false;
}
