{
  TupleDomain<ColumnHandle> tupleDomain=TupleDomain.withColumnDomains(ImmutableMap.of(cols.get(0),Domain.all(BigintType.BIGINT),cols.get(1),Domain.onlyNull(DoubleType.DOUBLE)));
  Connection connection=database.getConnection();
  PreparedStatement preparedStatement=new QueryBuilder("\"").buildSql(jdbcClient,connection,"","","test_table",cols,tupleDomain);
  ResultSet res=preparedStatement.executeQuery();
  assertEquals(res.next(),false);
}
