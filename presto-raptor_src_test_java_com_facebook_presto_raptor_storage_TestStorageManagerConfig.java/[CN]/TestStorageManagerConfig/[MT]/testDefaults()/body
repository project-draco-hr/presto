{
  assertRecordedDefaults(recordDefaults(StorageManagerConfig.class).setDataDirectory(null).setBackupDirectory(null).setOrcMaxMergeDistance(new DataSize(1,MEGABYTE)).setShardRecoveryTimeout(new Duration(30,SECONDS)).setMissingShardDiscoveryInterval(new Duration(5,MINUTES)).setRecoveryThreads(10));
