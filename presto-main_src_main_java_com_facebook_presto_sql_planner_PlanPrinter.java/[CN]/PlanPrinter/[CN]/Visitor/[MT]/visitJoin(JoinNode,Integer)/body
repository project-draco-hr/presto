{
  List<Expression> joinExpressions=new ArrayList<>();
  for (  JoinNode.EquiJoinClause clause : node.getCriteria()) {
    joinExpressions.add(new ComparisonExpression(ComparisonExpression.Type.EQUAL,new QualifiedNameReference(clause.getLeft().toQualifiedName()),new QualifiedNameReference(clause.getRight().toQualifiedName())));
  }
  print(indent,"- %s[%s] => [%s]",node.getType().getJoinLabel(),Joiner.on(" AND ").join(joinExpressions),formatOutputs(node.getOutputSymbols()));
  printStats(indent + 2,node.getId());
  node.getLeft().accept(this,indent + 1);
  node.getRight().accept(this,indent + 1);
  return null;
}
