{
  InformationSchemaTableHandle informationSchemaTableHandle=new InformationSchemaTableHandle(INFORMATION_SCHEMA_CONNECTOR_ID,"information_schema_catalog","information_schema_schema","information_schema_table");
  assertTrue(objectMapper.canSerialize(InformationSchemaTableHandle.class));
  String json=objectMapper.writeValueAsString(informationSchemaTableHandle);
  testJsonEquals(json,INFORMATION_SCHEMA_AS_MAP);
}
