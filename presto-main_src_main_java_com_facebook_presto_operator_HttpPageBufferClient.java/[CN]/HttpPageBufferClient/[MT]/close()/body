{
  boolean shouldSendDelete;
  Future<?> future;
synchronized (this) {
    shouldSendDelete=!closed;
    closed=true;
    future=this.future;
    this.future=null;
    lastUpdate=DateTime.now();
  }
  if (future != null) {
    future.cancel(true);
  }
  if (shouldSendDelete) {
    sendDelete();
  }
}
