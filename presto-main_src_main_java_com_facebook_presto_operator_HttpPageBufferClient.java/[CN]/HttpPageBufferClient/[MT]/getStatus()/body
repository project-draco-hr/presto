{
  String state;
  if (closed) {
    state="closed";
  }
 else   if (future != null) {
    state="running";
  }
 else {
    state="queued";
  }
  String httpRequestState="queued";
  if (future != null) {
    httpRequestState=future.getState();
  }
  return new ExchangeClientStatus(location,state,lastUpdate,requestsScheduled.get(),requestsCompleted.get(),httpRequestState);
}
