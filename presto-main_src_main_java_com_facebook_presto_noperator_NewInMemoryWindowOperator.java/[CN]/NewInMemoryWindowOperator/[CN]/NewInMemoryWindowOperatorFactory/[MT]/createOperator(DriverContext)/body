{
  checkState(!closed,"Factory is already closed");
  OperatorContext operatorContext=driverContext.addOperatorContext(operatorId,NewInMemoryWindowOperator.class.getSimpleName());
  return new NewInMemoryWindowOperator(operatorContext,sourceTupleInfos,orderingChannel,outputChannels,windowFunctions,partitionFields,sortFields,sortOrder,expectedPositions);
}
