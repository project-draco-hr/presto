{
  checkState(!closed,"Factory is already closed");
  OperatorContext operatorContext=driverContext.addOperatorContext(operatorId,planNodeId,ParallelHashBuildOperator.class.getSimpleName());
  ParallelHashBuildOperator operator=new ParallelHashBuildOperator(operatorContext,lookupSourceSupplier,partitionIndex,hashChannels,preComputedHashChannel,expectedPositions);
  partitionIndex++;
  return operator;
}
