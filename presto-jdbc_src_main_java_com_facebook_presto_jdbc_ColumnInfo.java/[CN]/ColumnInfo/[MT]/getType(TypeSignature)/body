{
  if (type.getBase().equals("array")) {
    return Types.ARRAY;
  }
switch (type.getBase()) {
case "boolean":
    return Types.BOOLEAN;
case "bigint":
  return Types.BIGINT;
case "double":
return Types.DOUBLE;
case "varchar":
return Types.LONGNVARCHAR;
case "varbinary":
return Types.LONGVARBINARY;
case "time":
return Types.TIME;
case "time with time zone":
return Types.TIME;
case "timestamp":
return Types.TIMESTAMP;
case "timestamp with time zone":
return Types.TIMESTAMP;
case "date":
return Types.DATE;
case "decimal":
return Types.DECIMAL;
default :
return Types.JAVA_OBJECT;
}
}
