{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Slice slice=(Slice)o;
  if (length != slice.length) {
    return false;
  }
  if (offset == slice.offset && data == slice.data) {
    return true;
  }
  for (int i=0; i < length; i++) {
    if (data[offset + i] != slice.data[slice.offset + i]) {
      return false;
    }
  }
  return true;
}
