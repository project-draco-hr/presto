{
  checkState(field != -1,"not in record");
  checkState(field == fieldCount,"not all fields set");
  field=-1;
  try {
    statement.addBatch();
    batchSize++;
    if (batchSize >= 1000) {
      statement.executeBatch();
      connection.commit();
      connection.setAutoCommit(false);
      batchSize=0;
    }
  }
 catch (  SQLException e) {
    throw Throwables.propagate(e);
  }
}
