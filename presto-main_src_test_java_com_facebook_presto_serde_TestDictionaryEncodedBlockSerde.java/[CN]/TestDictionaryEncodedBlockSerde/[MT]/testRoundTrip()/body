{
  Block block=createBlockBuilder(SINGLE_VARBINARY).append("alice").append("bob").append("charlie").append("dave").build();
  DynamicSliceOutput sliceOutput=new DynamicSliceOutput(1024);
  BlockEncoding blockEncoding=new DictionaryEncoder(new UncompressedEncoder(sliceOutput)).append(block).append(block).append(block).finish();
  Block actualBlock=blockEncoding.readBlock(sliceOutput.slice().getInput());
  assertBlockEquals(actualBlock,createBlockBuilder(SINGLE_VARBINARY).append("alice").append("bob").append("charlie").append("dave").append("alice").append("bob").append("charlie").append("dave").append("alice").append("bob").append("charlie").append("dave").build());
}
