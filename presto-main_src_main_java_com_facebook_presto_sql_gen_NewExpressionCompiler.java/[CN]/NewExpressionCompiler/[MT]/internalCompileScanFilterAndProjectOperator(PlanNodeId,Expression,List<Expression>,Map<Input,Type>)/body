{
  DynamicClassLoader classLoader=createClassLoader();
  TypedNewOperatorClass typedNewOperatorClass=compileScanFilterAndProjectOperator(filter,projections,inputTypes,classLoader);
  Constructor<? extends NewSourceOperator> constructor;
  try {
    constructor=typedNewOperatorClass.getNewOperatorClass().asSubclass(NewSourceOperator.class).getConstructor(OperatorContext.class,PlanNodeId.class,DataStreamProvider.class,Iterable.class,Iterable.class);
  }
 catch (  NoSuchMethodException e) {
    throw Throwables.propagate(e);
  }
  ScanFilterAndProjectOperatorFactoryFactory operatorFactoryFactory=new ScanFilterAndProjectOperatorFactoryFactory(constructor,sourceId,typedNewOperatorClass.getTupleInfos());
  return operatorFactoryFactory;
}
