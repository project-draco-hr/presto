{
  checkState(!closed,"Factory is already closed");
  OperatorContext operatorContext=driverContext.addOperatorContext(operatorId,planNodeId,WindowOperator.class.getSimpleName());
  return new WindowOperator(operatorContext,sourceTypes,outputChannels,windowFunctionDefinitions,partitionChannels,preGroupedChannels,sortChannels,sortOrder,preSortedChannelPrefix,frameInfo,expectedPositions);
}
