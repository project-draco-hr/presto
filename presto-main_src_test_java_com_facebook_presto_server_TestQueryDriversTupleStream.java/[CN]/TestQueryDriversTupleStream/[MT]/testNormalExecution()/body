{
  List<Page> pages=createPages();
  int expectedCount=0;
  for (  Page page : pages) {
    expectedCount+=page.getPositionCount();
  }
  ExecutorService executor=Executors.newCachedThreadPool();
  try {
    QueryDriversOperator operator=new QueryDriversOperator(10,new StaticQueryDriverProvider(executor,pages),new StaticQueryDriverProvider(executor,pages),new StaticQueryDriverProvider(executor,pages));
    int count=0;
    for (    Page page : operator) {
      BlockCursor cursor=page.getBlock(0).cursor();
      while (cursor.advanceNextPosition()) {
        count++;
      }
    }
    assertEquals(count,expectedCount * 3);
  }
  finally {
    executor.shutdownNow();
  }
}
