{
  assertFunction("case true " + "when true then cast(null as varchar) " + "else 'foo' "+ "end",VARCHAR,null);
  assertFunction("case true " + "when true then 33 " + "end",BIGINT,33L);
  assertFunction("case true " + "when false then 1 " + "else 33 "+ "end",BIGINT,33L);
  assertFunction("case true " + "when false then 1 " + "when false then 1 "+ "when true then 33 "+ "else 1 "+ "end",BIGINT,33L);
  assertFunction("case true " + "when false then 1 " + "end",BIGINT,null);
  assertFunction("case true " + "when true then null " + "else 'foo' "+ "end",createVarcharType(3),null);
  assertFunction("case true " + "when null then 1 " + "when true then 33 "+ "end",BIGINT,33L);
  assertFunction("case null " + "when true then 1 " + "else 33 "+ "end",BIGINT,33L);
  assertFunction("case true " + "when false then 1.0 " + "when true then 33 "+ "end",DOUBLE,33.0);
}
