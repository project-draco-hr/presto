{
  checkNotNull(partitionName,"partitionName is null");
  try {
    return retry().stopOn(NoSuchObjectException.class).stopOnIllegalExceptions().run("getPartitionsByNames",stats.getGetPartitionByName().wrap(new Callable<Partition>(){
      @Override public Partition call() throws Exception {
        try (HiveMetastoreClient client=clientProvider.createMetastoreClient()){
          return client.get_partition_by_name(partitionName.getHiveTableName().getDatabaseName(),partitionName.getHiveTableName().getTableName(),partitionName.getPartitionName());
        }
       }
    }
));
  }
 catch (  NoSuchObjectException e) {
    throw e;
  }
catch (  TException e) {
    throw new PrestoException(HIVE_METASTORE_ERROR,e);
  }
}
