{
  @Language("SQL") String sql="" + "SELECT orderstatus, clerk, sales\n" + ", custom_rank() OVER (PARTITION BY orderstatus ORDER BY sales DESC) rnk\n"+ "FROM (\n"+ "  SELECT orderstatus, clerk, sum(totalprice) sales\n"+ "  FROM orders\n"+ "  GROUP BY orderstatus, clerk\n"+ ")\n"+ "ORDER BY clerk";
  assertEquals(computeActual(sql),computeActual(sql.replace("custom_rank","rank")));
}
