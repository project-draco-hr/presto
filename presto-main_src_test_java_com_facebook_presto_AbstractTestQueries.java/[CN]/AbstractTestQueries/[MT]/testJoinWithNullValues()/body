{
  assertQuery("" + "SELECT *\n" + "FROM (\n"+ "  SELECT CASE WHEN orderkey % 3 = 0 THEN NULL ELSE orderkey END AS orderkey\n"+ "  FROM lineitem\n"+ "  WHERE partkey % 512 = 0\n"+ ") AS lineitem \n"+ "JOIN (\n"+ "  SELECT CASE WHEN orderkey % 2 = 0 THEN NULL ELSE orderkey END AS orderkey\n"+ "  FROM orders\n"+ "  WHERE custkey % 512 = 0\n"+ ") AS orders\n"+ "ON lineitem.orderkey = orders.orderkey");
}
