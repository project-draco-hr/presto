{
  assertQuery("SELECT TIME '3:04', TIMESTAMP '1960-01-22 3:04', DATE '2013-03-22', INTERVAL '123' DAY\n","SELECT " + MILLISECONDS.toSeconds(new DateTime(1970,1,1,3,4,0,0,DateTimeZone.UTC).getMillis()) + ",  "+ MILLISECONDS.toSeconds(new DateTime(1960,1,22,3,4,0,0,DateTimeZone.UTC).getMillis())+ ",  "+ MILLISECONDS.toSeconds(new DateTime(2013,3,22,0,0,0,0,DateTimeZone.UTC).getMillis())+ ",  "+ String.valueOf(TimeUnit.DAYS.toSeconds(123)));
}
