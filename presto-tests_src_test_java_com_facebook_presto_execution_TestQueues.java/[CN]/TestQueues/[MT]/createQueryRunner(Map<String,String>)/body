{
  DistributedQueryRunner queryRunner=new DistributedQueryRunner(testSessionBuilder().build(),2,properties);
  try {
    queryRunner.installPlugin(new TpchPlugin());
    queryRunner.createCatalog("tpch","tpch");
    return queryRunner;
  }
 catch (  Exception e) {
    queryRunner.close();
    throw e;
  }
}
