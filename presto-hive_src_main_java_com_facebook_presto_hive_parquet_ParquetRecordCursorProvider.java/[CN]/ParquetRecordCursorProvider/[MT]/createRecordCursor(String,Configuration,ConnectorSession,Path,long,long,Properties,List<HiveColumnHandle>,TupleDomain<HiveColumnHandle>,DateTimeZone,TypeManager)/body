{
  if (!PARQUET_SERDE_CLASS_NAMES.contains(getDeserializerClassName(schema))) {
    return Optional.empty();
  }
  return Optional.of(new ParquetHiveRecordCursor(hdfsEnvironment,session.getUser(),configuration,path,start,length,schema,columns,useParquetColumnNames,hiveStorageTimeZone,typeManager,isParquetPredicatePushdownEnabled(session),effectivePredicate));
}
