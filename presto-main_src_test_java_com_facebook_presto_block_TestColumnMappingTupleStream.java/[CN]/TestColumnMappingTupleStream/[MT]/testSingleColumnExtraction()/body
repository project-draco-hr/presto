{
  Blocks.assertTupleStreamEquals(ProjectionTupleStream.project(baseTupleStream,0),Blocks.createDoublesTupleStream(0,0.1,1.2,100.99));
  Blocks.assertTupleStreamEquals(ProjectionTupleStream.project(baseTupleStream,1),Blocks.createLongsTupleStream(0,15,0,-1));
  Blocks.assertTupleStreamEquals(ProjectionTupleStream.project(baseTupleStream,2),Blocks.createTupleStream(0,"test","fuu","t"));
}
