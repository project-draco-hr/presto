{
  TupleInfo expectedTupleInfo=new TupleInfo(DOUBLE,FIXED_INT_64);
  TupleStream expectedTupleStream=new GenericTupleStream<>(expectedTupleInfo,new BlockBuilder(0,expectedTupleInfo).append(0.1).append(15).append(1.2).append(0).append(100.99).append(-1).build());
  Blocks.assertTupleStreamEquals(ColumnMappingTupleStream.map(baseTupleStream,0,1),expectedTupleStream);
}
