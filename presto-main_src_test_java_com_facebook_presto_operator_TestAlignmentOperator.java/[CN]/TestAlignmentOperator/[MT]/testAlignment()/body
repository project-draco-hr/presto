{
  AlignmentOperator operator=createAlignmentOperator();
  assertOperatorEquals(operator,createOperator(new Page(createStringsBlock("alice","bob","charlie","dave"),createLongsBlock(0,1,2,3)),new Page(createStringsBlock("alice","bob","charlie","dave"),createLongsBlock(4,5,6,7)),new Page(createStringsBlock("alice","bob","charlie","dave"),createLongsBlock(8,9,10,11))));
}
