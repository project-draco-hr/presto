{
  return new Function<Object,Object>(){
    @Nullable @Override public Object apply(    @Nullable Object input){
      Map<Object,Object> map=new HashMap<>();
      if (input == null) {
        map.put(nullKeyValue,null);
      }
 else {
        map.put(input,input);
      }
      return map;
    }
  }
;
}
