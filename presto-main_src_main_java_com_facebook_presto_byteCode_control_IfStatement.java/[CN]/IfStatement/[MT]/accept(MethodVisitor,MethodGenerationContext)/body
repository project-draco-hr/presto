{
  Block block=new Block(context);
  block.append(condition).ifZeroGoto(falseLabel).append(ifTrue);
  if (ifFalse != null) {
    block.gotoLabel(outLabel).visitLabel(falseLabel).append(ifFalse).visitLabel(outLabel);
  }
 else {
    block.visitLabel(falseLabel);
  }
  block.accept(visitor,generationContext);
}
