{
  assertThat(query("SELECT" + "  node_id," + "  task_id,"+ "  stage_id,"+ "  query_id,"+ "  state,"+ "  splits,"+ "  queued_splits,"+ "  running_splits,"+ "  completed_splits,"+ "  split_scheduled_time_ms,"+ "  split_cpu_time_ms,"+ "  split_user_time_ms,"+ "  split_blocked_time_ms,"+ "  raw_input_bytes,"+ "  raw_input_rows,"+ "  processed_input_bytes,"+ "  processed_input_rows,"+ "  output_bytes,"+ "  output_rows,"+ "  created,"+ "  start,"+ "  last_heartbeat,"+ "  'end' "+ "FROM SYSTEM.runtime.tasks")).hasColumns(LONGNVARCHAR,LONGNVARCHAR,LONGNVARCHAR,LONGNVARCHAR,LONGNVARCHAR,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,BIGINT,TIMESTAMP,TIMESTAMP,TIMESTAMP,LONGNVARCHAR).hasAnyRows();
}
