{
  assertThat(query("SELECT" + "  node_id," + "  query_id,"+ "  state,"+ "  user,"+ "  query,"+ "  queued_time_ms,"+ "  analysis_time_ms,"+ "  distributed_planning_time_ms,"+ "  created,"+ "  started,"+ "  last_heartbeat,"+ "  'end' "+ "FROM system.runtime.queries")).hasColumns(LONGNVARCHAR,LONGNVARCHAR,LONGNVARCHAR,LONGNVARCHAR,LONGNVARCHAR,BIGINT,BIGINT,BIGINT,TIMESTAMP,TIMESTAMP,TIMESTAMP,LONGNVARCHAR).hasAnyRows();
}
