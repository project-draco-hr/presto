{
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("query.max-memory","2GB").put("query.max-memory-per-node","2GB").build();
  MemoryManagerConfig expected=new MemoryManagerConfig().setMaxQueryMemory(new DataSize(2,GIGABYTE)).setMaxQueryMemoryPerNode(new DataSize(2,GIGABYTE));
  assertFullMapping(properties,expected);
}
