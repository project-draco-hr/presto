{
  SystemTableHandle tableHandle=checkType(table,SystemTableHandle.class,"table");
  ConnectorTableLayout layout=new ConnectorTableLayout(new SystemTableLayoutHandle(tableHandle),Optional.empty(),TupleDomain.<ColumnHandle>all(),Optional.empty(),Optional.empty(),ImmutableList.of());
  return ImmutableList.of(new ConnectorTableLayoutResult(layout,constraint.getSummary()));
}
