{
  try {
    if (!stateMachine.transitionToRunning()) {
      return;
    }
    task.execute(statement,session,metadata,stateMachine);
    stateMachine.transitionToFinished();
  }
 catch (  Throwable e) {
    fail(e);
    if (!(e instanceof RuntimeException)) {
      throw Throwables.propagate(e);
    }
  }
}
