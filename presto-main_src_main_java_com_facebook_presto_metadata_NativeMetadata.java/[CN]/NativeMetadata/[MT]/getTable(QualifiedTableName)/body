{
  checkTable(table);
  Long tableId=dao.getTableId(table);
  if (tableId == null) {
    return null;
  }
  TableHandle tableHandle=new NativeTableHandle(tableId);
  List<ColumnMetadata> columns=dao.getTableColumnMetaData(tableId);
  if (columns.isEmpty()) {
    return null;
  }
  return new TableMetadata(table,columns,tableHandle);
}
