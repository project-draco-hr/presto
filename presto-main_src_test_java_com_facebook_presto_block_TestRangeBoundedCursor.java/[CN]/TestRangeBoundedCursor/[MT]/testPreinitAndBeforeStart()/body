{
  Cursor cursor=Blocks.createTupleStream(0,"a","bb","c","d","e","e","a","bb").cursor(new QuerySession());
  assertAdvanceNextPosition(cursor);
  assertCursorsEquals(RangeBoundedCursor.bound(cursor,Range.create(1,2)),Blocks.createTupleStream(1,"bb","c").cursor(new QuerySession()));
  assertEquals(cursor.getPosition(),3);
}
