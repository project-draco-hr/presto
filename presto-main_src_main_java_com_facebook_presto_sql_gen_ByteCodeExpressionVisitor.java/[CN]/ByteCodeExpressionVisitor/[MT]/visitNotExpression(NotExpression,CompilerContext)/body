{
  TypedByteCodeNode value=process(node.getValue(),context);
  if (value.getType() == void.class) {
    return value;
  }
  Preconditions.checkState(value.getType() == boolean.class);
  return typedByteCodeNode(new Block(context).comment(node.toString()).append(value.getNode()).invokeStatic(Operations.class,"not",boolean.class,boolean.class),boolean.class);
}
