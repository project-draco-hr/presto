{
  Type valueType=expressionTypes.get(node.getValue());
  if (valueType.equals(UNKNOWN)) {
    return loadBoolean(true);
  }
  ByteCodeNode value=process(node.getValue(),context);
  Block block=new Block(context).comment(node.toString()).append(value).pop(valueType.getJavaType()).getVariable("wasNull");
  block.putVariable("wasNull",false);
  return block;
}
