{
  assertEquals(setX.toString(),"point.x = 42");
  Block block=new Block(context).append(context.getVariable("point").set(newInstance(Point.class,constantInt(3),constantInt(7)))).append(setX).append(context.getVariable("point").ret());
  assertByteCodeNode(block,type(Point.class),new Point(42,7));
}
