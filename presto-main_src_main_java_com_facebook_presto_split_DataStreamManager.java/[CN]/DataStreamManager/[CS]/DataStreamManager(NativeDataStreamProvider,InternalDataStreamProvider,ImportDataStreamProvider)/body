{
  checkNotNull(nativeProvider,"nativeProvider is null");
  checkNotNull(internalProvider,"internalProvider is null");
  checkNotNull(importProvider,"importProvider is null");
  dataStreamProviderMap=ImmutableMap.<DataSourceType,DataStreamProvider>builder().put(DataSourceType.NATIVE,nativeProvider).put(DataSourceType.INTERNAL,internalProvider).put(DataSourceType.IMPORT,importProvider).build();
}
