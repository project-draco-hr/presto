{
  if (enabled.get()) {
    if (started.compareAndSet(false,true)) {
      this.scheduledFuture.set(executorService.scheduleAtFixedRate(new ImportControllerRunnable(),(long)checkInterval.toMillis(),(long)checkInterval.toMillis(),TimeUnit.MILLISECONDS));
    }
 else {
      log.info("Ignored double start.");
    }
  }
 else {
    log.info("Periodic Importer not enabled.");
  }
}
