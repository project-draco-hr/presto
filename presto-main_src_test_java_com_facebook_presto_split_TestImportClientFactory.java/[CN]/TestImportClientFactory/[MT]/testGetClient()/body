{
  ServiceSelector selector=new StaticServiceSelector(serviceDescriptor("hive-metastore").addProperty("thrift","meta1.test:9083").build(),serviceDescriptor("hive-metastore").addProperty("thrift","meta2.text:9083").build(),serviceDescriptor("hive-metastore").addProperty("thrift","missing-port").build(),serviceDescriptor("hive-metastore").build());
  ImportClientFactory factory=new ImportClientFactory(selector);
  assertInstanceOf(factory.getClient("hive"),HiveClient.class);
}
