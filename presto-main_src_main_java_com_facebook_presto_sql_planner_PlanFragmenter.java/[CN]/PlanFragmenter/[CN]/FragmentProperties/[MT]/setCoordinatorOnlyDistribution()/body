{
  if (partitioningHandle.isPresent() && partitioningHandle.get().isCoordinatorOnly()) {
    return this;
  }
  checkState(!partitioningHandle.isPresent() || isSinglePartitioning(partitioningHandle.get()),"Cannot overwrite partitioning with %s (currently set to %s)",coordinatorOnlyPartition(),partitioningHandle);
  partitioningHandle=Optional.of(coordinatorOnlyPartition().getHandle());
  return this;
}
