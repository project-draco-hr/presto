{
  checkState(outputPartitioning.isPresent() && outputPartitioning.get().equals(OutputPartitioning.HASH),"Can only set null replicate if output partitioning is %s (currently set to %s)",OutputPartitioning.HASH,outputPartitioning);
  this.nullPartitionPolicy=Optional.of(NullPartitioning.REPLICATE);
  return this;
}
