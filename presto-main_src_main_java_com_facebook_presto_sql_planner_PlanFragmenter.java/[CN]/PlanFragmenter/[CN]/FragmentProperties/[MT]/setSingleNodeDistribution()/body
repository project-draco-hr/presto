{
  if (distribution.isPresent()) {
    PlanDistribution value=distribution.get();
    checkState(value == PlanDistribution.SINGLE || value == PlanDistribution.COORDINATOR_ONLY,"Cannot overwrite distribution with %s (currently set to %s)",PlanDistribution.SINGLE,value);
  }
 else {
    distribution=Optional.of(PlanDistribution.SINGLE);
  }
  return this;
}
