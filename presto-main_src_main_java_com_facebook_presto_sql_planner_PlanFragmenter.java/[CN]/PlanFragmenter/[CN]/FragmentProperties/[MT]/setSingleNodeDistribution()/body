{
  if (partitioningHandle.isPresent() && partitioningHandle.get().isSingleNode()) {
    return this;
  }
  checkState(!partitioningHandle.isPresent(),"Cannot overwrite partitioning with %s (currently set to %s)",singlePartition(),partitioningHandle);
  partitioningHandle=Optional.of(singlePartition().getHandle());
  return this;
}
