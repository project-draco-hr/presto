{
  assertRewrite("SELECT ORDERS.*, LINEITEM.* FROM ORDERS, LINEITEM","SELECT ORDERS.orderkey orderkey, " + "ORDERS.custkey custkey, " + "ORDERS.totalprice totalprice, "+ "ORDERS.orderdate orderdate, "+ "ORDERS.orderstatus orderstatus, "+ "ORDERS.orderpriority orderpriority, "+ "ORDERS.clerk clerk, "+ "ORDERS.shippriority shippriority, "+ "ORDERS.comment comment, "+ "LINEITEM.orderkey orderkey, "+ "LINEITEM.partkey partkey, "+ "LINEITEM.suppkey suppkey, "+ "LINEITEM.linenumber linenumber, "+ "LINEITEM.quantity quantity, "+ "LINEITEM.extendedprice extendedprice, "+ "LINEITEM.discount discount, "+ "LINEITEM.tax tax, "+ "LINEITEM.returnflag returnflag, "+ "LINEITEM.linestatus linestatus, "+ "LINEITEM.shipdate shipdate, "+ "LINEITEM.commitdate commitdate, "+ "LINEITEM.receiptdate receiptdate, "+ "LINEITEM.shipinstruct shipinstruct, "+ "LINEITEM.shipmode shipmode, "+ "LINEITEM.comment comment\n"+ "FROM ORDERS, LINEITEM");
}
