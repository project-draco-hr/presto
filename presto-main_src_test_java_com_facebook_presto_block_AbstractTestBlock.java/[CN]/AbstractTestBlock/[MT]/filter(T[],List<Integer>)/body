{
  @SuppressWarnings("unchecked") T[] prunedExpectedValues=(T[])Array.newInstance(expectedValues.getClass().getComponentType(),positions.size());
  for (int i=0; i < prunedExpectedValues.length; i++) {
    prunedExpectedValues[i]=expectedValues[positions.get(i)];
  }
  return prunedExpectedValues;
}
