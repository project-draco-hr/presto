{
  String line;
  try {
    line=reader.readLine();
  }
 catch (  IOException e) {
    throw Throwables.propagate(e);
  }
  if (line == null) {
    endOfData();
    return null;
  }
  Iterable<String> split=columnSplitter.split(line);
  String value=Iterables.get(split,columnIndex);
  return new UncompressedValueBlock(position++,value);
}
