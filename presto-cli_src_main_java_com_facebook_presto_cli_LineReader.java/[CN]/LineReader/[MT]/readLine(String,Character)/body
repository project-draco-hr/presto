{
  String line;
  interrupted=false;
  try {
    line=super.readLine(prompt,mask);
  }
 catch (  UserInterruptException e) {
    getHistory().moveToEnd();
    interrupted=true;
    return null;
  }
  if (getHistory() instanceof Flushable) {
    ((Flushable)getHistory()).flush();
  }
  return line;
}
