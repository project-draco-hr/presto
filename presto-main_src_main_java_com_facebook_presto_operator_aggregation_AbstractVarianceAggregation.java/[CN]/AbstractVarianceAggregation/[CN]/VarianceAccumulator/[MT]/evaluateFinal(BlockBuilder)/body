{
  if (population) {
    if (currentCount == 0) {
      output.appendNull();
    }
 else {
      double result=currentM2 / currentCount;
      if (standardDeviation) {
        result=Math.sqrt(result);
      }
      output.append(result);
    }
  }
 else {
    if (currentCount < 2) {
      output.appendNull();
    }
 else {
      double result=currentM2 / (currentCount - 1);
      if (standardDeviation) {
        result=Math.sqrt(result);
      }
      output.append(result);
    }
  }
}
