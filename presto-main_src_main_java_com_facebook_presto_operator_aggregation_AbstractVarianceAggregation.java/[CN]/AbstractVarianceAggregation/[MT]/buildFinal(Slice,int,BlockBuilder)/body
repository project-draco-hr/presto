{
  if (VARIANCE_CONTEXT_INFO.isNull(valueSlice,valueOffset,0)) {
    return null;
  }
  final long count=VARIANCE_CONTEXT_INFO.getLong(valueSlice,valueOffset,0);
  final double m2=VARIANCE_CONTEXT_INFO.getDouble(valueSlice,valueOffset,2);
  if (population) {
    return m2 / count;
  }
 else   if (count == 1) {
    return 0.0;
  }
 else {
    return m2 / (count - 1);
  }
}
