{
  Session defaultSession=new Session("user","test","local",TpchMetadata.TINY_SCHEMA_NAME,UTC_KEY,Locale.ENGLISH,null,null);
  LocalQueryRunner localQueryRunner=new LocalQueryRunner(defaultSession);
  localQueryRunner.createCatalog(defaultSession.getCatalog(),new TpchConnectorFactory(localQueryRunner.getNodeManager(),1),ImmutableMap.<String,String>of());
  localQueryRunner.createCatalog(TPCH_SAMPLED_SCHEMA,new SampledTpchConnectorFactory(localQueryRunner.getNodeManager(),1,2),ImmutableMap.<String,String>of());
  localQueryRunner.getMetadata().addFunctions(CUSTOM_FUNCTIONS);
  return localQueryRunner;
}
