{
  MetadataUtil.TableMetadataBuilder builder=tableMetadataBuilder(DEFAULT_TEST_ORDERS).column("orderkey",BIGINT).column("custkey",BIGINT).column("totalprice",DOUBLE).column("orderdate",DATE);
  if (!properties.isEmpty()) {
    for (    Map.Entry<String,Object> entry : properties.entrySet()) {
      builder.property(entry.getKey(),entry.getValue());
    }
  }
  return builder.build();
}
