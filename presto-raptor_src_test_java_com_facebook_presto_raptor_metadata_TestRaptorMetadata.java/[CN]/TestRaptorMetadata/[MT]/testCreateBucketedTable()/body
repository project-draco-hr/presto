{
  assertNull(metadata.getTableHandle(SESSION,DEFAULT_TEST_ORDERS));
  ConnectorTableMetadata ordersTable=getOrdersTable(ImmutableMap.of(BUCKET_COUNT_PROPERTY,16,BUCKETED_ON_PROPERTY,ImmutableList.of("orderdate","orderkey")));
  metadata.createTable(SESSION,ordersTable);
  ConnectorTableHandle tableHandle=metadata.getTableHandle(SESSION,DEFAULT_TEST_ORDERS);
  assertInstanceOf(tableHandle,RaptorTableHandle.class);
  RaptorTableHandle raptorTableHandle=(RaptorTableHandle)tableHandle;
  assertEquals(raptorTableHandle.getTableId(),1);
  long tableId=raptorTableHandle.getTableId();
  MetadataDao metadataDao=dbi.onDemand(MetadataDao.class);
  assertEquals(metadataDao.listBucketColumns(tableId),ImmutableList.of(new TableColumn(DEFAULT_TEST_ORDERS,"orderdate",DATE,4),new TableColumn(DEFAULT_TEST_ORDERS,"orderkey",BIGINT,1)));
  assertEquals(raptorTableHandle.getBucketCount(),OptionalInt.of(16));
  metadata.dropTable(SESSION,tableHandle);
}
