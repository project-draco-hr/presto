{
  if (!context.isDefaultRewrite()) {
    Node result=nodeRewriter.rewriteSubquery(node,context.get(),TreeRewriter.this);
    if (result != null) {
      return result;
    }
  }
  Query child=rewrite(node.getQuery(),context.get());
  if (child != node.getQuery()) {
    return new Subquery(child);
  }
  return node;
}
