{
  if (!isParquetOptimizedReaderEnabled(session)) {
    return Optional.empty();
  }
  if (!PARQUET_SERDE_CLASS_NAMES.contains(getDeserializerClassName(schema))) {
    return Optional.empty();
  }
  if (!columnTypeSupported(columns)) {
    return Optional.empty();
  }
  return Optional.of(createParquetPageSource(configuration,path,start,length,schema,columns,partitionKeys,useParquetColumnNames,hiveStorageTimeZone,typeManager,isParquetPredicatePushdownEnabled(session),effectivePredicate));
}
