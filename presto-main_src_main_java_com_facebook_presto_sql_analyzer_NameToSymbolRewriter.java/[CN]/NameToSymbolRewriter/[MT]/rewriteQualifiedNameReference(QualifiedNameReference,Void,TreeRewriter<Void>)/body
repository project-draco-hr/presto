{
  List<Field> fields=descriptor.resolve(node.getName());
  if (fields.size() == 1) {
    return new QualifiedNameReference(Iterables.getOnlyElement(fields).getSymbol().toQualifiedName());
  }
  Preconditions.checkState(!node.getName().getPrefix().isPresent() && symbols.containsKey(Symbol.fromQualifiedName(node.getName())),"%s is an unknown symbol or field in descriptor %s",node.getName(),descriptor.getFields());
  return node;
}
