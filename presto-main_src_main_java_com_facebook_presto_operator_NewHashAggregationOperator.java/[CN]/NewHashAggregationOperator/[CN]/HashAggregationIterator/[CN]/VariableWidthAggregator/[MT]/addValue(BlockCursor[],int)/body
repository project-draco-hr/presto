{
  BlockCursor cursor;
  if (channel >= 0) {
    cursor=cursors[channel];
  }
 else {
    cursor=null;
  }
  T oldValue=intermediateValues.get(position);
  T newValue;
  if (step == Step.FINAL) {
    newValue=function.addIntermediate(cursor,oldValue);
  }
 else {
    newValue=function.addInput(cursor,oldValue);
  }
  intermediateValues.set(position,newValue);
}
