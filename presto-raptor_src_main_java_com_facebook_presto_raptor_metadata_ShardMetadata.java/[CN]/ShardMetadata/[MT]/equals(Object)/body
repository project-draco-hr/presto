{
  if (this == other) {
    return true;
  }
  if (other == null || getClass() != other.getClass()) {
    return false;
  }
  ShardMetadata that=(ShardMetadata)other;
  return Objects.equals(this.shardId,that.shardId) && Objects.equals(this.shardUuid,that.shardUuid) && Objects.equals(this.rowCount,that.rowCount)&& Objects.equals(this.dataSize,that.dataSize);
}
