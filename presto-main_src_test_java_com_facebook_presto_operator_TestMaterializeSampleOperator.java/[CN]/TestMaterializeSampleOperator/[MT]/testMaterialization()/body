{
  List<Page> input=rowPagesBuilder(BIGINT).addSequencePage(100,1).build();
  input=appendSampleWeight(input,2);
  OperatorFactory operatorFactory=new MaterializeSampleOperatorFactory(0,ImmutableList.of(BIGINT),1);
  Operator operator=operatorFactory.createOperator(driverContext);
  List<Page> expectedPages=rowPagesBuilder(BIGINT).addSequencePage(100,1).addSequencePage(100,1).build();
  MaterializedResult expected=resultBuilder(BIGINT).pages(expectedPages).build();
  OperatorAssertion.assertOperatorEqualsIgnoreOrder(operator,input,expected);
}
