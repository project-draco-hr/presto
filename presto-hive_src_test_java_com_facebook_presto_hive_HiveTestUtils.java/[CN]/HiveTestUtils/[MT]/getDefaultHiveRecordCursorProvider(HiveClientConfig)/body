{
  return ImmutableSet.<HiveRecordCursorProvider>builder().add(new OrcRecordCursorProvider()).add(new ParquetRecordCursorProvider(hiveClientConfig)).add(new DwrfRecordCursorProvider()).add(new ColumnarTextHiveRecordCursorProvider()).add(new ColumnarBinaryHiveRecordCursorProvider()).add(new GenericHiveRecordCursorProvider()).build();
}
