{
  HdfsEnvironment testHdfsEnvironment=createTestHdfsEnvironment(hiveClientConfig);
  return ImmutableSet.<HiveRecordCursorProvider>builder().add(new ParquetRecordCursorProvider(hiveClientConfig,testHdfsEnvironment)).add(new ColumnarTextHiveRecordCursorProvider(testHdfsEnvironment)).add(new ColumnarBinaryHiveRecordCursorProvider(testHdfsEnvironment)).add(new GenericHiveRecordCursorProvider()).build();
}
