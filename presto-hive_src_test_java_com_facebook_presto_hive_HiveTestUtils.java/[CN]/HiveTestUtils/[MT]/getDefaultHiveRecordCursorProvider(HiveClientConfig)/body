{
  HdfsEnvironment testHdfsEnvironment=createTestHdfsEnvironment(hiveClientConfig);
  return ImmutableSet.<HiveRecordCursorProvider>builder().add(new ParquetRecordCursorProvider(hiveClientConfig,testHdfsEnvironment)).add(new ColumnarTextHiveRecordCursorProvider()).add(new ColumnarBinaryHiveRecordCursorProvider()).add(new GenericHiveRecordCursorProvider()).build();
}
