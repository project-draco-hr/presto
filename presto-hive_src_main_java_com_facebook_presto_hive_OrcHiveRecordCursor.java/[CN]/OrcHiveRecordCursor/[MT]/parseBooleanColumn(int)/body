{
  checkArgument(!isPartitionColumn[column],"Column is a partition key");
  loaded[column]=true;
  Object object=getFieldValue(row,hiveColumnIndexes[column]);
  if (object == null) {
    nulls[column]=true;
  }
 else {
    nulls[column]=false;
    booleans[column]=((BooleanWritable)object).get();
  }
}
