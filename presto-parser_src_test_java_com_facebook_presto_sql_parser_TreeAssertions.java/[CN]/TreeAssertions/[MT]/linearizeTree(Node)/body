{
  final ImmutableList.Builder<Node> nodes=ImmutableList.builder();
  new DefaultTraversalVisitor<Node,Void>(){
    @Override public Node process(    Node node,    @Nullable Void context){
      Node result=super.process(node,context);
      nodes.add(node);
      return result;
    }
  }
.process(tree,null);
  return nodes.build();
}
