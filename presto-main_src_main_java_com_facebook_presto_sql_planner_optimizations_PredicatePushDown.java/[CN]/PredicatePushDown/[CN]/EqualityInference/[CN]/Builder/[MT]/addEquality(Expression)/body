{
  checkArgument(simpleEquality().apply(expression),"Expression must be a simple equality: " + expression);
  ComparisonExpression comparison=(ComparisonExpression)expression;
  addEquality(Symbol.fromQualifiedName(((QualifiedNameReference)comparison.getLeft()).getName()),Symbol.fromQualifiedName(((QualifiedNameReference)comparison.getRight()).getName()));
  return this;
}
