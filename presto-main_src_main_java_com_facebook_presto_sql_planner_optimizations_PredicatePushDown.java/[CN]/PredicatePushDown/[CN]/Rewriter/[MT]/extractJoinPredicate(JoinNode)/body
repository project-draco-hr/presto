{
  ImmutableList.Builder<Expression> builder=ImmutableList.builder();
  for (  JoinNode.EquiJoinClause equiJoinClause : joinNode.getCriteria()) {
    builder.add(new ComparisonExpression(ComparisonExpression.Type.EQUAL,new QualifiedNameReference(equiJoinClause.getLeft().toQualifiedName()),new QualifiedNameReference(equiJoinClause.getRight().toQualifiedName())));
  }
  return combineConjuncts(builder.build());
}
