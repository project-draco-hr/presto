{
  SubPlanBuilder current=node.getSource().accept(this,context);
  if (current.isPartitioned()) {
    current=newSubPlan(new ExchangeNode(current.getId(),current.getRoot().getOutputSymbols())).setPartitioned(false).setChildren(ImmutableList.of(current.build()));
  }
  current.setRoot(new OutputNode(current.getRoot(),node.getColumnNames(),node.getAssignments()));
  return current;
}
