{
  SubPlanBuilder subPlanBuilder=node.getSource().accept(this,context);
  subPlanBuilder.setRoot(new TableWriterNode(node.getId(),subPlanBuilder.getRoot(),node.getTableHandle(),node.getInputSymbols(),node.getInputTypes(),node.getColumnHandles(),node.getOutputTypes()));
  if (!createSingleNodePlan) {
    subPlanBuilder.addPartitionedSource(node.getId());
  }
  return subPlanBuilder;
}
