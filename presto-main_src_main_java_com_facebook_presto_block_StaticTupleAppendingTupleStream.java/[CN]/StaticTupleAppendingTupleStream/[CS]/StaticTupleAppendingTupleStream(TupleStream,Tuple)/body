{
  checkNotNull(delegate,"delegate is null");
  checkNotNull(tuple,"tuple is null");
  checkArgument(tuple.getTupleInfo().getFieldCount() > 0,"tuple should have at least one column");
  this.delegate=delegate;
  this.tuple=tuple;
  tupleInfo=new TupleInfo(ImmutableList.<TupleInfo.Type>builder().addAll(delegate.getTupleInfo().getTypes()).addAll(tuple.getTupleInfo().getTypes()).build());
}
