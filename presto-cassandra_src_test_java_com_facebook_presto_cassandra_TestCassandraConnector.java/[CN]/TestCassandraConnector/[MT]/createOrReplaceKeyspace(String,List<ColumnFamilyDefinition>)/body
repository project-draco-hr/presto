{
  Cluster cluster=getOrCreateCluster();
  KeyspaceDefinition keyspaceDefinition=HFactory.createKeyspaceDefinition(keyspaceName,StrategyModel.SIMPLE_STRATEGY.value(),1,columnFamilyDefinitions);
  if (cluster.describeKeyspace(keyspaceName) != null) {
    cluster.dropKeyspace(keyspaceName,true);
  }
  cluster.addKeyspace(keyspaceDefinition,true);
  return HFactory.createKeyspace(keyspaceName,cluster);
}
