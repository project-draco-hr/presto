{
  try {
    SortedRangeSet.none(BIGINT).getSpan();
    Assert.fail();
  }
 catch (  IllegalStateException e) {
  }
  Assert.assertEquals(SortedRangeSet.all(BIGINT).getSpan(),Range.all(BIGINT));
  Assert.assertEquals(SortedRangeSet.of(BIGINT,0L).getSpan(),Range.equal(BIGINT,0L));
  Assert.assertEquals(SortedRangeSet.of(Range.equal(BIGINT,0L),Range.equal(BIGINT,1L)).getSpan(),Range.range(BIGINT,0L,true,1L,true));
  Assert.assertEquals(SortedRangeSet.of(Range.equal(BIGINT,0L),Range.greaterThan(BIGINT,1L)).getSpan(),Range.greaterThanOrEqual(BIGINT,0L));
  Assert.assertEquals(SortedRangeSet.of(Range.lessThan(BIGINT,0L),Range.greaterThan(BIGINT,1L)).getSpan(),Range.all(BIGINT));
}
