{
  BlockCursor values=block.cursor();
  for (int position=0; position < block.getPositionCount(); position++) {
    checkState(values.advanceNextPosition());
    if (!values.isNull(0)) {
      count++;
      if (inputIsLong) {
        sum+=values.getLong(0);
      }
 else {
        sum+=values.getDouble(0);
      }
    }
  }
}
