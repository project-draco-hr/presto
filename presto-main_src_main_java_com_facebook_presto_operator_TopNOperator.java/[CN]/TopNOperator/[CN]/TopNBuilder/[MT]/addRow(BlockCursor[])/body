{
  long sizeDelta=0;
  if (globalCandidates.size() >= n) {
    Tuple[] previous=globalCandidates.remove();
    for (    Tuple tuple : previous) {
      sizeDelta-=tuple.size();
    }
    sizeDelta-=OVERHEAD_PER_TUPLE.toBytes();
  }
  Tuple[] row=getValues(cursors);
  for (  Tuple tuple : row) {
    sizeDelta+=tuple.size();
  }
  sizeDelta+=OVERHEAD_PER_TUPLE.toBytes();
  globalCandidates.add(row);
  return sizeDelta;
}
