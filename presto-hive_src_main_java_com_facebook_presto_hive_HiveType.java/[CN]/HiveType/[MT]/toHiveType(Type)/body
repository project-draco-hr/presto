{
  if (BooleanType.BOOLEAN.equals(type)) {
    return HIVE_BOOLEAN;
  }
  if (BigintType.BIGINT.equals(type)) {
    return HIVE_LONG;
  }
  if (DoubleType.DOUBLE.equals(type)) {
    return HIVE_DOUBLE;
  }
  if (VarcharType.VARCHAR.equals(type)) {
    return HIVE_STRING;
  }
  throw new IllegalArgumentException("unsupported type: " + type);
}
