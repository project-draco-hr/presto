{
  try {
    int index=0;
    for (    Tuple tuple : tuples) {
      for (      Object value : tuple.toValues()) {
        writer.write(String.valueOf(index));
        writer.write(":\t");
        writer.write(String.valueOf(value));
        writer.write('\n');
        index++;
      }
    }
    writer.write('\n');
    writer.flush();
  }
 catch (  IOException e) {
    throw Throwables.propagate(e);
  }
}
