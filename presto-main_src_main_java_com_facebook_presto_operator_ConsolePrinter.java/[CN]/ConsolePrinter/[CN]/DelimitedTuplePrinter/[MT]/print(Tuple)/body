{
  try {
    boolean first=true;
    for (    Tuple tuple : tuples) {
      if (!first) {
        writer.write(delimiter);
      }
      Joiner.on(delimiter).useForNull("").appendTo(writer,tuple.toValues());
      first=false;
    }
    writer.write('\n');
    writer.flush();
  }
 catch (  IOException e) {
    throw Throwables.propagate(e);
  }
}
