{
  ImmutableMap.Builder<Symbol,Type> types=ImmutableMap.builder();
  for (  Symbol symbol : planNode.getOutputSymbols()) {
    types.put(symbol,VARCHAR);
  }
  return new PlanFragment(new PlanFragmentId(planNode.getId() + "_fragment_id"),planNode,types.build(),SOURCE_DISTRIBUTION,planNode.getId(),new PartitionFunctionBinding(SINGLE_DISTRIBUTION,planNode.getOutputSymbols(),ImmutableList.of()));
}
