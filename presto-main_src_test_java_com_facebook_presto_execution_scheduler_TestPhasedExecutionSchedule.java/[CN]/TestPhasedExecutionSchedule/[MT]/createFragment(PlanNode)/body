{
  ImmutableMap.Builder<Symbol,Type> types=ImmutableMap.builder();
  for (  Symbol symbol : planNode.getOutputSymbols()) {
    types.put(symbol,VARCHAR);
  }
  return new PlanFragment(new PlanFragmentId(planNode.getId() + "_fragment_id"),planNode,types.build(),unknownPartitioning().getHandle(),ImmutableList.of(planNode.getId()),new PartitioningScheme(singlePartition(),planNode.getOutputSymbols()));
}
