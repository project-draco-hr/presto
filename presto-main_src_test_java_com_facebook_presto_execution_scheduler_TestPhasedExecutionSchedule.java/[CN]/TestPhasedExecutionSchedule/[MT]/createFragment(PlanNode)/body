{
  ImmutableMap.Builder<Symbol,Type> types=ImmutableMap.builder();
  for (  Symbol symbol : planNode.getOutputSymbols()) {
    types.put(symbol,VARCHAR);
  }
  return new PlanFragment(new PlanFragmentId(planNode.getId() + "_fragment_id"),planNode,types.build(),planNode.getOutputSymbols(),SOURCE,planNode.getId(),Optional.empty());
}
