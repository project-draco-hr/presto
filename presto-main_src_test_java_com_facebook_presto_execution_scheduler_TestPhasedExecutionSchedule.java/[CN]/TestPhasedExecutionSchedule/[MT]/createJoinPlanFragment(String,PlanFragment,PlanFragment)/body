{
  PlanNode planNode=new JoinNode(new PlanNodeId(name + "_id"),CROSS,new RemoteSourceNode(new PlanNodeId("probe_id"),probeFragment.getId(),ImmutableList.of()),new RemoteSourceNode(new PlanNodeId("build_id"),buildFragment.getId(),ImmutableList.of()),ImmutableList.of(),Optional.<Symbol>empty(),Optional.<Symbol>empty());
  return createFragment(planNode);
}
