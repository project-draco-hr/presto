{
  if (!isDeserializerClass(schema,OrcSerde.class)) {
    return Optional.empty();
  }
  return Optional.of(createOrcPageSource(new OrcMetadataReader(),hdfsEnvironment,session.getUser(),configuration,path,start,length,columns,partitionKeys,useOrcColumnNames,effectivePredicate,hiveStorageTimeZone,typeManager,getOrcMaxMergeDistance(session),getOrcMaxBufferSize(session),getOrcStreamBufferSize(session),isOrcBloomFiltersEnabled(session)));
}
