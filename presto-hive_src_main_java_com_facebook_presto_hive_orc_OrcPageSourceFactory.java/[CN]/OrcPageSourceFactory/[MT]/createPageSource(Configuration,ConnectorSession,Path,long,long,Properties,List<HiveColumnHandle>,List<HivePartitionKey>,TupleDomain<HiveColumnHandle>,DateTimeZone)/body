{
  if (!isOptimizedReaderEnabled(session,enabled)) {
    return Optional.empty();
  }
  if (!isDeserializerClass(schema,OrcSerde.class)) {
    return Optional.empty();
  }
  return Optional.of(createOrcPageSource(new OrcMetadataReader(),configuration,path,start,length,columns,partitionKeys,effectivePredicate,hiveStorageTimeZone,typeManager,getOrcMaxMergeDistance(session,orcMaxMergeDistance),getOrcMaxBufferSize(session,orcMaxBufferSize),getOrcStreamBufferSize(session,orcStreamBufferSize)));
}
