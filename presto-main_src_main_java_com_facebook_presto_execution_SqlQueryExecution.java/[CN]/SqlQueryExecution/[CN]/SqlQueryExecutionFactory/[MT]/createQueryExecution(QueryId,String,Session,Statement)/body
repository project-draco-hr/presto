{
  SqlQueryExecution queryExecution=new SqlQueryExecution(queryId,query,session,locationFactory.createQueryLocation(queryId),statement,metadata,splitManager,nodeScheduler,planOptimizers,remoteTaskFactory,locationFactory,scheduleSplitBatchSize,maxPendingSplitsPerNode,initialHashPartitions,experimentalSyntaxEnabled,executor,shardManager);
  return queryExecution;
}
