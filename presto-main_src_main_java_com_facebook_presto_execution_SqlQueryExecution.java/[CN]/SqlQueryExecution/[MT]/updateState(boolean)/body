{
  if (!forceRefresh) {
    if (stateMachine.isDone()) {
      return;
    }
  }
  SqlStageExecution outputStage=this.outputStage.get();
  if (outputStage == null) {
    return;
  }
  waitForFuture(outputStage.updateState(forceRefresh));
  outputStage.updateState(forceRefresh);
  doUpdateState(outputStage.getStageInfo());
}
