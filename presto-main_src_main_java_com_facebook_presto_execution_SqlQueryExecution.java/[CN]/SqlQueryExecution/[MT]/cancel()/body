{
  while (true) {
    QueryState queryState=this.queryState.get();
    if (queryState.isDone()) {
      break;
    }
    if (this.queryState.compareAndSet(queryState,QueryState.CANCELED)) {
      break;
    }
  }
  for (  HttpTaskClient taskClient : Iterables.concat(stages.values())) {
    taskClient.cancel();
  }
}
