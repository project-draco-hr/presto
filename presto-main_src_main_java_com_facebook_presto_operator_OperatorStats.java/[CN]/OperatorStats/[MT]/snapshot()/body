{
  Duration wall=null;
  Duration cpu=null;
  Duration user=null;
  CpuTimer cpuTimer=this.cpuTimer.get();
  if (cpuTimer != null) {
    CpuDuration cpuDuration=cpuTimer.elapsedTime();
    wall=cpuDuration.getWall();
    cpu=cpuDuration.getCpu();
    user=cpuDuration.getUser();
  }
  return new SplitExecutionStats(executionStartTime.get(),timeToFirstByte.get(),timeToLastByte.get(),completedDataSize.snapshot(),completedPositions.snapshot(),wall,cpu,user,splitInfo);
}
