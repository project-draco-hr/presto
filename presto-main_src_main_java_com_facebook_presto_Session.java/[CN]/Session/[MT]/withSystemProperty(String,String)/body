{
  requireNonNull(key,"key is null");
  requireNonNull(value,"value is null");
  Map<String,String> systemProperties=new LinkedHashMap<>(this.systemProperties);
  systemProperties.put(key,value);
  return new Session(identity,source,catalog,schema,timeZoneKey,locale,remoteUserAddress,userAgent,startTime,systemProperties,catalogProperties,sessionPropertyManager);
}
