{
  checkNotNull(key,"key is null");
  checkNotNull(value,"value is null");
  Map<String,String> systemProperties=new LinkedHashMap<>(this.systemProperties);
  systemProperties.put(key,value);
  return new Session(user,source,catalog,schema,timeZoneKey,locale,remoteUserAddress,userAgent,startTime,systemProperties,catalogProperties,sessionPropertyManager);
}
