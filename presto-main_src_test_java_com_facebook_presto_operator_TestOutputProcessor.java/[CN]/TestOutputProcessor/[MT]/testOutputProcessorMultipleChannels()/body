{
  Operator source=createOperator(new Page(valueBlock,valueBlock,valueBlock));
  CollectingHandler handler=new CollectingHandler();
  processOutput(source,handler);
  assertEquals(handler.getRows(),list(list(0L,"zero",0L,"zero",0L,"zero"),list(1L,"one",1L,"one",1L,"one"),list(2L,"two",2L,"two",2L,"two")));
}
