{
  MaterializedResult result=queryRunner.execute("SELECT count(1) from " + tableName);
  MaterializedResult expected=MaterializedResult.resultBuilder(SESSION,BigintType.BIGINT).row(0).build();
  assertEquals(result,expected);
  int count=1000;
  populateData(count);
  result=queryRunner.execute("SELECT count(1) from " + tableName);
  expected=MaterializedResult.resultBuilder(SESSION,BigintType.BIGINT).row(count).build();
  assertEquals(result,expected);
}
