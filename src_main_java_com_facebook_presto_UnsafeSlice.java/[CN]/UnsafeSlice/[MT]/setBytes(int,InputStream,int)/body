{
  checkIndexLength(index,length);
  byte[] bytes=new byte[4096];
  ByteBuffer dst=toByteBuffer(index,length);
  while (dst.remaining() > 0) {
    int n=in.read(bytes);
    if (n < 0) {
      if (dst.position() == 0) {
        return -1;
      }
      break;
    }
    dst.put(bytes,0,n);
  }
  return dst.position();
}
