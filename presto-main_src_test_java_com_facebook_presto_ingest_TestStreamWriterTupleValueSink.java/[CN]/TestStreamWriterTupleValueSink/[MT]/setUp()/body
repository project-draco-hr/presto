{
  byteArrayOutputStream=new ByteArrayOutputStream(1024);
  serde=TupleStreamSerdes.Encoding.RAW.createSerde();
  streamWriterTupleValueSink=new StreamWriterTupleValueSink(new OutputSupplier<OutputStream>(){
    @Override public OutputStream getOutput() throws IOException {
      return byteArrayOutputStream;
    }
  }
,serde.createSerializer());
  tupleStream=new GroupByOperator(Blocks.createTupleStream(0,"a","a","b","b"));
