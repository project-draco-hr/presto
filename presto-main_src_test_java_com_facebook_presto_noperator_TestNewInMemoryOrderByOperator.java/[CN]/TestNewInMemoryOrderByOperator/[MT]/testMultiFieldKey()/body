{
  TupleInfo tupleInfo=new TupleInfo(VARIABLE_BINARY,FIXED_INT_64);
  List<Page> input=rowPagesBuilder(tupleInfo).row("a",1).row("b",2).pageBreak().row("b",3).row("a",4).build();
  NewInMemoryOrderByOperatorFactory operatorFactory=new NewInMemoryOrderByOperatorFactory(0,ImmutableList.of(tupleInfo),0,new int[]{0},10);
  NewOperator operator=operatorFactory.createOperator(driverContext);
  MaterializedResult expected=resultBuilder(tupleInfo).row("a",1).row("a",4).row("b",2).row("b",3).build();
  assertOperatorEquals(operator,input,expected);
}
