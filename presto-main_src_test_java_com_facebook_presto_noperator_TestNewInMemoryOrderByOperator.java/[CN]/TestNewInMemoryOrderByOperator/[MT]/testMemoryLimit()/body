{
  List<Page> input=rowPagesBuilder(SINGLE_LONG,SINGLE_DOUBLE).row(1,0.1).row(2,0.2).pageBreak().row(-1,-0.1).row(4,0.4).build();
  NewOperator operator=new NewInMemoryOrderByOperator(ImmutableList.of(SINGLE_LONG,SINGLE_DOUBLE),0,new int[]{1},10,new TaskMemoryManager(new DataSize(10,Unit.BYTE)));
  toPages(operator,input);
}
