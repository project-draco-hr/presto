{
  List<Page> input=rowPagesBuilder(SINGLE_LONG,SINGLE_DOUBLE).row(1,0.1).row(2,0.2).pageBreak().row(-1,-0.1).row(4,0.4).pageBreak().row(5,0.5).row(4,0.41).row(6,0.6).row(5,0.5).pageBreak().build();
  input=appendSampleWeight(input,2);
  TopNOperatorFactory factory=new TopNOperatorFactory(0,5,ImmutableList.of(singleColumn(FIXED_INT_64,0),singleColumn(DOUBLE,1),singleColumn(FIXED_INT_64,2)),Ordering.from(new FieldOrderedTupleComparator(ImmutableList.of(0),ImmutableList.of(SortOrder.DESC_NULLS_LAST))),Optional.of(input.get(0).getChannelCount() - 1),false);
  Operator operator=factory.createOperator(driverContext);
  MaterializedResult expected=resultBuilder(FIXED_INT_64,DOUBLE,FIXED_INT_64).row(6,0.6,2).row(5,0.5,3).build();
  assertOperatorEquals(operator,input,expected);
}
