{
  List<Page> input=rowPagesBuilder(BIGINT,DOUBLE).row(1,0.1).row(2,0.2).pageBreak().row(-1,-0.1).row(4,0.4).pageBreak().row(5,0.5).row(4,0.41).row(6,0.6).pageBreak().build();
  TopNOperatorFactory operatorFactory=new TopNOperatorFactory(0,new PlanNodeId("test"),ImmutableList.of(BIGINT,DOUBLE),2,ImmutableList.of(0),ImmutableList.of(ASC_NULLS_LAST),false);
  Operator operator=operatorFactory.createOperator(driverContext);
  MaterializedResult expected=resultBuilder(driverContext.getSession(),BIGINT,DOUBLE).row(-1,-0.1).row(1,0.1).build();
  assertOperatorEquals(operator,input,expected);
}
