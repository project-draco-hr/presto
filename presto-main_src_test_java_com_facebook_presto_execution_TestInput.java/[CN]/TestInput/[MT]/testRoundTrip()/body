{
  Input expected=new Input("connectorId","schema","table",ImmutableList.of(new Column("column1","string",Optional.<SimpleDomain>absent()),new Column("column2","string",Optional.<SimpleDomain>absent()),new Column("column3","string",Optional.<SimpleDomain>absent())));
  String json=codec.toJson(expected);
  Input actual=codec.fromJson(json);
  assertEquals(actual,expected);
}
