{
  boolean minSet=false;
  boolean maxSet=false;
  boolean min=false;
  boolean max=false;
  while (true) {
    int batchSize=reader.nextBatch();
    if (batchSize <= 0) {
      break;
    }
    BooleanVector vector=new BooleanVector(batchSize);
    reader.readVector(columnIndex,vector);
    for (int i=0; i < batchSize; i++) {
      if (vector.isNull[i]) {
        continue;
      }
      if (!minSet || Boolean.compare(vector.vector[i],min) < 0) {
        minSet=true;
        min=vector.vector[i];
      }
      if (!maxSet || Boolean.compare(vector.vector[i],max) > 0) {
        maxSet=true;
        max=vector.vector[i];
      }
    }
  }
  return new ColumnStats(columnId,minSet ? min : null,maxSet ? max : null);
}
