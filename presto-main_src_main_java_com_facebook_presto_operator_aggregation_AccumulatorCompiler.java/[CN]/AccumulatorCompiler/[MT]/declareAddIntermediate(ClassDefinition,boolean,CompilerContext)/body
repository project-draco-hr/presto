{
  ImmutableList.Builder<NamedParameterDefinition> parameters=ImmutableList.builder();
  if (grouped) {
    parameters.add(arg("groupIdsBlock",GroupByIdBlock.class));
  }
  parameters.add(arg("block",com.facebook.presto.spi.block.Block.class));
