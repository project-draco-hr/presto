{
  Block block=new Block(context).comment("sampleWeight = computeSampleWeight(masks, sampleWeights, position);").getVariable(masks).getVariable(sampleWeights).getVariable(position).invokeStatic(ApproximateUtils.class,"computeSampleWeight",long.class,com.facebook.presto.spi.block.Block.class,com.facebook.presto.spi.block.Block.class,int.class).putVariable(sampleWeight);
  block.append(new IfStatement("if(sampleWeight > 0)").condition(new Block(context).getVariable(sampleWeight).invokeStatic(CompilerOperations.class,"longGreaterThanZero",boolean.class,long.class)).ifTrue(body).ifFalse(NOP));
  return block;
}
