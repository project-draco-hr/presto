{
  TupleStream tupleStream=new UncompressedBlockStream(new TupleInfo(VARIABLE_BINARY),Blocks.createBlock(1,"a","a","b","a","c"),Blocks.createBlock(6,"c","a","b","b","b"),Blocks.createBlock(100,"y","y","a","y","b"),Blocks.createBlock(200,"b"));
  rleSerde.serialize(tupleStream,sliceOutput);
  Blocks.assertBlockStreamEquals(tupleStream,rleSerde.deserialize(sliceOutput.slice()));
}
