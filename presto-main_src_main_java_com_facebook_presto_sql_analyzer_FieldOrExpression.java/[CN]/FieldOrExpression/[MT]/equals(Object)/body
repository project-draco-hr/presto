{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  FieldOrExpression that=(FieldOrExpression)o;
  if (!expression.equals(that.expression)) {
    return false;
  }
  if (!fieldIndex.equals(that.fieldIndex)) {
    return false;
  }
  return true;
}
