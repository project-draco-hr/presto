{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  SampledRelation that=(SampledRelation)o;
  return Objects.equals(relation,that.relation) && Objects.equals(type,that.type) && Objects.equals(samplePercentage,that.samplePercentage)&& Objects.equals(columnsToStratifyOn,that.columnsToStratifyOn);
}
