{
  InternalTable.Builder table=InternalTable.builder(informationSchemaTableColumns(TABLE_TABLES));
  for (  QualifiedTableName name : getTablesList(catalogName,filters)) {
    table.add(table.getTupleInfo().builder().append(name.getCatalogName()).append(name.getSchemaName()).append(name.getTableName()).append("BASE TABLE").build());
  }
  return table.build();
}
