{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Pair pair=(Pair)o;
  if (position != pair.position) {
    return false;
  }
  if (value != null ? !value.equals(pair.value) : pair.value != null) {
    return false;
  }
  return true;
}
