{
  PlanWithProperties probe=planAndEnforce(node.getLeft(),defaultParallelism(session),parentPreferences.constrainTo(node.getLeft().getOutputSymbols()).withDefaultParallelism(session));
  PlanWithProperties build=planAndEnforce(node.getRight(),singleStream(),singleStream());
  return rebaseAndDeriveProperties(node,ImmutableList.of(probe,build));
}
