{
  Page page=getAtMostOnePage(operator,SOURCE_PAGE);
  boolean value;
  if (page != null) {
    assertEquals(page.getPositionCount(),1);
    assertEquals(page.getChannelCount(),1);
    assertTrue(page.getBoolean(operator.getTypes().get(0),0,0));
    value=true;
  }
 else {
    value=false;
  }
  return value;
}
