{
  if (operator.needsInput()) {
    operator.addInput(SOURCE_PAGE);
  }
  Page result=operator.getOutput();
  operator.finish();
  while (!operator.isFinished()) {
    assertTrue(operator.isBlocked().isDone());
    Page output=operator.getOutput();
    if (output != null) {
      assertNull(result);
      result=output;
    }
  }
  return result;
}
