{
  try {
    beforeRequest.await(1,TimeUnit.SECONDS);
  }
 catch (  Exception e) {
    if (e instanceof InterruptedException) {
      Thread.currentThread().interrupt();
    }
    throw Throwables.propagate(e);
  }
  try {
    return response.get();
  }
  finally {
    try {
      afterRequest.await(1,TimeUnit.SECONDS);
    }
 catch (    Exception e) {
      if (e instanceof InterruptedException) {
        Thread.currentThread().interrupt();
      }
      throw Throwables.propagate(e);
    }
  }
}
