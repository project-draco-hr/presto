{
  QualifiedTableName table=new QualifiedTableName(r.getString("catalog_name"),r.getString("schema_name"),r.getString("table_name"));
  String typeName=r.getString("data_type");
  Type type=typeManager.getType(typeName);
  checkArgument(type != null,"Unknown type %s",typeName);
  return new TableColumn(table,r.getString("column_name"),r.getInt("ordinal_position"),type,r.getLong("column_id"));
}
