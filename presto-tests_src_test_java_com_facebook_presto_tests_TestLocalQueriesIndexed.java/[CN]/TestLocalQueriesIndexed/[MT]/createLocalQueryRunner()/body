{
  Session defaultSession=testSessionBuilder().setCatalog("local").setSchema(TINY_SCHEMA_NAME).build();
  LocalQueryRunner localQueryRunner=new LocalQueryRunner(defaultSession);
  localQueryRunner.createCatalog(defaultSession.getCatalog().get(),new IndexedTpchConnectorFactory(localQueryRunner.getNodeManager(),INDEX_SPEC,1),ImmutableMap.<String,String>of());
  return localQueryRunner;
}
