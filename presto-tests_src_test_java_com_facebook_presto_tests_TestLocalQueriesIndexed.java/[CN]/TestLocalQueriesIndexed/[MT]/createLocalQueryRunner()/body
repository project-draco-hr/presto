{
  ConnectorSession defaultSession=new ConnectorSession("user","test","local",TpchMetadata.TINY_SCHEMA_NAME,UTC_KEY,Locale.ENGLISH,null,null);
  LocalQueryRunner localQueryRunner=new LocalQueryRunner(defaultSession);
  localQueryRunner.createCatalog(defaultSession.getCatalog(),new IndexedTpchConnectorFactory(localQueryRunner.getNodeManager(),INDEX_SPEC,1),ImmutableMap.<String,String>of());
  return localQueryRunner;
}
