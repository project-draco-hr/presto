{
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("shard-cleaner.enabled","true").put("shard-cleaner.interval","10m").put("shard-cleaner.max-threads","100").build();
  ShardCleanerConfig expected=new ShardCleanerConfig().setEnabled(true).setCleanerInterval(new Duration(10,TimeUnit.MINUTES)).setMaxThreads(100);
  ConfigAssertions.assertFullMapping(properties,expected);
}
