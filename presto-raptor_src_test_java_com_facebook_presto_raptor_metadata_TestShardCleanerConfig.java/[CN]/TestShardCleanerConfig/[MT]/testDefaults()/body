{
  assertRecordedDefaults(recordDefaults(ShardCleanerConfig.class).setMaxTransactionAge(new Duration(1,DAYS)).setLocalCleanerInterval(new Duration(1,HOURS)).setLocalCleanTime(new Duration(4,HOURS)).setLocalPurgeTime(new Duration(3,DAYS)).setBackupCleanerInterval(new Duration(5,MINUTES)).setBackupCleanTime(new Duration(1,DAYS)).setBackupPurgeTime(new Duration(3,DAYS)).setBackupDeletionThreads(50));
}
