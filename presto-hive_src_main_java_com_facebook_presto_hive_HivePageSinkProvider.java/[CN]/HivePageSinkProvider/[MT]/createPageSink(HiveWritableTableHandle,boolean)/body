{
  return new HivePageSink(handle.getSchemaName(),handle.getTableName(),isCreateTable,handle.getInputColumns(),handle.getHiveStorageFormat(),handle.getLocationHandle(),locationService,handle.getFilePrefix(),metastore,pageIndexerFactory,typeManager,hdfsEnvironment,respectTableFormat,maxOpenPartitions,immutablePartitions,compressed,partitionUpdateCodec);
}
