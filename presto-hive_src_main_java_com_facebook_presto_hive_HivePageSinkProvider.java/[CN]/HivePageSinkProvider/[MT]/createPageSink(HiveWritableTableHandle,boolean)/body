{
  return new HivePageSink(handle.getSchemaName(),handle.getTableName(),isCreateTable,handle.getInputColumns(),handle.getTableStorageFormat(),handle.getPartitionStorageFormat(),handle.getLocationHandle(),locationService,handle.getFilePrefix(),metastore,pageIndexerFactory,typeManager,hdfsEnvironment,maxOpenPartitions,immutablePartitions,compressed,partitionUpdateCodec);
}
