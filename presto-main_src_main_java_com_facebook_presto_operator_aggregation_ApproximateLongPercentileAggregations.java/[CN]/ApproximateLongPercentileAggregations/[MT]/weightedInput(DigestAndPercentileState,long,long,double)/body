{
  QuantileDigest digest=state.getDigest();
  if (digest == null) {
    digest=new QuantileDigest(0.01);
    state.setDigest(digest);
    state.addMemoryUsage(digest.estimatedInMemorySizeInBytes());
  }
  state.addMemoryUsage(-digest.estimatedInMemorySizeInBytes());
  digest.add(value,weight);
  state.addMemoryUsage(digest.estimatedInMemorySizeInBytes());
  state.setPercentile(percentile);
}
