{
  TupleDomain<ColumnHandle> tupleDomain=TupleDomain.withColumnDomains(ImmutableMap.of(COL1,Domain.create(ValueSet.ofRanges(range(BIGINT,100L,false,200L,true)),false),COL2,Domain.singleValue(VARCHAR,utf8Slice("a value"))));
  Document query=MongoSession.buildQuery(tupleDomain);
  Document expected=new Document().append(COL1.getName(),new Document().append("$gt",100L).append("$lte",200L)).append(COL2.getName(),new Document("$eq","a value"));
  assertEquals(query,expected);
}
