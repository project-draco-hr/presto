{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  SourceInfo that=(SourceInfo)o;
  if (port != that.port) {
    return false;
  }
  if (timestamp != that.timestamp) {
    return false;
  }
  if (host != null ? !host.equals(that.host) : that.host != null) {
    return false;
  }
  return true;
}
