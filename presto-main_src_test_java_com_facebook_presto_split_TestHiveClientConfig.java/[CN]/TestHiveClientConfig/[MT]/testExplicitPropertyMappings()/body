{
  Map<String,String> properties=new ImmutableMap.Builder<String,String>().put("hive.max-chunk-size","256MB").put("hive.max-outstanding-chunks","10").put("hive.max-chunk-iterator-threads","2").build();
  HiveClientConfig expected=new HiveClientConfig().setMaxChunkSize(new DataSize(256,Unit.MEGABYTE)).setMaxOutstandingChunks(10).setMaxChunkIteratorThreads(2);
  ConfigAssertions.assertFullMapping(properties,expected);
}
