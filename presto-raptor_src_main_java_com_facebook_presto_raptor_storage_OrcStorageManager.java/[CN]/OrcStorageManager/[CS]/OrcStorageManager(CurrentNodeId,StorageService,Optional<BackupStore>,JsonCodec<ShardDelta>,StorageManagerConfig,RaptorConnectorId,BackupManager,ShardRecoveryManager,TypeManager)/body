{
  this(currentNodeId.toString(),storageService,backupStore,shardDeltaCodec,config.getOrcMaxMergeDistance(),config.getOrcMaxReadSize(),config.getOrcStreamBufferSize(),backgroundBackupManager,recoveryManager,typeManager,connectorId.toString(),config.getDeletionThreads(),config.getShardRecoveryTimeout(),config.getMaxShardRows(),config.getMaxShardSize());
