{
  this(nodeManager.getCurrentNode().getNodeIdentifier(),storageService,backupStore,shardDeltaCodec,readerAttributes,backgroundBackupManager,recoveryManager,shardRecorder,typeManager,connectorId.toString(),config.getDeletionThreads(),config.getShardRecoveryTimeout(),config.getMaxShardRows(),config.getMaxShardSize(),config.getMinAvailableSpace());
