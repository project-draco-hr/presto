{
  this(currentNodeId.toString(),storageService,backupStore,shardDeltaCodec,config.getOrcMaxMergeDistance(),config.getOrcMaxReadSize(),config.getOrcStreamBufferSize(),recoveryManager,typeManager,config.getShardRecoveryTimeout(),config.getMaxShardRows(),config.getMaxShardSize(),config.getMaxBufferSize());
}
