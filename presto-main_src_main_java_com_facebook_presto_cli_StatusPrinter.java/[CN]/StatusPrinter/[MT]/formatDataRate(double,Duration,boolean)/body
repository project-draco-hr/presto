{
  double rate=dataSize / duration.convertTo(SECONDS);
  if (Double.isNaN(rate) || Double.isInfinite(rate)) {
    rate=0;
  }
  String rateString=formatDataSize((long)rate,false);
  if (longForm) {
    if (!rateString.endsWith("B")) {
      rateString+="B";
    }
    rateString+="/s";
  }
  return rateString;
}
