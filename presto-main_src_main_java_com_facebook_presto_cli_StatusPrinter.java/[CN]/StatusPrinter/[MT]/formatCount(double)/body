{
  String unit=" ";
  if (count > 1000) {
    count/=1000;
    unit="K";
  }
  if (count > 1000) {
    count/=1000;
    unit="M";
  }
  if (count > 1000) {
    count/=1000;
    unit="B";
  }
  if (count > 1000) {
    count/=1000;
    unit="T";
  }
  if (count > 1000) {
    count/=1000;
    unit="Q";
  }
  return String.format("%d%s",(long)count,unit);
}
