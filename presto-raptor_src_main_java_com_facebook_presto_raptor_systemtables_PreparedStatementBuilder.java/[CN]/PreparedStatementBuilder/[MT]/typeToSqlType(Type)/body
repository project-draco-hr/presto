{
  if (type == BIGINT) {
    return Types.BIGINT;
  }
  if (type == DOUBLE) {
    return Types.DOUBLE;
  }
  if (type == BOOLEAN) {
    return Types.BOOLEAN;
  }
  if (type == VARCHAR) {
    return Types.VARCHAR;
  }
  if (type == VARBINARY) {
    return Types.VARBINARY;
  }
  throw new IllegalArgumentException("Unknown type: " + type);
}
