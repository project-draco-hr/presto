{
switch (type) {
case FIXED_INT_64:
{
      Long value=record.getLong(field);
      if (value == null) {
        value=0L;
      }
      output.append(value);
      break;
    }
case VARIABLE_BINARY:
{
    String value=record.getString(field);
    if (value == null) {
      value="";
    }
    output.append(value.getBytes(Charsets.UTF_8));
    break;
  }
case DOUBLE:
{
  Double value=record.getDouble(field);
  if (value == null) {
    value=0.0;
  }
  output.append(value);
  break;
}
}
}
