{
  int pos=(murmurHash3(hashPosition(position) ^ mask)) & mask;
  while (key[pos] != -1) {
    int currentKey=key[pos];
    if (positionEqualsPosition(currentKey,position)) {
      key[pos]=position;
      positionLinks[position]=currentKey;
      return;
    }
    pos=(pos + 1) & mask;
  }
  key[pos]=position;
}
