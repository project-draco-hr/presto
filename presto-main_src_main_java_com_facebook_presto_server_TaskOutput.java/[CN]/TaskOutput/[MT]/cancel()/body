{
  while (true) {
    State state=taskState.get();
    if (state != State.PREPARING && state != State.RUNNING) {
      break;
    }
    if (taskState.compareAndSet(state,State.CANCELED)) {
      break;
    }
  }
  for (  QueryState outputBuffer : outputBuffers.values()) {
    outputBuffer.cancel();
  }
}
