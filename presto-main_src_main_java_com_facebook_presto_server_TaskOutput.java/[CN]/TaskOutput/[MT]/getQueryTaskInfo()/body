{
  updateState();
  return new QueryTaskInfo(taskId,location,getOutputBufferStates(),getTupleInfos(),getState(),getBufferedPageCount(),getSplits(),getStartedSplits(),getCompletedSplits(),(long)getSplitCpu().toMillis(),getInputDataSize().toBytes(),getInputPositions(),getCompletedDataSize().toBytes(),getCompletedPositions(),0,0);
}
