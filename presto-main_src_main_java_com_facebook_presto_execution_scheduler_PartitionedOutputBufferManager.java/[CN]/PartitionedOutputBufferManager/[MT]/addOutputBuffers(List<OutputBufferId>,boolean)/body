{
  OutputBuffers outputBuffers;
synchronized (this) {
    if (noMoreBufferIds) {
      return;
    }
    for (    OutputBufferId newBuffer : newBuffers) {
      partitions.put(newBuffer,newBuffer.getId());
    }
    if (!noMoreBuffers) {
      return;
    }
    noMoreBufferIds=true;
    outputBuffers=createInitialEmptyOutputBuffers().withBuffers(partitions).withNoMoreBufferIds();
  }
  outputBufferTarget.accept(outputBuffers);
}
