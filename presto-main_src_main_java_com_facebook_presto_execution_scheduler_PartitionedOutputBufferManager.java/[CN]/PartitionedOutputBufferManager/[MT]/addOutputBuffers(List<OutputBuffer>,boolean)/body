{
synchronized (this) {
    if (noMoreBufferIds) {
      return;
    }
    for (    OutputBuffer newBuffer : newBuffers) {
      partitions.put(newBuffer.getBufferId(),newBuffer.getPartition());
    }
    if (!noMoreBuffers) {
      return;
    }
    noMoreBufferIds=true;
  }
  OutputBuffers outputBuffers=createInitialEmptyOutputBuffers().withBuffers(partitions).withNoMoreBufferIds();
  outputBufferTarget.accept(outputBuffers);
}
