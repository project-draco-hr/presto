{
  assertFunction("INTERVAL '3' SECOND + INTERVAL '3' SECOND",new IntervalDayTime(6 * 1000));
  assertFunction("INTERVAL '6' DAY + INTERVAL '6' DAY",new IntervalDayTime(12 * 24 * 60* 60* 1000));
  assertFunction("INTERVAL '3' SECOND + INTERVAL '6' DAY",new IntervalDayTime((6 * 24 * 60* 60* 1000) + (3 * 1000)));
}
