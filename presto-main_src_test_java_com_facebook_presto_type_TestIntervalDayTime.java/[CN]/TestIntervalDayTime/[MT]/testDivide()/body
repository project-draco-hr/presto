{
  assertFunction("INTERVAL '3' SECOND / 2",new IntervalDayTime(1500));
  assertFunction("INTERVAL '6' SECOND / 2.5",new IntervalDayTime(2400));
  assertFunction("INTERVAL '3' DAY / 2",new IntervalDayTime((long)(1.5 * 24 * 60* 60* 1000)));
  assertFunction("INTERVAL '4' DAY / 2.5",new IntervalDayTime((long)(1.6 * 24 * 60* 60* 1000)));
}
