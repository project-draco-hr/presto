{
  assertFunction("INTERVAL '6' SECOND * 2",new IntervalDayTime(12 * 1000));
  assertFunction("2 * INTERVAL '6' SECOND",new IntervalDayTime(12 * 1000));
  assertFunction("INTERVAL '1' SECOND * 2.5",new IntervalDayTime(2500));
  assertFunction("2.5 * INTERVAL '1' SECOND",new IntervalDayTime(2500));
  assertFunction("INTERVAL '6' DAY * 2",new IntervalDayTime(12 * 24 * 60* 60* 1000));
  assertFunction("2 * INTERVAL '6' DAY",new IntervalDayTime(12 * 24 * 60* 60* 1000));
  assertFunction("INTERVAL '1' DAY * 2.5",new IntervalDayTime((long)(2.5 * 24 * 60* 60* 1000)));
  assertFunction("2.5 * INTERVAL '1' DAY",new IntervalDayTime((long)(2.5 * 24 * 60* 60* 1000)));
}
