{
  long rows=0;
  long bytes=0;
  try (PageIterator pages=sink.iterator(new OperatorStats())){
    while (pages.hasNext()) {
      Page page=pages.next();
      rows+=page.getPositionCount();
      bytes=page.getDataSize().toBytes();
    }
  }
   handler.finish();
  return new OutputStats(rows,bytes);
}
