{
  checkArgument(listType.getFieldCount() == 1,"Expected LIST column '%s' to only have one field, but has %s fields",columnName,listType.getFieldCount());
  parquet.schema.Type elementType=listType.getType(0);
  if (elementType.isPrimitive() || elementType.asGroupType().getFieldCount() > 1 || elementType.getName().equals("array") || elementType.getName().equals(listType.getName() + "_tuple")) {
    elementConverter=createConverter(columnName + ".element",elementType);
  }
 else {
    elementConverter=new ParquetListEntryConverter(columnName,elementType.asGroupType());
  }
}
