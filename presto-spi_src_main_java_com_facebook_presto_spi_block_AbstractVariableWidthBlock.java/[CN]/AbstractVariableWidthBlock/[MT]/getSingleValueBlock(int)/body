{
  if (isNull(position)) {
    return new VariableWidthBlock(1,Slices.wrappedBuffer(new byte[0]),new int[]{0,0},Slices.wrappedBooleanArray(true));
  }
  int offset=getPositionOffset(position);
  int entrySize=getLength(position);
  Slice copy=Slices.copyOf(getRawSlice(position),offset,entrySize);
  return new VariableWidthBlock(1,copy,new int[]{0,copy.length()},Slices.wrappedBooleanArray(false));
}
