{
  this.session=checkNotNull(session,"session is null");
  this.queryResultsCodec=checkNotNull(queryResultsCodec,"queryResultsCodec is null");
  this.httpClient=new StandaloneNettyAsyncHttpClient("cli",new HttpClientConfig().setConnectTimeout(new Duration(1,TimeUnit.DAYS)).setReadTimeout(new Duration(10,TimeUnit.DAYS)));
