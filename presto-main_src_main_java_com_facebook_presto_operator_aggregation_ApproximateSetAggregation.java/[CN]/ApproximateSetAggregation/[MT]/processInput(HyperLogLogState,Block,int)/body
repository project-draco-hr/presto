{
  HyperLogLog hll=state.getHyperLogLog();
  if (hll == null) {
    hll=HyperLogLog.newInstance(NUMBER_OF_BUCKETS);
    state.setHyperLogLog(hll);
    state.addMemoryUsage(hll.estimatedInMemorySize());
  }
  state.addMemoryUsage(-hll.estimatedInMemorySize());
  add(block,index,parameterType,hll);
  state.addMemoryUsage(hll.estimatedInMemorySize());
}
