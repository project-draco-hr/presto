{
  return !shipDateCursor.isNull(0) && shipDateCursor.getSlice(0).compareTo(MIN_SHIP_DATE) >= 0 && !shipDateCursor.isNull(0) && shipDateCursor.getSlice(0).compareTo(MAX_SHIP_DATE) < 0 && !discountCursor.isNull(0) && discountCursor.getDouble(0) >= 0.05 && !discountCursor.isNull(0) && discountCursor.getDouble(0) <= 0.07 && !quantityCursor.isNull(0) && quantityCursor.getDouble(0) < 24;
}
