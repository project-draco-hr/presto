{
  List<Entry<ObjectInspector,Object>> bindings=ImmutableList.<Entry<ObjectInspector,Object>>builder().add(entry(javaStringObjectInspector,"sequencefile test")).build();
  Optional<HiveBucket> bucket=HiveBucketing.getHiveBucket(bindings,32);
  assertTrue(bucket.isPresent());
  assertEquals(bucket.get().getBucketCount(),32);
  assertEquals(bucket.get().getBucketNumber(),21);
}
