{
  Cursor cursor=Blocks.createTupleStream(0,"a","bb","c","d","e","e","a","bb").cursor();
  assertTrue(cursor.advanceNextPosition());
  assertTrue(cursor.advanceNextPosition());
  assertTrue(cursor.advanceNextPosition());
  assertCursorsEqual(RangeBoundedCursor.bound(cursor,Range.create(1,3)),Blocks.createTupleStream(2,"c","d").cursor());
  assertEquals(cursor.getPosition(),4);
}
