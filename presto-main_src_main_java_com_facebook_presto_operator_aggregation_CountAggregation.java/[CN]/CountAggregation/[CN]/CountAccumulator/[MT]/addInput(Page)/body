{
  if (!maskChannel.isPresent()) {
    count+=page.getPositionCount();
  }
 else {
    Block masks=page.getBlock(maskChannel.get());
    for (int position=0; position < page.getPositionCount(); position++) {
      if (masks == null || masks.getBoolean(position)) {
        count++;
      }
    }
  }
}
