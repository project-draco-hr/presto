{
  Cursor cursor=tupleStream.cursor(new QuerySession());
  cursor.advanceNextPosition();
  return new Stats(cursor.getLong(Fields.ROW_COUNT.getFieldIndex()),cursor.getLong(Fields.RUNS_COUNT.getFieldIndex()),cursor.getLong(Fields.MIN_POSITION.getFieldIndex()),cursor.getLong(Fields.MAX_POSITION.getFieldIndex()),cursor.getLong(Fields.AVG_RUN_LENGTH.getFieldIndex()));
}
