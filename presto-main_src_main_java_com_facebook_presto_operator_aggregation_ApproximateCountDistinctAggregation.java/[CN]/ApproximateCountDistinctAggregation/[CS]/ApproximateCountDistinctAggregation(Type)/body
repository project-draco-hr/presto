{
  super(SINGLE_LONG,SINGLE_VARBINARY,parameterType);
  checkArgument(parameterType == Type.FIXED_INT_64 || parameterType == Type.DOUBLE || parameterType == Type.VARIABLE_BINARY,"Expected parameter type to be FIXED_INT_64, DOUBLE, or VARIABLE_BINARY, but was %s",parameterType);
  this.parameterType=parameterType;
}
