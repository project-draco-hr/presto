{
  if (!isParentFirstResource(name)) {
    URL url=findResource(name);
    if (url != null) {
      return url;
    }
  }
  if (!isHiddenResource(name)) {
    URL url=getParent().getResource(name);
    if (url != null) {
      return url;
    }
  }
  if (isParentFirstResource(name)) {
    URL url=findResource(name);
    if (url != null) {
      return url;
    }
  }
  return null;
}
