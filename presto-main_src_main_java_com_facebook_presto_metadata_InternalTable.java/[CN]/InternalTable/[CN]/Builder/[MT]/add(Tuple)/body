{
  checkArgument(tuple.getTupleInfo().getTypes().equals(tupleInfos),"tuple schema does not match builder");
  for (int i=0; i < tupleInfos.size(); i++) {
    pageBuilder.getBlockBuilder(i).append(tuple,i);
  }
  if (pageBuilder.isFull()) {
    flushPage();
    pageBuilder.reset();
  }
  return this;
}
