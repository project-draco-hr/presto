{
  checkState(Thread.holdsLock(SharedBuffer.this),"Thread must hold a lock on the %s",SharedBuffer.class.getSimpleName());
  if (finished.get()) {
    return true;
  }
  if (!state.get().canAddPages() && sequenceId.get() >= pagesAdded.get()) {
    finished.set(true);
    checkFlushComplete();
  }
  return finished.get();
}
