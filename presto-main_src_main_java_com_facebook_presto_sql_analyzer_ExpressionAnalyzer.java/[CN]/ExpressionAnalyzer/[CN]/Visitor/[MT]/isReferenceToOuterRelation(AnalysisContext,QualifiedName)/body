{
  AnalysisContext current=context;
  while (current != null) {
    RelationType type=current.getParentRelationType();
    if (!type.resolveFields(qualifiedName).isEmpty()) {
      return true;
    }
    if (isColumnPrefix(qualifiedName,type)) {
      return true;
    }
    current=current.getParent().orElse(null);
  }
  return false;
}
