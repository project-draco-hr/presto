{
  for (  WhenClause whenClause : node.getWhenClauses()) {
    coerceToSingleType(context,node,"CASE operand type does not match WHEN clause operand type: %s vs %s",node.getOperand(),whenClause.getOperand());
  }
  Type type=coerceToSingleType(context,"All CASE results must be the same type: %s",getCaseResultExpressions(node.getWhenClauses(),node.getDefaultValue()));
  expressionTypes.put(node,type);
  return type;
}
