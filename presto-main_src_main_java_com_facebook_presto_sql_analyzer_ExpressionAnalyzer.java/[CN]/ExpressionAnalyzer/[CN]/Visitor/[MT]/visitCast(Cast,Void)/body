{
  process(node.getExpression(),context);
  Type type;
switch (node.getType()) {
case "BOOLEAN":
    type=Type.BOOLEAN;
  break;
case "DOUBLE":
type=Type.DOUBLE;
break;
case "BIGINT":
type=Type.LONG;
break;
case "VARCHAR":
type=Type.STRING;
break;
default :
throw new SemanticException(node,"Cannot cast to type: " + node.getType());
}
subExpressionTypes.put(node,type);
return type;
}
