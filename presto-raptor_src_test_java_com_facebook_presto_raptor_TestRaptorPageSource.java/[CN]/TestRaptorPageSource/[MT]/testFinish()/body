{
  BlockPageSource raptorDataSource=createRaptorDataSource();
  assertEquals(raptorDataSource.isFinished(),false);
  assertPageEquals(TYPES,raptorDataSource.getNextPage(),rowPageBuilder(TYPES).row("alice",0).row("bob",1).row("charlie",2).row("dave",3).build());
  assertEquals(raptorDataSource.isFinished(),false);
  assertPageEquals(TYPES,raptorDataSource.getNextPage(),rowPageBuilder(TYPES).row("alice",4).row("bob",5).row("charlie",6).row("dave",7).build());
  assertEquals(raptorDataSource.isFinished(),false);
  raptorDataSource.close();
  assertEquals(raptorDataSource.isFinished(),true);
  assertEquals(raptorDataSource.getNextPage(),null);
}
