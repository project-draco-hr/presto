{
  try {
    if (out == null) {
      out=outputSupplier.getOutput();
      writeTupleInfo(out,new TupleInfo(type));
    }
    Slice blockSlice=((UncompressedValueBlock)block).getSlice();
    write(out,block.getCount(),blockSlice);
  }
 catch (  IOException e) {
    throw Throwables.propagate(e);
  }
}
