{
synchronized (this) {
    if (endTime == null) {
      endTime=DateTime.now();
    }
  }
synchronized (this) {
    if (failureCause == null) {
      failureCause=new PrestoException(StandardErrorCode.USER_CANCELED.toErrorCode(),"Query was canceled");
    }
  }
  return queryState.setIf(CANCELED,Predicates.not(inDoneState()));
}
