{
synchronized (this) {
    if (queryState.isDone()) {
      return false;
    }
    queryState=QueryState.CANCELED;
    failureCause=new RuntimeException("Query was canceled");
  }
  log.debug("Canceled query %s",queryId);
  queryStats.recordEnd();
  queryMonitor.completionEvent(getQueryInfo());
  return true;
}
