{
  if (!cursor.isValid()) {
    if (!cursor.advanceNextPosition()) {
      return false;
    }
  }
  checkState(cursor.isValid(),"invariant");
  if (cursor.getPosition() < validRange.getStart()) {
    if (!cursor.advanceToPosition(validRange.getStart())) {
      return false;
    }
  }
  return true;
}
