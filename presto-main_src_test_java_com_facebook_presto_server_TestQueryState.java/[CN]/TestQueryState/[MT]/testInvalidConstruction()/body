{
  try {
    new PageBuffer(TUPLE_INFOS,0,4);
    fail("Expected IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new PageBuffer(ImmutableList.of(SINGLE_LONG),4,0);
    fail("Expected IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
}
