{
  try {
    new PageBuffer("bufferId",TUPLE_INFOS,0,4);
    fail("Expected IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new PageBuffer("bufferId",ImmutableList.of(SINGLE_LONG),4,0);
    fail("Expected IllegalArgumentException");
  }
 catch (  IllegalArgumentException e) {
  }
}
