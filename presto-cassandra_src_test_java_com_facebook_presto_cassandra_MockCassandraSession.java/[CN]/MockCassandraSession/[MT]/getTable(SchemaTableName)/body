{
  accessCount.incrementAndGet();
  if (throwException) {
    throw new IllegalStateException();
  }
  if (tableName.getSchemaName().equals(TEST_SCHEMA) && tableName.getTableName().equals(TEST_TABLE)) {
    return new CassandraTable(new CassandraTableHandle(connectorId,TEST_SCHEMA,TEST_TABLE),ImmutableList.of(new CassandraColumnHandle(connectorId,TEST_COLUMN1,0,CassandraType.VARCHAR,null,true,false,false),new CassandraColumnHandle(connectorId,TEST_COLUMN2,0,CassandraType.INT,null,false,false,false)));
  }
  throw new TableNotFoundException(tableName);
}
