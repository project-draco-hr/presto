{
  SharedBuffer<Integer> sharedBuffer=new SharedBuffer<>(10);
  sharedBuffer.finish();
  assertFalse(sharedBuffer.offer(0));
  assertFalse(sharedBuffer.add(0));
  sharedBuffer=new SharedBuffer<>(10);
  sharedBuffer.destroy();
  assertFalse(sharedBuffer.offer(0));
  assertFalse(sharedBuffer.add(0));
}
