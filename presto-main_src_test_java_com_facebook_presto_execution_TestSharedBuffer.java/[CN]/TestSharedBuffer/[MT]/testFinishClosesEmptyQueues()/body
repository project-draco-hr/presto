{
  OutputBuffers outputBuffers=INITIAL_EMPTY_OUTPUT_BUFFERS.withBuffer("first",new UnpartitionedPagePartitionFunction()).withBuffer("second",new UnpartitionedPagePartitionFunction());
  SharedBuffer sharedBuffer=new SharedBuffer(TASK_ID,executor,sizeOfPages(10),outputBuffers);
  sharedBuffer.finish();
  assertQueueClosed(sharedBuffer,"first",0);
  assertQueueClosed(sharedBuffer,"second",0);
}
