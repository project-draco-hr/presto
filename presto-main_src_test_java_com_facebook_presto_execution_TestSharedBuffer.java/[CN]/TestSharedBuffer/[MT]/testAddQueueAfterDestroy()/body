{
  SharedBuffer sharedBuffer=new SharedBuffer(sizeOfPages(10));
  assertFalse(sharedBuffer.isFinished());
  sharedBuffer.destroy();
  assertFinished(sharedBuffer);
  sharedBuffer.noMoreQueues();
  assertFinished(sharedBuffer);
  sharedBuffer.noMoreQueues();
  assertFinished(sharedBuffer);
  sharedBuffer.addQueue("foo");
}
