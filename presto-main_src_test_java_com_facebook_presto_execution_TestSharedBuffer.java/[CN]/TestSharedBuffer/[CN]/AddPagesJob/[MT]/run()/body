{
  started.countDown();
  try {
    for (T element=elements.peek(); element != null; element=elements.peek()) {
      try {
        sharedBuffer.add(element);
        assertNotNull(elements.poll());
      }
 catch (      InterruptedException e) {
        Thread.currentThread().interrupt();
        throw Throwables.propagate(e);
      }
    }
  }
  finally {
    finished.countDown();
  }
}
