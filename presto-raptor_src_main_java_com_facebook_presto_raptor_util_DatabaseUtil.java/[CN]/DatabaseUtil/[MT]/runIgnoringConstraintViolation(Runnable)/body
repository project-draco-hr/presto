{
  try {
    task.run();
  }
 catch (  RuntimeException e) {
    for (    Throwable throwable : Throwables.getCausalChain(e)) {
      if (throwable instanceof SQLException) {
        String state=((SQLException)throwable).getSQLState();
        if (state.startsWith("23")) {
          return;
        }
      }
    }
    throw e;
  }
}
