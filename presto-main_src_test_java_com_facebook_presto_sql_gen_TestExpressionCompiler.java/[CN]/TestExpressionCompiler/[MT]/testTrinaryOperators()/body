{
  for (  Long first : longLefts) {
    for (    Long second : longLefts) {
      for (      Long third : longRights) {
        assertExecute(generateExpression("%s between %s and %s",first,second,third),first == null || second == null || third == null ? null : second <= first && first <= third);
      }
    }
  }
  for (  Long first : longLefts) {
    for (    Double second : doubleLefts) {
      for (      Long third : longRights) {
        assertExecute(generateExpression("%s between %s and %s",first,second,third),first == null || second == null || third == null ? null : second <= first && first <= third);
      }
    }
  }
  for (  Double first : doubleLefts) {
    for (    Double second : doubleLefts) {
      for (      Long third : longRights) {
        assertExecute(generateExpression("%s between %s and %s",first,second,third),first == null || second == null || third == null ? null : second <= first && first <= third);
      }
    }
  }
  for (  String first : stringLefts) {
    for (    String second : stringLefts) {
      for (      String third : stringRights) {
        assertExecute(generateExpression("%s between %s and %s",first,second,third),first == null || second == null || third == null ? null : second.compareTo(first) <= 0 && first.compareTo(third) <= 0);
      }
    }
  }
}
