{
  String tableNameInDatabase=mutableTablesState().get(TABLE_NAME).getNameInDatabase();
  assertThat(query("SELECT * FROM " + tableNameInDatabase)).hasNoRows();
  query("INSERT INTO " + tableNameInDatabase + " VALUES("+ "127, "+ "32767, "+ "2147483647, "+ "9223372036854775807, "+ "123.345, "+ "234.567, "+ "timestamp '2015-05-10 12:15:35.123', "+ "date '2015-05-10', "+ "'ala ma kota', "+ "'ala ma kota', "+ "true, "+ "from_base64('a290IGJpbmFybnk=')"+ ")");
  assertThat(query("SELECT * FROM " + tableNameInDatabase)).containsOnly(row(127,32767,2147483647,9223372036854775807L,123.34500122070312,234.567,Timestamp.valueOf("2015-05-10 12:15:35.123"),Date.valueOf("2015-05-10"),"ala ma kota","ala ma kot",true,"kot binarny".getBytes()));
}
