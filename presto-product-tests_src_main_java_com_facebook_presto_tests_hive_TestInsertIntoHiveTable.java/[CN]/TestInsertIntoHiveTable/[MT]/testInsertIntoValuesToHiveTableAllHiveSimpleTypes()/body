{
  String tableNameInDatabase=mutableTablesState().get(TABLE_NAME).getNameInDatabase();
  assertThat(query("SELECT * FROM " + tableNameInDatabase)).hasNoRows();
  query("INSERT INTO " + tableNameInDatabase + " VALUES("+ "TINYINT '127', "+ "SMALLINT '32767', "+ "2147483647, "+ "9223372036854775807, "+ "234.567, "+ "CAST(346 as DECIMAL(10,0)),"+ "CAST(345.67800 as DECIMAL(10,5)),"+ "timestamp '2015-05-10 12:15:35.123', "+ "date '2015-05-10', "+ "'ala ma kota', "+ "'ala ma kot', "+ "true, "+ "from_base64('a290IGJpbmFybnk=')"+ ")");
  assertThat(query("SELECT * FROM " + tableNameInDatabase)).containsOnly(row(127,32767,2147483647,9223372036854775807L,234.567,new BigDecimal("346"),new BigDecimal("345.67800"),Timestamp.valueOf("2015-05-10 12:15:35.123"),Date.valueOf("2015-05-10"),"ala ma kota","ala ma kot",true,"kot binarny".getBytes()));
}
