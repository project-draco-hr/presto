{
  try {
    OperatorContext operatorContext=driverContext.addOperatorContext(operatorId,MaterializedViewWriterOperator.class.getSimpleName());
    return new MaterializedViewWriterOperator(operatorContext,sourceId,storageManager,nodeIdentifier,columnHandles);
  }
 catch (  IOException e) {
    throw Throwables.propagate(e);
  }
}
