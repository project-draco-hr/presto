{
  while (true) {
    try {
      QueryInfo queryInfo=queryClient.getQueryInfo(false);
      if ((queryInfo == null) || queryInfo.getState().isDone()) {
        break;
      }
      if (queryInfo.resultsPending()) {
        break;
      }
      Uninterruptibles.sleepUninterruptibly(100,MILLISECONDS);
    }
 catch (    Exception e) {
      throw propagate(e);
    }
  }
}
