{
  SignalHandler oldHandler=Signal.handle(SIGINT,new SignalHandler(){
    @Override public void handle(    Signal signal){
      if (!ignoreUserInterrupt.get()) {
        cancelLeafStage();
      }
    }
  }
);
  try {
    renderQueryOutput(out);
  }
  finally {
    Signal.handle(SIGINT,oldHandler);
  }
}
