{
  return new Function<String,Type>(){
    @Override public Type apply(    String type){
switch (type) {
case "bigint":
        return Type.FIXED_INT_64;
case "double":
      return Type.DOUBLE;
case "varchar":
    return Type.VARIABLE_BINARY;
}
throw new AssertionError("Unhandled type: " + type);
}
}
;
}
