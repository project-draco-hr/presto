{
  MapBinder<DataSourceType,Metadata> metadataBinder=MapBinder.newMapBinder(binder,DataSourceType.class,Metadata.class);
  metadataBinder.addBinding(DataSourceType.TPCH).toInstance(tpchMetadata);
  MapBinder<DataSourceType,DataStreamProvider> dataStreamProviderBinder=MapBinder.newMapBinder(binder,DataSourceType.class,DataStreamProvider.class);
  dataStreamProviderBinder.addBinding(DataSourceType.TPCH).toInstance(dataStreamProvider);
}
