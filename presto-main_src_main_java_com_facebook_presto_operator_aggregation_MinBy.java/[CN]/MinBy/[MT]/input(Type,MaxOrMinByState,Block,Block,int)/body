{
  if (state.getKey() == null || state.getKey().isNull(0)) {
    state.setKey(key.getSingleValueBlock(position));
    state.setValue(value.getSingleValueBlock(position));
  }
 else   if (keyType.compareTo(key,position,state.getKey(),0) < 0) {
    state.setKey(key.getSingleValueBlock(position));
    state.setValue(value.getSingleValueBlock(position));
  }
}
