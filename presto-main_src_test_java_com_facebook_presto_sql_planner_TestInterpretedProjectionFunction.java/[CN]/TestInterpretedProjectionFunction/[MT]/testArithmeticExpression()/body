{
  assertProjection(LONG,"42 + 87",42L + 87L);
  assertProjection(DOUBLE,"42 + 22.2",42L + 22.2);
  assertProjection(DOUBLE,"11.1 + 22.2",11.1 + 22.2);
  assertProjection(LONG,"42 - 87",42L - 87L);
  assertProjection(DOUBLE,"42 - 22.2",42L - 22.2);
  assertProjection(DOUBLE,"11.1 - 22.2",11.1 - 22.2);
  assertProjection(LONG,"42 * 87",42L * 87L);
  assertProjection(DOUBLE,"42 * 22.2",42L * 22.2);
  assertProjection(DOUBLE,"11.1 * 22.2",11.1 * 22.2);
  assertProjection(LONG,"42 / 87",42L / 87L);
  assertProjection(DOUBLE,"42 / 22.2",42L / 22.2);
  assertProjection(DOUBLE,"11.1 / 22.2",11.1 / 22.2);
  assertProjection(LONG,"42 % 87",42L % 87L);
  assertProjection(DOUBLE,"42 % 22.2",42L % 22.2);
  assertProjection(DOUBLE,"11.1 % 22.2",11.1 % 22.2);
}
