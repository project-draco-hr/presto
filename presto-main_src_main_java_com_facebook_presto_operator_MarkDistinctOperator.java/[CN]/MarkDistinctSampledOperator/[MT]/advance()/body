{
  if (markerBlock == null) {
    return false;
  }
  if (distinct && sampleWeight > 1) {
    distinct=false;
    sampleWeight--;
    return true;
  }
  position++;
  if (position >= markerBlock.getPositionCount()) {
    markerBlock=null;
    Arrays.fill(blocks,null);
    return false;
  }
 else {
    sampleWeight=blocks[sampleWeightChannel].getLong(position);
    distinct=markerBlock.getBoolean(position);
    return true;
  }
}
