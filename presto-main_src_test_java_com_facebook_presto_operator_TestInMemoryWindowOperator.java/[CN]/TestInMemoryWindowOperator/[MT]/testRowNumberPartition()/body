{
  TupleInfo tupleInfo=new TupleInfo(VARIABLE_BINARY,FIXED_INT_64,DOUBLE,BOOLEAN);
  Operator source=createOperator(new Page(new BlockBuilder(tupleInfo).append("b").append(-1).append(-0.1).append(true).append("a").append(2).append(0.3).append(false).append("a").append(4).append(0.2).append(true).build()),new Page(new BlockBuilder(tupleInfo).append("b").append(5).append(0.4).append(false).append("a").append(6).append(0.1).append(true).build()));
  Operator actual=new InMemoryWindowOperator(source,0,ints(0),ROW_NUMBER,ints(0),ints(1),bools(true),10,new TaskMemoryManager(MAX_SIZE));
  Operator expected=createOperator(new Page(new BlockBuilder(tupleInfo).append("a").append(2).append(0.3).append(false).append("a").append(4).append(0.2).append(true).append("a").append(6).append(0.1).append(true).append("b").append(-1).append(-0.1).append(true).append("b").append(5).append(0.4).append(false).build(),createLongsBlock(1,2,3,1,2)));
  assertOperatorEquals(actual,expected);
}
