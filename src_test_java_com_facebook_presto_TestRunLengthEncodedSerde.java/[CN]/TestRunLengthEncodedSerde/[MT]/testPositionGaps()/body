{
  BlockStream<UncompressedValueBlock> blockStream=new UncompressedBlockStream(new TupleInfo(VARIABLE_BINARY),Blocks.createBlock(1,"a","a","b","a","c"),Blocks.createBlock(6,"c","a","b","b","b"),Blocks.createBlock(100,"y","y","a","y","b"));
  rleSerde.serialize(blockStream,sliceOutput);
  Assert.assertTrue(BlockStreams.equivalent(blockStream,rleSerde.deserialize(sliceOutput.slice())));
}
