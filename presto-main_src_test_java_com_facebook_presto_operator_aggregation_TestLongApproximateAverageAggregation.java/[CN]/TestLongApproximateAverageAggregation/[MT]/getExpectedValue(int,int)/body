{
  if (length == 0) {
    return null;
  }
  double sum=0;
  for (int i=start; i < start + length; i++) {
    sum+=i;
  }
  double mean=sum / length;
  double m2=0.0;
  for (int i=start; i < start + length; i++) {
    m2+=(i - mean) * (i - mean);
  }
  double variance=m2 / length;
  StringBuilder sb=new StringBuilder();
  sb.append(mean);
  sb.append(" +/- ");
  sb.append(zScore(getConfidence()) * Math.sqrt(variance / length));
  return sb.toString();
}
