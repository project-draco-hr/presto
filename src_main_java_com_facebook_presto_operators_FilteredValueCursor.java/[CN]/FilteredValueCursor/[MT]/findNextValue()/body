{
  do {
    if (!nextValueBlockCursor.advanceToNextValue()) {
      if (iterator.hasNext()) {
        nextValueBlockCursor=iterator.next().blockCursor();
        nextValueBlockCursor.advanceToNextValue();
      }
 else {
        nextValueBlockCursor=null;
        return;
      }
    }
  }
 while (!predicate.apply(nextValueBlockCursor));
}
