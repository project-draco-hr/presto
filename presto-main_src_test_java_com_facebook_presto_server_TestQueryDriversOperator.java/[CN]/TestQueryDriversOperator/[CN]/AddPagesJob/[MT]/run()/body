{
  try {
    for (    Page page : pages) {
      try {
        if (queryState.addPage(page)) {
          pagesAdded.incrementAndGet();
        }
      }
 catch (      InterruptedException e) {
        queryState.queryFailed(e);
        Thread.currentThread().interrupt();
        throw Throwables.propagate(e);
      }
    }
  }
  finally {
    queryState.sourceFinished();
  }
}
