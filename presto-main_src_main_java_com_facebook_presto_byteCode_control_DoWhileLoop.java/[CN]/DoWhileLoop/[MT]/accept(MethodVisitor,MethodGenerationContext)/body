{
  checkState(!condition.isEmpty(),"DoWhileLoop does not have a condition set");
  Block block=new Block().visitLabel(beginLabel).append(new Block().setDescription("body").append(body)).visitLabel(continueLabel).append(new Block().setDescription("condition").append(condition)).ifFalseGoto(endLabel).gotoLabel(beginLabel).visitLabel(endLabel);
  block.accept(visitor,generationContext);
}
