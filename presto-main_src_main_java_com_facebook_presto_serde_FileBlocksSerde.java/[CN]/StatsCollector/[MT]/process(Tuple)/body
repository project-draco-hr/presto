{
  Preconditions.checkNotNull(tuple,"tuple is null");
  Preconditions.checkState(!finished,"already finished");
  if (lastTuple == null) {
    lastTuple=tuple;
    if (set.size() < MAX_UNIQUE_COUNT) {
      set.add(lastTuple);
    }
  }
 else   if (!tuple.equals(lastTuple)) {
    runsCount++;
    lastTuple=tuple;
    if (set.size() < MAX_UNIQUE_COUNT) {
      set.add(lastTuple);
    }
  }
  rowCount++;
}
