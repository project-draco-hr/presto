{
  return new Function<PartitionChunk,Split>(){
    @Override public Split apply(    PartitionChunk chunk){
      ImportClient importClient=importClientManager.getClient(sourceName);
      return new ImportSplit(sourceName,chunk.getPartitionName(),chunk.isLastChunk(),SerializedPartitionChunk.create(importClient,chunk),toAddresses(chunk.getHosts()),chunk.getInfo());
    }
  }
;
}
