{
  if (hash == null) {
    hash=sourceHashProvider.get();
  }
  pageBuilder.reset();
  while (joinCurrentPosition(pageBuilder)) {
    if (!advanceProbePosition()) {
      break;
    }
    if (!outerJoinCurrentPosition(pageBuilder)) {
      break;
    }
  }
  if (pageBuilder.isEmpty()) {
    return endOfData();
  }
  return pageBuilder.build();
}
