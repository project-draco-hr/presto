{
  if (cursors[0].advanceNextPosition()) {
    for (int i=1; i < cursors.length; i++) {
      checkState(cursors[i].advanceNextPosition());
    }
    return true;
  }
 else {
    for (int i=1; i < cursors.length; i++) {
      checkState(!cursors[i].advanceNextPosition());
    }
    return false;
  }
}
