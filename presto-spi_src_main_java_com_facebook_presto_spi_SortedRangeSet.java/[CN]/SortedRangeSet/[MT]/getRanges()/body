{
  return new Ranges(){
    @Override public int getRangeCount(){
      return SortedRangeSet.this.getRangeCount();
    }
    @Override public List<Range> getOrderedRanges(){
      return SortedRangeSet.this.getOrderedRanges();
    }
    @Override public Range getSpan(){
      return SortedRangeSet.this.getSpan();
    }
  }
;
}
